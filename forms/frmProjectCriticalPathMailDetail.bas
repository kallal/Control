Private Sub CalculateDates()
Dim myrs As Recordset
Dim mysql As String
Dim myLatestPrintDate As Date
Dim myAssignedDueDate As Date




Me.Refresh

'Check if previous due date is firm. If so: the final specified due date must be the firm date indicated. If they want to change, they need to change the docket completed
'   by field on the form footer. If due date is firm, inform the user that the final date will not be changed.

'Don't check for firm dates - dates seem to be not very firm, even if it's checked.
'If Parent.chkFirmDueDate = True Then
'    MsgBox "You have indicated that the due date for this docket is firm. In order to change dates, either deselect the 'Check if Delivery Date is FIRM' below, or change the 'Completed Date' below.", vbCritical, ProgramName
'    Exit Sub
'End If


'Select only the print records


mysql = "SELECT * from qryProjectComponentHeaderPrintOnly WHERE [ProjectHeaderID] = " & Me.ProjectHeaderID
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)


'This loop will recalculate all of the Assigned dates, and will also recalculate the final dates on the bottom of the form.
With myrs
'Check first record for a specified date - if it has one, then set that as the specified date for all print components - if not, let the system calculate the dates.
If Not myrs.EOF Then
    myrs.MoveFirst
    If Nz(myrs!SpecifiedDueDate, 0) > 0 Then
        myLatestPrintDate = myrs!SpecifiedDueDate
    Else
        myLatestPrintDate = 0
    End If
End If


'if we have a specified date, populate up all print records with that date, or clear all of them.

    Do Until myrs.EOF
        myrs.Edit
        If Nz(myLatestPrintDate, 0) <> 0 Then
            myrs!SpecifiedDueDate = myLatestPrintDate
        Else
            myrs!SpecifiedDueDate = Null
        End If
        
        myrs.Update
        myrs.MoveNext
    Loop



'not using this - want to trigger new dates only from the _detail forms.

'Check if we have a new date (will not be firm, as already passed that point.)
'If Parent.txtDocketCompletedBy <> myrs!SpecifiedDueDate Then 'We have a new Final Due Date that takes precedence.
'    myFinalDueDate = Parent.txtDocketCompletedBy
'End If


If Not .EOF Then .MoveFirst




Do Until .EOF
    If !MailComponentYN = False Then
        myDaysNeeded = Nz(!DYBindery, 0) + Nz(!DyCreative, 0) + Nz(!DYLFFinish) + Nz(!DYMailData) + Nz(!DYMailShop) + Nz(!DYOutsideSrv) + Nz(!DYPrepress) + Nz(!DYPrint) + Nz(!DYStock)
        myStartDate = Nz(!ArtExpectedDate, !StartDate)
        If Nz(myStartDate, 0) = 0 Then myStartDate = Date
        
        .Edit
        myAssignedDueDate = fncWorkday(myStartDate, myDaysNeeded)
        !AssignedDueDate = myAssignedDueDate
        
        If Nz(!SpecifiedDueDate, 0) > 0 Then
            If Nz(!SpecifiedDueDate, 0) > myLatestPrintDate Then myLatestPrintDate = !SpecifiedDueDate
        Else
            If myAssignedDueDate > myLatestPrintDate Then myLatestPrintDate = myAssignedDueDate
        End If
        
            
        .Update
        
      End If
      
    .MoveNext
Loop




'Put the highest date on the print jobs due date on the parent screen.

Parent.txtPrintCompletedBy = myLatestPrintDate


'Grab the highest date of all the print jobs for a start date for the Mail/Install components, and recalc those dates.
Set myrs = Nothing
End With


mysql = "SELECT * from qryProjectComponentHeaderMailOnly WHERE [ProjectHeaderID] = " & Me.ProjectHeaderID
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
With myrs

.MoveFirst

mylatestmaildate = 0
Do Until .EOF
    If !MailComponentYN = True Then
        
        .Edit
        !StartDate = myLatestPrintDate
        myDaysNeeded = Nz(!DYBindery, 0) + Nz(!DyCreative, 0) + Nz(!DYLFFinish) + Nz(!DYMailData) + Nz(!DYMailShop) + Nz(!DYOutsideSrv) + Nz(!DYPrepress) + Nz(!DYPrint) + Nz(!DYStock)
        If myLatestPrintDate > Nz(!DataExpectedDate, 0) Then
            !StartDate = myLatestPrintDate
            myStartDate = myLatestPrintDate
            
        Else
            !StartDate = !DataExpectedDate
            myStartDate = !DataExpectedDate
        End If
        If Nz(myStartDate, 0) = 0 Then myStartDate = Date
        
        myAssignedDueDate = fncWorkday(myStartDate, myDaysNeeded)
        !AssignedDueDate = myAssignedDueDate
        If Nz(!SpecifiedDueDate, 0) > 0 Then
            If Nz(!SpecifiedDueDate, 0) > mylatestmaildate Then mylatestmaildate = !SpecifiedDueDate
        Else
            If myAssignedDueDate > mylatestmaildate Then mylatestmaildate = myAssignedDueDate
        End If
        
        
        
        
        .Update
    End If
    
    .MoveNext
Loop


'Update the ProjectHeader with the new dates


myfinaldate = myLatestPrintDate



If mylatestmaildate > myLatestPrintDate Then
    myfinaldate = mylatestmaildate
End If

Parent.txtDocketCompletedBy = myfinaldate


mysql = "SELECT * from dbo_ProjectHeader WHERE [ID] = " & Me.ProjectHeaderID
End With

Set myrs = Nothing

Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)

myrs.MoveLast



myrs.Edit
'Inform user that you are changing the final due date.
mypreviousduedate = Nz(myrs!SpecifiedDocketDueDate, myrs!AssignedDocketDueDate)
myrs!SpecifiedDocketDueDate = myfinaldate
'MsgBox "Final Docket Due Date has been changed from " & Format(mypreviousduedate, "dd-mmm-yy") & " to " & Format(myfinaldate, "dd-mmm-yy") & ".", vbInformation, ProgramName
myrs.Update

AddContactHistory Me.ContactGeneralID, , , Me.ProjectHeaderID, 1, "Shipping Dates changed - Final date now set to: " & Format(myfinaldate, "dd-mmm-yy")





Parent.Form.Refresh
End Sub

