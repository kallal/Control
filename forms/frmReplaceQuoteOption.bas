Private Sub cmdCancel_Click()


mysql = "SELECT * from tmpDboReplaceQuoteOption WHERE [ID] = 1"
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset)
myrs.Edit
myrs!frmReplaceOption = 3 'Option 3 tells the other code to cancel the update
myrs.Update

DoCmd.Close
End Sub

Private Sub cmdReplaceAll_Click()

Dim myrs As Recordset
Dim mysql As String

If Nz(Me.frmSelectQuote, 0) = 0 Then
    MsgBox "You must select a Quote Select Option first.", vbCritical, ProgramName
    Exit Sub
End If

mysql = "SELECT * from tmpDboReplaceQuoteOption"
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset)
myrs.MoveFirst
myrs.Edit
myrs!frmSelectQuote = Me.frmSelectQuote
myrs!frmReplaceOption = 1
myrs.Update

DoCmd.Close


End Sub

Private Sub cmdReplaceStock_Click()
Dim myrs As Recordset
Dim mysql As String

If Nz(Me.frmSelectQuote, 0) = 0 Or Nz(Me.frmSelectPricing, 0) = 0 Then
    MsgBox "You must select from both option groups first.", vbCritical, ProgramName
    Exit Sub
End If

mysql = "SELECT * from tmpDboReplaceQuoteOption"
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset)
myrs.MoveFirst
myrs.Edit
myrs!frmSelectQuote = Me.frmSelectQuote
myrs!frmReplaceOption = 2
myrs!frmSelectPriceOption = Me.frmSelectPricing
myrs.Update

DoCmd.Close
End Sub