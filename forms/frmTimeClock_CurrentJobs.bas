Private Sub cmdEnd_Click()
If Nz(Me.StartTime, 0) = 0 Then
    MsgBox "This Task has not been started yet. You cannot end a task that hasn't started.", vbCritical, ProgramName
    Exit Sub
End If

If Nz(Me.EndTime, 0) <> 0 Then
    MsgBox "This Task has already been closed.", vbCritical, ProgramName
    Exit Sub
End If

myresp = vbNo
myPrompt = "You have indicated you want to end this task:" & vbCrLf & vbCrLf & Me.txtClientName & vbCrLf & Me.Description & vbCrLf & vbCrLf & "for Employee: " & Me.EmployeeName & vbCrLf & vbCrLf & "Are you sure?"
myresp = MsgBox(myPrompt, vbQuestion + vbYesNo, ProgramName)
If myresp = vbNo Then Exit Sub


myPrompt = "Ending task " & Me.Description & ". Please type any additional production notes regarding this task (i.e. machine problems, stock issues, etc.)"
myresp = ""
myresp = InputBox(myPrompt, ProgramName)


Me.EndTime = Time
Me.enddate = Date


mytime = mytime + (Me.EndTime - Me.StartTime) * 24 ' result in fraction of day X 24 hrs

mytime = mytime + (Me.enddate - Me.StartDate) * 24 'add in number of days if it went overnight


Me.TotalTime = mytime

Me.Notes = myresp
Me.Requery
End Sub

