Private Sub QtyToReceive_Click()
Dim myResponse As String
Dim myqty As Variant
Dim mysql As String
Dim myRST As Recordset
myResponse = Nz(Me.qtyToReceive, 0)

myResponse = Nz(InputBox("How many of this item to receive?", ProgramName, myResponse), 0)

myqty = Int(Val(myResponse))
'take the result and place it in the database in the correct position
If myqty = 0 Then myqty = Null


mysql = "SELECT * from dbo_ProjectComponentStock WHERE [ID] = " & Me.ID
Set myRST = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
'doublecheck we have the right record
    If myRST!ID = Me.ID Then
        myRST.Edit
        myRST!RecTagQty = myqty
        myRST!RecTagID = cLoginID
        myRST.Update
        
    End If

Set myRST = Nothing

Me.Requery


End Sub

