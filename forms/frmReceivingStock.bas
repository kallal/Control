If Nz(Me.qtyToReceive, 0) = 0 Then
    Me.qtyToReceive = Me.QtyOrdered - Nz(Me.QtyReceived, 0)
    Me.RecTagID = cLoginID
Else
    Me.qtyToReceive = Null
    Me.RecTagID = Null
End If
If Me.Dirty Then Me.Dirty = False
End Sub


