Private Sub QtyToReceive_AfterUpdate()
If Nz(Me.qtyToReceive, 0) > Nz(Me.QtyOrdered, 0) - Nz(Me.QtyReceived, 0) Then
    myResponse = vbNo
    myResponse = MsgBox("You are receiving more than the quantity that we are supposed to be receiving. Are you sure?", vbQuestion + vbYesNo + vbDefaultButton2, ProgramName)
    If myResponse = vbNo Then
        Me.qtyToReceive = Nz(Me.QtyOrdered, 0) - Nz(Me.QtyReceived, 0)
    End If
End If

Me.Refresh

If Nz(Me.qtyToReceive, 0) = 0 Then
    Me.qtyToReceive = Null
Else
    Me.RecTagID = cLoginID
End If




If Me.Dirty Then Me.Dirty = False

End Sub

