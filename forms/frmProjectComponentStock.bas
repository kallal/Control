End Sub

Private Sub SectionName_AfterUpdate()
'If you change the section name, first make sure it isn't the same as any others in this job, then change it on the Press database as well.

Dim MYDB As Database
Dim myRST As Recordset

Set MYDB = CurrentDb
Set myRST = MYDB.OpenRecordset("dbo_ProjectComponentStock", dbOpenDynaset, dbSeeChanges)

myPreviousName = Me.PressRunName.OldValue

mycriteria = "[ProjectComponentID] = " & Me.ProjectComponentHeaderID & " AND [PressRunName] = " & Chr(34) & Me.PressRunName & Chr(34)

myRST.FindFirst mycriteria

If myRST.NoMatch = False Then
    MsgBox "You cannot use the same name as another section in this project. Please enter another name.", vbCritical, ProgramName
    Me.PressRunName = myPreviousName
    Exit Sub
End If

myRST.Close
Set myRST = MYDB.OpenRecordset("dbo_ProjectComponentPress", dbOpenDynaset, dbSeeChanges)

done = False
Do Until done = True
    mycriteria = "[ProjectComponentHeaderID] = " & Me.ProjectComponentHeaderID & " AND [PressRunName] = " & myPreviousName
    myRST.FindFirst mycriteria
    If myRST.NoMatch = False Then 'Match found, change section to new name and loop again
        myRST.Edit
        myRST!PressRunName = Me.PressRunName
        myRST.Update
    Else
        done = True
    End If
Loop




End Sub

