Public Sub Form_Current()


'If you move to a new record, make sure it's locked first.

Dim ctl As control
    Me.cmdEdit.caption = "LOCKED"
    Me.cmdEdit.BackColor = vbRed
    For Each ctl In Me.Controls
        If ctl.tag = "L" Then
            ctl.enabled = False
            ctl.Locked = True

        End If
    Next

    
' Provide a record counter for using with
' custom navigation buttons (when not using
' Access built in navigation)

On Error Resume Next
    
    
    Dim rst As DAO.Recordset
    Dim lngCount As Long

    Set rst = Me.RecordsetClone
    
    With rst
        .MoveFirst
        
        If Me.ID = Null Then
            Me.AllowAdditions = True
            MsgBox "No records"
        End If
        
       
        
        If Me.ID = rst!ID Then
            cmdPrev.enabled = False
        Else
            cmdPrev.enabled = True
        End If
        
        .MoveLast
        
        If Me.ID = rst!ID Then
            cmdNext.enabled = False
        Else
            cmdNext.enabled = True
        End If
        
        lngCount = .RecordCount
    End With
    
'Show the result of the record count in the text box (txtRecordNo)
    If Me.CurrentRecord > lngCount Then
    Me.txtRecordNo = "NO RECORD"

Else

    Me.txtRecordNo = "Contact " & Me.CurrentRecord & " of " & lngCount

    
End If

If Len(Me.Email) > 0 Then
    Me.cmdSendEmail.enabled = True
Else
    Me.cmdSendEmail.enabled = False
End If

If Len(Me.EmailRes) > 0 Then
    Me.cmdSendEmailRes.enabled = True
Else
    Me.cmdSendEmailRes.enabled = False
End If

If Len(Me.PhDirect) > 0 Then
    Me.cmdCallDirect.enabled = True
Else
    Me.cmdCallDirect.enabled = False
End If

If Len(Me.PhMobile) > 0 Then
    Me.cmdCallMobile.enabled = True
Else
    Me.cmdCallMobile.enabled = False
End If
If Me.Dirty Then Me.Dirty = False
End Sub

