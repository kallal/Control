Private Sub AddNote_AfterUpdate()

If Len(Me.AddNote) > 0 Then

notestring = Format(Now(), "MMM/DD/YYYY  HH:MM AM/PM") & " : " & rsLocSS![FN] & " " & rsLocSS![LN] & " : " & Me.AddNote
Me.Notes = notestring & Chr(13) & Chr(10) & Me.Notes
Me.AddNote = ""
End If
 
If Me.Dirty Then Me.Dirty = False
End Sub

Private Sub cmdAddNew_Click()

Me.AllowAdditions = True



' First check rights.

DoCmd.GoToRecord , , acNewRec
    Me.cmdEdit.caption = "EDITABLE"
    Me.txtRecordNo = "Adding New Contact"
    Me.cmdEdit.BackColor = vbGreen
    

    
    AddContactHistory Me.ContactGeneralID, , Nz(Me.ID), , 1, "Added new Contact Name"
    
    
    Dim ctl As control

    For Each ctl In Me.Controls
        If ctl.tag = "L" Then
            ctl.enabled = True
            ctl.Locked = False
        End If
    Next
    
Me.AllowEdits = True

DoCmd.GoToControl "Prefix"


If Me.Dirty Then Me.Dirty = False

End Sub

Private Sub cmdCallDirect_Click()
Dim mynumber As String

mySubject = InputBox("Calling " & Me!FN & " " & Me!LN & " at " & Format(Me.PhDirect, "(000) 000-0000") & vbCrLf & "What is the nature of this call?", ProgramName)
If mySubject = "" Then
    MsgBox "No subject, call cancelled.", vbCritical, ProgramName
    Exit Sub
End If

'make phone call - Call routine and pass the number parameter


Dim callresult As String

fncMakePhoneCall Me.PhDirect, callresult

'Ignore call result for now... I want to use it to trigger callbacks, etc, in the future.


AddContactHistory Me.ContactGeneralID, Nz(Me.ContactLocationID), Me.ID, , 2, "Phone (Direct) Call - Subject: " & mySubject



If Me.Dirty Then Me.Dirty = False




End Sub

Private Sub cmdCallMobile_Click()
mySubject = InputBox("Calling " & Me!FN & " " & Me!LN & " at " & Format(Me.PhMobile, "(000) 000-0000") & vbCrLf & "What is the nature of this call?", ProgramName)
If mySubject = "" Then
    MsgBox "No subject, call cancelled.", vbCritical, ProgramName
    Exit Sub
End If

fncMakePhoneCall Me.PhMobile

AddContactHistory Me.ContactGeneralID, Nz(Me.ContactLocationID), Me.ID, , 2, "Phone (Mobile) Call - Subject: " & mySubject
If Me.Dirty Then Me.Dirty = False
End Sub

Private Sub cmdCreateProject_Click()

If Me.Dirty Then Me.Dirty = False
Me.Refresh

'Create a new project using this contact, Location, Name
If Nz(Me.ContactLocationID) = 0 Or Nz(Me.FN, "") = "" Or Nz(Me.LN, "") = "" Then
    MsgBox "To create a project, you must have a full name, email address and location assigned to this individual.", vbCritical, ProgramName
    Exit Sub
End If


If Nz(DLookup("SalesRepID", "dbo_ContactGeneral", "[id] = " & Me.ContactGeneralID), 0) = 0 Then
    MsgBox "You need to assign a sales rep to this Company - Cancelling new project.", vbCritical, ProgramName
    Exit Sub
End If

If Nz(DLookup("CSRID", "dbo_ContactGeneral", "[id] = " & Me.ContactGeneralID), 0) = 0 Then
    MsgBox "You need to assign a CSR to this Company - Cancelling new project.", vbCritical, ProgramName
    Exit Sub
End If


myProjectName = InputBox("Please type in the name for this new project.", ProgramName)

If Nz(myProjectName, "") = "" Then
    MsgBox "No project name provided - Cancelling new project.", vbCritical, ProgramName
    Exit Sub
End If

If Len(Nz(myProjectName, "")) > 50 Then
    MsgBox "You must enter a Project name of 50 Characters or Less - Cancelling new project.", vbCritical, ProgramName
    Exit Sub
End If
    

'get next quote number
Dim myRST As Recordset
Dim MYDB As Database
Set MYDB = CurrentDb()
Set myRST = MYDB.OpenRecordset("dbo_NextNumber", dbOpenDynaset, dbSeeChanges)
Dim myQuoteNum As Double


myQuoteNum = Val(myRST!NextEstimateNum)
myRST.Edit

myRST!NextEstimateNum = myQuoteNum + 1
myRST.Update

myRST.Close


fncSetLastDate Me.ContactGeneralID, myQuoteNum, 1 'Update the last dates in the contact database.

'Open recordset of dbo_ProjectHeader and set initial values, and get ID number


Set myRST = MYDB.OpenRecordset("dbo_projectHeader", dbOpenDynaset, dbSeeChanges)
myRST.AddNew
myRST!ContactGeneralID = Me.ContactGeneralID
myRST!ContactNameID = Me.ID
myRST!CreatedBy = cLoginID
myRST!Created = Now()
myRST!ProjectName = myProjectName
myRST!CompanyID = rsLocSS!CompanyID
myRST!CompanyLocationID = rsLocSS!LocationID
If Nz(Forms!frmLogin!LocationID, 0) > 0 Then
myRST!CompanyLocationID = Forms!frmLogin!LocationID
End If

myRST!CompanyEmployeeID = cLoginID
myRST!QuoteNum = myQuoteNum
myRST!QuoteDate = Date
myRST!VOID = False
If Nz(Me.ContactNameSalesRepID, 0) > 0 Then
    myRST!SalesRepID = Me.ContactNameSalesRepID
Else
    myRST!SalesRepID = DLookup("SalesRepID", "dbo_ContactGeneral", "[id] = " & Me.ContactGeneralID)
End If

myRST!GSTExempt = DLookup("GSTExempt", "dbo_ContactGeneral", "[id] = " & Me.ContactGeneralID)
myRST!DeliveryCutoffTime = DLookup("DeliveryCutoffTime", "dbo_ContactGeneral", "[id] = " & Me.ContactGeneralID)

If Nz(Me.ContactNameCSRID, 0) > 0 Then
    myRST!CsrID = Me.ContactNameCSRID
Else
    myRST!CsrID = DLookup("CSRID", "dbo_ContactGeneral", "[id] = " & Me.ContactGeneralID)
End If


'myrst!SalesMgrID

'Billing address defaults
myRST!BillAddLocationID = Me.ContactLocationID
myRST!BillOverride = False
myRST!BillOrderedBy = Me.ID
myRST!BillAdd1 = DLookup("Add1", "dbo_ContactLocation", "[id] = " & Me.ContactLocationID)
myRST!BillADD2 = DLookup("Add2", "dbo_ContactLocation", "[id] = " & Me.ContactLocationID)
myRST!BillCITY = DLookup("City", "dbo_ContactLocation", "[id] = " & Me.ContactLocationID)
