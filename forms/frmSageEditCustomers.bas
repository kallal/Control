Private Sub cmdAddAll_Click()

   Dim rst     As DAO.Recordset
   'Dim SageCustomer As New Sage50com.Customer
   'SageCustomer = create

   
   Set rst = Me.RecordsetClone
   
   Do While rst.EOF = False
   
   ' send this one customer to sage
      If IsNull(rst!Added) = True Then
         SageCustomer.AddCustomer rst!CompName, Nz(rst!Add1, ""), Nz(rst!Add2, ""), Nz(rst!City, ""), _
                                         Nz(rst!Prov, ""), quPhone(rst!PhMain), "", Nz(rst!PC, ""), Nz(rst!Country, "")
      End If
      rst.MoveNext
   Loop
   
   rst.Close
   
   ' exit the form
   
   DoCmd.Close acForm, Me.name
   

End Sub

Private Sub cmdAddSingle_Click()

    
   If IsNull(Me.Added) = False Then
      MsgBox "Already added", vbInformation, "Added"
      Exit Sub
   End If
   
   
   ' send this one customer to sage
   SageCustomer.AddCustomer Me.CompName.Value, Nz(Me.Add1, ""), Nz(Me.Add2, ""), Nz(Me.City, ""), _
                                         Nz(Me.Prov, ""), Nz(Me!PhMain, ""), "", Nz(Me.PC, ""), Nz(Me!Country, "")
      
   
   Me.Added = "Added"
   Me.Dirty = False
   
End Sub

Private Sub cmdCancel_Click()

   ' to cancel, just close the form
   DoCmd.Close acForm, Me.name
   
End Sub

Private Sub cmdRefresh_Click()

   ' to attempt a refresh, simple kick out of dialog mode
