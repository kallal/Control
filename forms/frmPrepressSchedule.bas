Private Sub cboShowOnly_AfterUpdate()

Dim myfilter As String

If Me.chkShowOnlyArtworkArrived = True Then
    myfilter = "nz(ArtArrivedDate,0) > 0"
Else
    myfilter = "[DocketNum] > 0"
End If

If Me.chkDoNotShowApproval = True Then
    myfilter = myfilter & " AND [StPrepress] <> " & Chr(34) & "WAIT - APPROVAL" & Chr(34)
End If


If Nz(Me.cboShowOnly, "") <> "" Then 'set the filter up.
    myfilter = myfilter & " AND [StPrepress] = " & Chr(34) & Me.cboShowOnly & Chr(34)
End If

Me.Filter = myfilter
Me.FilterOn = True

    
End Sub

Private Sub chkDoNotShowApproval_AfterUpdate()
Call cboShowOnly_AfterUpdate
End Sub

Private Sub chkShowOnlyArtworkArrived_AfterUpdate()
Call cboShowOnly_AfterUpdate

End Sub

Private Sub cmdShowAll_Click()
Me.cboShowOnly = ""
