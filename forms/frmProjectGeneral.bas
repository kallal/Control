    'add record to watch list

    myRST.AddNew
    myRST!ProjectHeaderID = Me.ID
    myRST!EmployeeID = cLoginID
    myRST!WatchNotes = a
    myRST.Update
    Me.togWatchList.BackColor = constOrange
    
    
Else
    'confirm removal from watch list
    a = vbNo
    a = MsgBox("Are you sure you want to remove this project from your Watch List?", vbQuestion + vbYesNo + vbDefaultButton2, ProgramName)
    If a = vbYes Then
     
    'delete record from watch list

        myRST.MoveFirst
        Do Until myRST.EOF
               
        If myRST!ProjectHeaderID = Me.ID And myRST!EmployeeID = cLoginID Then
        
            myRST.Delete
            Me.togWatchList.BackColor = constGrey
            
        End If
        myRST.MoveNext
        Loop
        
     End If
     
        
End If


End Sub



