Private Sub GEOriginalDocket_AfterUpdate()

    If Me.GEOriginalDocket < 300000 Then Me.GEOriginalDocket = Me.GEOriginalDocket + 300000
    
    myid = DLookup("ID", "dbo_ProjectHeader", "[DocketNum] = " & Me.GEOriginalDocket)
    If Nz(myid) = 0 Then
        MsgBox "Docket Not Found.", vbCritical, ProgramName
        Me.GEOriginalDocket = Null
        DoCmd.GoToControl "GEOriginalDocket"
        Exit Sub
    End If
    

    myContactGeneralID = DLookup("[ContactGeneralID]", "dbo_ProjectHeader", "[DocketNum] = " & Me.GEOriginalDocket)
    myProjectName = DLookup("[ProjectName]", "dbo_ProjectHeader", "[DocketNum] = " & Me.GEOriginalDocket)
    mycompany = DLookup("[Compname]", "dbo_ContactGeneral", "[id] = " & myContactGeneralID)
    
    If myContactGeneralID <> Me.ContactGeneralID Then
    
        myPrompt = "You cannot assign a GE docket when the client doesn't match the original docket. Please Edit the Sold To: to match the original docket." & vbCrLf & "Original Client: " & mycompany
        MsgBox myPrompt, vbCritical, ProgramName
        Me.GEOriginalDocket = Null
        DoCmd.GoToControl "GEOriginalDocket"
        Exit Sub
    End If
    

    myPrompt = "Found Docket " & GEOriginalDocket & vbCrLf & "Client: " & mycompany & vbCrLf & "Project: " & myProjectName & vbCrLf & vbCrLf & "PLEASE CONFIRM"
    myResponse = vbNo
    myResponse = MsgBox(myPrompt, vbYesNo, ProgramName)
    If myResponse = vbNo Then
        MsgBox "Cancelling..", vbInformation, ProgramName
        Me.GEOriginalDocket = Null
        DoCmd.GoToControl "GEOriginalDocket"
    Else
        Dim MYDB As Database
        Dim myRST As Recordset
        Set MYDB = CurrentDb()
        
        mysql = "SELECT * FROM DBO_PROJECTHEADER WHERE ID = " & myid
        
        Set myRST = MYDB.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
        
        myRST.Edit
        myRST!GESource = True
        myRST.Update
        myRST.Close
        
        
        AddContactHistory Me.ContactGeneralID, Nz(Me.BillAddLocationID), Nz(Me.BillOrderedBy), Me.ID, 3, "Assigned Docket as a GE Docket."
        AddContactHistory Me.ContactGeneralID, Nz(Me.BillAddLocationID), Nz(Me.BillOrderedBy), Val(myid), 3, "Assigned Docket as a GE Source (Problem) Docket."
        
    
        
           
    End If
    
  
        
        
             



    
    
    If Me.Dirty Then Me.Dirty = False
