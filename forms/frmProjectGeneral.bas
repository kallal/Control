    'create Username
    myUsername = ""
    i = 1
    Do Until done = True 'Get first five characters
        If Mid(myCompname, i, 1) <> " " Then
            myUsername = myUsername & Mid(myCompname, i, 1)
        End If
        If Len(myUsername) >= 5 Then done = True
        i = i + 1
        If Len(myCompname) < i Then done = True 'check to make sure we don't have a short company name.
    Loop
    
    Randomize
    
    myUsername = myUsername + Trim(str(Me.DocketNum)) 'Add docket number to end of Username
    
    myPassword = ""
    'create Password
        For i = 1 To 6
            myPassword = myPassword & Mid(myPwdChars, Int(Rnd * Len(myPwdChars)), 1)
        Next i
    myrs!Docket_Username = myUsername
    myrs!Docket_Password = myPassword
    myrs.Update
    
    'Also, create folder structure on the old server for now.
    myrs.MoveFirst
    mydir = "\\srv-edm-0001\public\Dockets\0" & Trim(myrs!Docket_number) & " " & Trim(myrs!Docket_Company) & " " & Format(myrs!Docket_Date, "m.d.yyyy")
    
