Private Sub Form_Load()

DoCmd.SetWarnings False 'Turn off screen warnings while running Make-Table Queries
DoCmd.OpenQuery ("qryMtCboEmployeeSalesRep")
DoCmd.OpenQuery ("qryMtCboEmployeeCSR")

DoCmd.SetWarnings True

Dim MYDB As Database
Dim myRST As Recordset
Set MYDB = CurrentDb()
Set myRST = MYDB.OpenRecordset("srchFrmWorkInProgressReadyToInvoice", dbOpenDynaset, dbSeeChanges)
With myRST

.MoveFirst
Me.txtCompanyID = !txtCompanyID
Me.txtCsrID = !txtCsrID
Me.txtDateEnd = !txtDateEnd
Me.txtDateStart = !txtDateStart
Me.txtProjectName = !txtProjectName
Me.txtSalesRepID = !txtSalesRepID
Me.fraShowBy = !fraShowBy

.Close
End With

'Set Up Location Radio Buttons
Dim myLoc As Recordset
Set myLoc = CurrentDb.OpenRecordset("dbo_CompanyLocation", dbOpenSnapshot)
myLoc.MoveFirst



For i = 1 To 9
    myradiobut = "Loc" & Trim(i)
    mylabel = "lblLoc" & Trim(i)
    If Nz(myLoc!LocationCode, "") <> "" Then
        Me(myradiobut).enabled = True
        Me(mylabel).caption = myLoc!LocationCode
        Me(mylabel).BackColor = myLoc!ReportColor
        Me(myradiobut).visible = True
        Me(mylabel).visible = True
    Else
        Me(myradiobut).enabled = False
        Me(myradiobut).visible = False
