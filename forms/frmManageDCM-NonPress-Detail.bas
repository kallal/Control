    Else
        Me.SubFieldName1.RowSource = ""
        Me.SubFieldName2.RowSource = ""
        Me.SubFieldName3.RowSource = ""
    End If
End If

End Sub

Private Sub SubDatabase_BeforeUpdate(Cancel As Integer)
If Nz(Me.SubFieldName1, "") <> "" Or Nz(Me.SubFieldName2, "") <> "" Or Nz(Me.SubFieldName3, "") <> "" Or Nz(Me.SubFieldData1, "") <> "" Or Nz(Me.SubFieldData2, "") <> "" Or Nz(Me.SubFieldData3, "") <> "" Then
    myResponse = vbNo
    myResponse = MsgBox("If you change the database for this service, all triggers will be cleared. Continue?", vbYesNo, ProgramName)
    If myResponse = vbNo Then
        Me.Undo
        Cancel = True
    Else
        Me.SubFieldData1 = ""
        Me.SubFieldData2 = ""
        Me.SubFieldData3 = ""
        Me.SubFieldName1 = ""
        Me.SubFieldName2 = ""
        Me.SubFieldName3 = ""
    End If
End If

