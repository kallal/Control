Private Sub frmDateSelection_AfterUpdate()
Select Case Me.frmDateSelection

    Case 0 'Custom Date
        Me.srchDateStart.enabled = True
        Me.srchDateEnd.enabled = True
        DoCmd.GoToControl "srchDateStart"
    Case 1 'This Month
        Me.srchDateStart.enabled = False
        Me.srchDateEnd.enabled = False
        Me.srchDateStart = DateSerial(Year(Date), Month(Date), 1)
        Me.srchDateEnd = Date
        DoCmd.GoToControl "srchCustomer"
    Case 2 'Last Month
        Me.srchDateStart.enabled = False
        Me.srchDateEnd.enabled = False
        Me.srchDateStart = DateSerial(Year(Date), Month(Date) - 1, 1)
        Me.srchDateEnd = DateSerial(Year(Date), Month(Date), 0)
        DoCmd.GoToControl "SrchCustomer"
    Case 3 'Calendar Year
        Me.srchDateStart.enabled = False
        Me.srchDateEnd.enabled = False
        Me.srchDateStart = DateSerial(Year(Date), 1, 1)
        Me.srchDateEnd = DateSerial(Year(Date), Month(Date), 0)
        DoCmd.GoToControl "SrchCustomer"
    Case 4 'Fiscal Year
        Me.srchDateStart.enabled = False
        Me.srchDateEnd.enabled = False
        myFiscalYearStartMonth = 6 ' Change this to reflect actual Fiscal Year coding. - hard coded for June.
        If Month(Date) < myFiscalYearStartMonth Then
            Me.srchDateStart = DateSerial(Year(Date) - 1, myFiscalYearStartMonth, 1)
        Else
            Me.srchDateStart = DateSerial(Year(Date), myFiscalYearStartMonth, 1)
        End If
        
        Me.srchDateEnd = DateSerial(Year(Date), Month(Date), 0)
        DoCmd.GoToControl "SrchCustomer"
        
    Case 5 'Last Fiscal Year
        Me.srchDateStart.enabled = False
        Me.srchDateEnd.enabled = False
        myFiscalYearStartMonth = 6 ' Change this to reflect actual Fiscal Year coding. - hard coded for June.
        If Month(Date) < myFiscalYearStartMonth Then
            Me.srchDateStart = DateSerial(Year(Date) - 2, myFiscalYearStartMonth, 1)
        Else
            Me.srchDateStart = DateSerial(Year(Date) - 1, myFiscalYearStartMonth, 1)
        End If
        
        If Month(Date) < myFiscalYearStartMonth Then
            Me.srchDateEnd = DateSerial(Year(Date) - 1, myFiscalYearStartMonth, 0)
        Else
            Me.srchDateEnd = DateSerial(Year(Date) - 0, myFiscalYearStartMonth, 0)
        End If
        
        
        'Me.srchDateEnd = DateSerial(Year(Date), Month(Date), 0)
        DoCmd.GoToControl "SrchCustomer"

End Select

        
End Sub

