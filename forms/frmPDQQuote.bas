Private Sub cmdAssign_Click()


'Check if XML file exists


myPDQNumber = Me!txtQuoteNumber

mypdqlocation = cPDQQuoteXMLFolder & "\" 'constPDQxmlfiles


myQuantity = str(Me.primaryQty)

myFileName = mypdqlocation & "PDQ" & myPDQNumber & "_" & Trim(myQuantity) & ".xml"


'MsgBox myFileName
'MsgBox Len(Dir(myFileName))


If Len(Dir(myFileName)) = 0 Then
    cPDQImportResult = False
    MsgBox "Your XML File named " & myFileName & " does not exist. Cancelling.", vbCritical, ProgramName
    Exit Sub
End If

myAdditionalQtys = Me.additionalQtys

If Len(myAdditionalQtys) > 0 Then
    done = False
    
    Do Until done = True
    
        a = InStr(myAdditionalQtys, ",")
        If a > 0 Then
            myqty = Left(myAdditionalQtys, a - 1)
            myAdditionalQtys = Right(myAdditionalQtys, Len(myAdditionalQtys) - a)
         
    
        Else
            myqty = Val(myAdditionalQtys)
            done = True
        End If
        
        myFileName = mypdqlocation & "PDQ" & myPDQNumber & "_" & Trim(myqty) & ".xml"
        
        If Len(Dir(myFileName)) = 0 Then
            cPDQImportResult = False
            MsgBox "Your XML File named " & myFileName & " does not exist. Cancelling.", vbCritical, ProgramName
            Exit Sub
        End If
    
    Loop


End If 'Check for additional quantity XML files


'Check if there is already a quote assigned - if so, confirm.
TMPIDTOCLEAN = 0


TMPIDTOCLEAN = Forms!navmaster.Form!navSubform.Form!NavProject!ExtQuoteNum



'Insert the ID numbers into the Component Header for Xref
Forms!navmaster.Form!navSubform.Form!NavProject!ExtQuoteNum = Me.ID
Forms!navmaster.Form!navSubform.Form!NavProject!ExtQuoteNumDisplay = Me.txtQuoteNumber


'Copy from the component header to show the Xref on this side
Me.ProjectID = Forms!navmaster.Form!navSubform.Form!NavProject!ProjectHeaderID
Me.ComponentID = Forms!navmaster.Form!navSubform.Form!NavProject!ID

'Populate the Additional Quantities on the Component Headers


tmpDesc = ""

Dim MYDB As Database
Dim myRST As Recordset

Set MYDB = CurrentDb()
Set myRST = MYDB.OpenRecordset("tblQuoteSections", dbOpenDynaset, dbSeeChanges)

myRST.MoveFirst

myRST.FindNext ("[quoteid] = " & Me.ID)

done = False

Do Until done = True
    tmpDesc = tmpDesc & myRST!SectionName & " (Final Size): "
    
    'FYI.... 1 millimeter = 0.0393700787 inches - Round to nearest 1/8 of inch
    tmpSizeX = Round((myRST!sizeX) * 0.0393700787 * 16) / 16
    tmpSizeY = Round((myRST!sizeY) * 0.0393700787 * 16) / 16
    tmpDesc = tmpDesc & tmpSizeX & Chr(34) & " x " & tmpSizeY & Chr(34) & " "
    tmpDesc = tmpDesc & myRST!Pages & " Page(s) " & IIf(myRST!SectionType = "FoldedItem", " (Folded)", "") & vbCrLf
    tmpDesc = tmpDesc & "Printed on : " & myRST!PaperName & vbCrLf
    tmpDesc = tmpDesc & "Colours: "
    tmpDesc = tmpDesc & IIf((myRST!ColourCYMKFront > 0 Or myRST!ColourCYMKBack > 0), "CMYK " & myRST!ColourCYMKFront & "/" & myRST!ColourCYMKBack, "")
    tmpDesc = tmpDesc & IIf((myRST!ColourPMSFront > 0 Or myRST!ColourPMSBack > 0), "PMS " & myRST!ColourPMSFront & "/" & myRST!ColourPMSBack, "")
    
    
    
    tmpDesc = tmpDesc & vbCrLf & vbCrLf
    myRST.FindNext ("[quoteid] = " & Me.ID)
    If myRST.NoMatch Then done = True
 
    
Loop

'MsgBox tmpDesc




'zero out all the pricing shown

For i = 1 To 5
    tmpFieldName = "ExtQuoteAmt" & i
    Forms!navmaster.Form!navSubform.Form!NavProject(tmpFieldName) = Null
    tmpFieldName = "ExtQuoteQty" & i
    Forms!navmaster.Form!navSubform.Form!NavProject(tmpFieldName) = Null
    tmpFieldName = "ExtQuoteSelect" & i
    Forms!navmaster.Form!navSubform.Form!NavProject(tmpFieldName) = Null
    
    If Forms!navmaster.Form!navSubform.Form!NavProject!StartDate <> Null Then
        tmpFieldName = "ExtQuoteSell" & i
        Forms!navmaster.Form!navSubform.Form!NavProject(tmpFieldName) = Null
    End If
Next i



'Populate up all the pricing levels using the database tblPrice

Dim myRST2 As Recordset
Set myRST2 = MYDB.OpenRecordset("tblPrice", dbOpenDynaset, dbSeeChanges)

myRST2.FindNext ("[quoteid] = " & Me.ID)
done = False
i = 1

Do Until done = True
    tmpFieldName = "ExtQuoteAmt" & i
    Forms!navmaster.Form!navSubform.Form!NavProject(tmpFieldName) = myRST2!sellat
    tmpFieldName = "ExtQuoteQty" & i
    Forms!navmaster.Form!navSubform.Form!NavProject(tmpFieldName) = myRST2!Qty
    
    If Forms!navmaster.Form!navSubform.Form!NavProject!StartDate <> Null Then
    tmpFieldName = "ExtQuoteSell" & i
    Forms!navmaster.Form!navSubform.Form!NavProject(tmpFieldName) = Round(myRST2!sellat + (myRST2!sellat * (Forms!navmaster.Form!navSubform!DefaultDiscount / 100)))
    End If
    
    
    If myRST2!Qty = Val(Me.primaryQty) Then
        tmpFieldName = "ExtQuoteSelect" & i
        Forms!navmaster.Form!navSubform.Form!NavProject(tmpFieldName) = True
        Forms!navmaster.Form!navSubform.Form!NavProject!ExpectedQty = myRST2!Qty
    End If
    
    
    
    myRST2.FindNext ("[quoteid] = " & Me.ID)
    i = i + 1
    If myRST2.NoMatch Then done = True
    
    
Loop
    


'Select the "Primary Price" and highlight it.


Forms!navmaster.Form!navSubform.Form!NavProject!ComponentName = Me.jobTitle
'Forms!navMaster.Form!navSubform.Form!NavProject!FriendlyDescription = tmpDesc ' do not update description here - it's done now in the XML import.

'find the tmpIDToClean and get rid of entries on it.


'MsgBox "Clean out: " & TMPIDTOCLEAN
If TMPIDTOCLEAN > 0 Then


Me.ID.enabled = True
Me.ID.visible = True
DoCmd.GoToControl "[ID]"
'MsgBox "Current Project ID: " & Me.ProjectID

DoCmd.FindRecord TMPIDTOCLEAN
'MsgBox "moved to: " & Me.ID

'Cleanout not working - try later.
'Me.ProjectID = Null
'Me.ComponentID = Null
'Me.NotUsed = False

'MsgBox "Old Project ID (Cleaned out)" & Me.ProjectID
End If


DoCmd.Close
End Sub

