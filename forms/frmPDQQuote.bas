Private Sub cmdSearch_Click()

Dim myfilter As String

myfilter = "1=1"

If Me.cmdShow = 1 Then
    myfilter = myfilter & " AND nz([ProjectID])=0 "
End If

If Me.cmdShow = 2 Then
    myfilter = myfilter & " AND nz([ProjectID]) > 0"
End If

If Me.cmdShow = 3 Then
End If

mytxtFindNumber = Val(Nz(Me.txtFindNumber, 0))

If mytxtFindNumber > 0 Then
    myfilter = myfilter & " AND [ID] = " & mytxtFindNumber
End If

If Len(Nz(Me.txtFindClient, "")) > 0 Then
    myfilter = myfilter & " AND [CUSTOMERNAME] LIKE '*" & Trim(Me.txtFindClient) & "*' "
End If

'MsgBox myfilter

Me.Filter = myfilter
Me.FilterOn = True

