myRST.FindFirst ("[ID] = " & Me.ProjectComponentHeaderID)
If myRST.NoMatch = False Then
    myRST.Edit
    
    Select Case Me.Category
       Case 1
            myRST!StStock = Me.Description.Column(1)
       Case 2
            myRST!StCreative = Me.Description.Column(1)
        Case 3
            myRST!StPrepress = Me.Description.Column(1)
        Case 4
            myRST!StMailData = Me.Description.Column(1)
        Case 5
            myRST!StPrint = Me.Description.Column(1)
        Case 6
            myRST!StOutsideSrv = Me.Description.Column(1)
        Case 7
            myRST!StLFFinish = Me.Description.Column(1)
        Case 8
            myRST!StBindery = Me.Description.Column(1)
        Case 9
            myRST!StMailShop = Me.Description.Column(1)
        

    End Select
    myRST.Update
 
End If

myRST.Close

DoCmd.Close

End Sub

