Private Sub Form_Load()
DoCmd.SetWarnings False 'Turn off screen warnings while running Make-Table Queries
DoCmd.OpenQuery ("qryMTCboTaskListForStation")

DoCmd.SetWarnings True


Dim mydata As String
Dim MyField As String
Dim myType As Integer


If Me.OpenArgs <> "" Then
    
    tmpArg = Me.OpenArgs ' Set opening value
    done = False
    
    
    
    Do While done = False
    
        ' Parse out myType, myField and myData
        myType = Left(tmpArg, 1)
        tmpArg = Right(tmpArg, Len(tmpArg) - 1)
        
        MyField = Left(tmpArg, InStr([tmpArg], "|") - 1)
        tmpArg = Right(tmpArg, Len(tmpArg) - InStr([tmpArg], "|"))
        mydata = Left(tmpArg, InStr([tmpArg], "|") - 1)
        tmpArg = Right(tmpArg, Len(tmpArg) - InStr([tmpArg], "|"))
        
        If myType = 2 Then ' 1= data, 2 = default value
            Me(MyField).DefaultValue = mydata
        Else
            Me(MyField) = mydata
        End If
        
        If Len(Trim(tmpArg)) = 0 Then done = True
    
    Loop
       
 
End If




If rsLocSS!LoginID = 70 Then 'Only show task user options if it is a Production Staff machine. (uses Roger's id)
    Me.CompanyEmployeeID.visible = True
    Me.lbl__lblEmployee.visible = True
    If Nz(rsLocSS!tmpsTationid, 0) > 0 Then  'Get list of logged in employees at this station
        Me!CompanyEmployeeID = Null
        mysql = "Select CompanyEmployeeID, FULLNAME FROM qryCompanyEmployeeShiftLive WHERE [StationID] = " & rsLocSS!tmpsTationid
        Me!CompanyEmployeeID.RowSource = mysql
    End If

Else
    Me.CompanyEmployeeID.visible = False
    Me.lbl__lblEmployee.visible = False
    
End If


mycriteria = "SELECT * FROM tblCboTaskListForStation WHERE [CategoryID] = " & Me.Category

'MsgBox mycriteria


Me!Description.RowSource = mycriteria





End Sub