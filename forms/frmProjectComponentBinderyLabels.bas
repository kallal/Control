Option Compare Database

Private Sub cmdPrint_Click()
If Me.Dirty Then Me.Dirty = False

'Confirm all data is available

If Nz(Me.txtFullBoxQty) = 0 Then
    MsgBox "You need to provide a count per box before printing.", vbCritical, ProgramName
    Exit Sub
End If


'Update the BCData and BCDataLast Boxes
Me.BCData = "*C" & Format([ID], "0000000") & "Q" & Format([txtFullBoxQty], "00000") & "*"
Me.BCDataLast = "*C" & Format([ID], "0000000") & "Q" & Format([txtResidueCount], "00000") & "*"


'Delete all old data in temporary database
DoCmd.SetWarnings False
DoCmd.RunSQL ("DELETE * FROM tmp_ProjectComponentBinderyLabel")
DoCmd.SetWarnings True

'Populate up the temporary Database to print from.
Dim MYDB As Database
Dim myRST As Recordset

Set MYDB = CurrentDb()
Set myRST = MYDB.OpenRecordset("tmp_ProjectComponentBinderyLabel", dbOpenDynaset, dbSeeChanges)


'set the printed quantity of boxes to include the residue box
tmpTotalBoxCount = Me.txtFullBoxCount
If Me.txtResidueCount > 0 Then tmpTotalBoxCount = Me.txtFullBoxCount + 1


'Create one record for each label so that the label report can run continuously
For i = 1 To Me.txtFullBoxCount
    myRST.AddNew
    myRST!CompanyName = Me.txtCompanyName
    myRST!ComponentName = Me.txtComponentName
    myRST!ProjectName = Me.txtProjectName
    myRST!BoxQty = Me.txtFullBoxQty
    myRST!BoxNumber = i
    myRST!TotalBoxQty = tmpTotalBoxCount
    myRST!PackageDate = Now()
    myRST!BCData = "*C" & Format([ID], "0000000") & "Q" & Format([txtFullBoxQty], "00000") & "*"
    myRST.Update
    
Next i

'Add one more record for residue if required
If Me.txtResidueCount > 0 Then
    myRST.AddNew
    myRST!CompanyName = Me.txtCompanyName
    myRST!ComponentName = Me.txtComponentName
    myRST!ProjectName = Me.txtProjectName
    myRST!BoxQty = Me.txtResidueCount
    myRST!BoxNumber = i
    myRST!TotalBoxQty = tmpTotalBoxCount
    myRST!PackageDate = Now()
    myRST!BCData = "*C" & Format([ID], "0000000") & "Q" & Format([txtResidueCount], "00000") & "*"
    myRST.Update
End If


'Call the print job

If Me.LabelFormat = 1 Then
   ' MsgBox "1 Up"
    DoCmd.OpenReport "rptBoxLabel_1up", acViewNormal
    
Else
  '  MsgBox " 6 Up"
    DoCmd.OpenReport "rptBoxLabel_6up", acViewPreview
End If

'DoCmd.Close acForm, "frmProjectComponentBinderyLabels"

End Sub

Private Sub Form_Load()
Me.txtCompanyName = DLookup("CompName", "dbo_ContactGeneral", "[ID] = " & [txtClientID])

Me.txtProjectName = DLookup("[ProjectName]", "dbo_ProjectHeader", "[ID] = " & [ProjectHeaderID])


If cControlCompanyCode = "DPI" Then
    Me.LabelFormat = 2
    Me.txtComponentName = Me.ComponentName
Else
    Me.LabelFormat = 1
    Me.txtComponentName = ""
End If

DoCmd.GoToControl "txtFullBoxQty"

End Sub

