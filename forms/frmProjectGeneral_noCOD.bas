Private Sub cmdSendMailDataUpload_Click()
'Mail Data Path = Z:\Inetpub\wwwroot\docket\_DocketSystem
'Filename for upload = docket.mdb

'Database is MailDocketUploadCredentials

If Nz(Me.DocketNum, 0) = 0 Then
    MsgBox "You can only send uploads on Dockets - This job has no docket number yet.", vbCritical, ProgramName
    Exit Sub
End If

Dim mysql As String
Dim myrs As Recordset



myPwdChars = "1234567890abcdefghjkmnpqrstuvwxyz" 'don't use i or o or l
mysql = "SELECT * from MailDocketUploadCredentials WHERE DOCKET_NUMBER = " & Me.DocketNum
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
If myrs.EOF Then
    'no records yet - need to create a new credential
    myrs.AddNew
    myrs!Docket_number = Me.DocketNum
    myrs!Docket_Quote_Number = Me.QuoteNum
    myCompname = DLookup("CompName", "dbo_ContactGeneral", "[ID] = " & Me.ContactGeneralID)
    myrs!Docket_Company = myCompname
    myrs!Docket_Date = Me.DocketDate
    'create Username
    myUsername = ""
    i = 1
    Do Until done = True 'Get first five characters
        If Mid(myCompname, i, 1) <> " " Then
            myUsername = myUsername & Mid(myCompname, i, 1)
        End If
        If Len(myUsername) >= 5 Then done = True
        i = i + 1
        If Len(myCompname) < i Then done = True 'check to make sure we don't have a short company name.
    Loop
    
    Randomize
    
    myUsername = myUsername + Trim(str(Me.DocketNum)) 'Add docket number to end of Username
    
    myPassword = ""
    'create Password
        For i = 1 To 6
            myPassword = myPassword & Mid(myPwdChars, Int(Rnd * Len(myPwdChars)), 1)
        Next i
    myrs!Docket_Username = myUsername
    myrs!Docket_Password = myPassword
    myrs.Update
    
    'Also, create folder structure on the old server for now.
    myrs.MoveFirst
    mydir = "\\srv-edm-0001\public\Dockets\0" & Trim(myrs!Docket_number) & " " & Trim(myrs!Docket_Company) & " " & Format(myrs!Docket_Date, "m.d.yyyy")
    
    On Error Resume Next
    
    MkDir mydir
    MkDir mydir & "\Data"
    On Error GoTo 0
    
    
    
End If 'Finished creating user and password
    
myrs.MoveFirst 'should have a record now.

'Create email



mySubject = ""

If Nz(Me.QuoteNum) > 0 Then mySubject = "E" & Me.QuoteNum
If Nz(Me.DocketNum) > 0 Then mySubject = "D" & Me.DocketNum
'If Nz(Me.InvoiceNum) > 0 Then mySubject = "I" & Me.InvoiceNum
