Private Sub BillAddLocationID_BeforeUpdate(Cancel As Integer)

resp = vbYes

If Len(Me.BillAdd1) > 0 Then ' Check to see if there's already something there before overriding
resp = MsgBox("You already have a billing address. Ok to change?", vbYesNo, ProgramName)
End If

If resp = vbNo Then ' Cancel the change
Me.BillAddLocationID.Undo
DoCmd.CancelEvent
End If

Me.BillOverride = False ' turn off the override flag
End Sub

Private Sub BillOrderedBy_AfterUpdate()
If Me.ShipToSameAsBillTo = True Then
    Me.ShipAttn = Me.BillOrderedBy.Column(1)
End If
If Nz(Me.BillOrderedBy) > 0 Then
    Me.cmdCall.caption = "CALL: " & Me.BillOrderedBy.Column(4)
    Me.cmdCall.enabled = True
    Me.cmdEmailGeneral.caption = "EMAIL: " & Me.BillOrderedBy.Column(2)
    Me.cmdEmailGeneral.enabled = True
Else
    Me.cmdCall.enabled = False
    Me.cmdEmailGeneral.enabled = False
End If

If Me.Dirty Then Me.Dirty = False
End Sub

Private Sub BillOverride_AfterUpdate()
If BillOverride = False Then
    Call BillAddLocationID_AfterUpdate
End If

'Call Form_Current

If BillOverride = True Then
    DoCmd.GoToControl "BillAdd1"
End If
If Me.Dirty Then Me.Dirty = False
End Sub

Private Sub BillPC_AfterUpdate()
If Len(Me!BillPC) = 6 Or Len(Me!BillPC) = 7 Then
    Me!BillPC = UCase(Left(Me!BillPC, 3) & " " & Right(Me!BillPC, 3))
End If

If Len(Me!BillPC) = 9 Then
    Me!BillPC = Left(Me!BillPC, 5) & "-" & Right(Me!BillPC, 4)
End If
If Me.Dirty Then Me.Dirty = False
End Sub

Private Sub chkKeepInProductionYN_Click()
If Me.chkKeepInProductionYN = False Then
    Me.ProjectStatusID = 39
    Exit Sub
End If

myResponse = vbNo
myResponse = MsgBox("Are you sure you want to put this Docket back into Production? It has already been invoiced, so when the job is complete, simply uncheck the box.", vbYesNo, ProgramName)
If myResponse = vbYes Then
    Me.ProjectStatusID = 21
Else
    Me.chkKeepInProductionYN = False
End If

End Sub

