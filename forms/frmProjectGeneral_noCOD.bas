    'confirm removal from watch list
    a = vbNo
    a = MsgBox("Are you sure you want to remove this project from your Watch List?", vbQuestion + vbYesNo + vbDefaultButton2, ProgramName)
    If a = vbYes Then
     
    'delete record from watch list

        myRST.MoveFirst
        Do Until myRST.EOF
               
        If myRST!ProjectHeaderID = Me.ID And myRST!EmployeeID = cLoginID Then
        
            myRST.Delete
            Me.togWatchList.BackColor = constGrey
            
        End If
        myRST.MoveNext
        Loop
        
     End If
     
        
End If


End Sub



Public Function RibRun(sFunc As String)

   ' runs the passed fucntion name
   ' this routine thus allows code to "stay" in the form
   
   Select Case sFunc
   
      Case "togWatchList_Click"
         Call togWatchList_Click
               
      Case "cmdCreateDocket_Click"
         Call cmdCreateDocket_Click
         
      Case "cmdCreateInvoice_Click"
         Call cmdCreateInvoice_Click
         
      Case "cmdPrintInvoice_Click"
         Call cmdPrintInvoice_Click
         
      Case "cmdEmailInvoice_Click"
         Call cmdEmailInvoice_Click
      
      Case "cmdCreateToDo_Click"
         Call cmdCreateToDo_Click
             
      Case Else
   End Select

End Function


Public Sub UpdateCboBoxes()

DoCmd.SetWarnings False

'disconnect from combo boxes for the ones we are going to run
Set Me.BillAddLocationID.Recordset = Nothing
Set Me.BillOrderedBy.Recordset = Nothing
Set Me.ProofToID.Recordset = Nothing
Set Me.ShipAddLocationID.Recordset = Nothing

'Run queries to update cbobox local tables
mysql = "SELECT dbo_ContactLocation.* INTO tblCboBillAddLocationID FROM dbo_ContactLocation WHERE dbo_ContactLocation.ContactGeneralID = " & ContactGeneralID & " ORDER BY dbo_ContactLocation.LocationName;"
DoCmd.RunSQL mysql

mysql = "SELECT dbo_ContactName.ID, [FN] & ' ' & [LN] AS FULLNAME, dbo_ContactName.Email, dbo_ContactName.PreferredName, dbo_ContactName.PhDirect, dbo_ContactName.FN INTO tblCboBillOrderedBy FROM dbo_ContactName WHERE dbo_ContactName.ContactGeneralID = " & ContactGeneralID & " ORDER BY [FN] & ' ' & [LN];"
DoCmd.RunSQL mysql

mysql = "SELECT dbo_ContactName.ID, [FN] & ' ' & [LN] AS FULLNAME, dbo_ContactName.Email, dbo_ContactName.PreferredName, dbo_ContactName.PhDirect, dbo_ContactName.FN INTO tblCboProofToID FROM dbo_ContactName WHERE dbo_ContactName.ContactGeneralID = " & ContactGeneralID & " ORDER BY [FN] & ' ' & [LN];"
DoCmd.RunSQL mysql

mysql = "SELECT dbo_ContactLocation.* INTO tblCboShipAddLocationID FROM dbo_ContactLocation WHERE dbo_ContactLocation.ContactGeneralID = " & ContactGeneralID & " ORDER BY dbo_ContactLocation.LocationName;"
DoCmd.RunSQL mysql

'reconnect comboboxes to their local tables.
Set Me.BillAddLocationID.Recordset = CurrentDb.OpenRecordset("tblCboBillAddLocationID", dbOpenSnapshot)
Set Me.BillOrderedBy.Recordset = CurrentDb.OpenRecordset("tblCboBillOrderedBy", dbOpenSnapshot)
Set Me.ProofToID.Recordset = CurrentDb.OpenRecordset("tblCboProofToID", dbOpenSnapshot)
Set Me.ShipAddLocationID.Recordset = CurrentDb.OpenRecordset("tblCboShipAddLocationID", dbOpenSnapshot)
 
DoCmd.SetWarnings True

End Sub

