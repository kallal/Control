Private Sub cmdReadyToSend_Click()
Dim myRecipient As String
Dim MyMessage As String
Dim mySubject As String
Dim MySuccess As String
Dim myFileName As String


Me.Refresh
'CHECK IF THERE IS A SALES REP AND A CSR - IF NOT - CANCEL OUT.

If Nz(Me.cboSalesRepID.Column(5), "") = "" Or Nz(Me.cboCSRID.Column(5), "") = "" Then
    MsgBox "You have no Sales Rep or CSR assigned to this job... Please correct and retry.", vbCritical, ProgramName
    Exit Sub
End If

myResponse = vbNo
myResponse = MsgBox("Mark this Estimate as Ready to Send. Are you sure?", vbQuestion + vbYesNo + vbDefaultButton2, ProgramName)
If myResponse = vbYes Then
    
    'Check how is this estimate to be handled. Do I send directly?
    If Me.EstimateTo = 1 Then 'Send email
        Call cmdEmailEstimate_Click
        Me.Requery
        
       
        Me.ProjectStatusID = 11
        Me.EstimateSent = Now
        Me.EstimateSentByID = cLoginID
        Me.LastContact = Now
        Me.LastContactByID = cLoginID
        Me.LastContactMethod = "Email"

    Else
        'Otherwise, mark as ready to send.
        'Send email to Sales/CSR? check with them.
        
        DoCmd.OpenForm "frmTempPleaseWait"
        Forms.frmTempPleaseWait.Repaint
        
        myMsgbox = "Estimate is Ready for Sales/CSR to send to client, and an automated email has been sent to the "
        Me.ProjectStatusID = 3
        
        myFileName = rsLocSS!FolderEstimate & "\E" & Format(Me.QuoteNum, "000000") & ".pdf"
        myReportName = "rptEstimate" & IIf(cControlCompanyCode <> "BG" And cControlCompanyCode <> "DPI", cControlCompanyCode, "") & IIf(Me.QuoteTotalsYN = True, "Totals", "")
        
        Application.Echo False
        DoCmd.Close acReport, myReportName
        
        
        DoCmd.OpenReport myReportName, acViewPreview, , "[ID] = " & Me.ID
        DoCmd.OutputTo acOutputReport, "", acFormatPDF, myFileName, False
        DoCmd.Close acReport, myReportName, acSaveYes
        
        Application.Echo True
        
        If Me.EstimateTo = 2 Then
            myRecipient = Me.cboCSRID.Column(5)
            myMsgbox = myMsgbox & "Inside Salesperson (CSR)."
        Else
            myRecipient = Me.cboSalesRepID.Column(5)
            myMsgbox = myMsgbox & "Sales Representative."
        End If
        

        
        mySubject = ""
        If Nz(Me.QuoteNum) > 0 Then mySubject = "E" & Me.QuoteNum
        If Nz(Me.DocketNum) > 0 Then mySubject = "D" & Me.DocketNum
        If Nz(Me.InvoiceNum) > 0 Then mySubject = "I" & Me.InvoiceNum
        mySubject = mySubject & " : " & Me.ContactGeneralID.Column(1) & " : " & Me.ProjectName
        MyMessage = "Estimate is ready to send to client. Please go to the 'Ready to Send' list and forward the Estimate to your client."
        
        MySuccess = SendMailCDO(myRecipient, mySubject, MyMessage, , , myFileName)
        
        If MySuccess = "Cancel" Then
            MsgBox "Email Failed", vbCritical, ProgramName
            Exit Sub
        Else
            Me.cmdReadyToSend.enabled = False
            Me.cmdReadyToSend.visible = False
        End If
        
        DoCmd.Close acForm, "frmTempPleaseWait"
        MsgBox myMsgbox, vbInformation, ProgramName 'Let user know we're done.
        
    End If
    
    
    

End If
If Me.Dirty Then Me.Dirty = False

End Sub

