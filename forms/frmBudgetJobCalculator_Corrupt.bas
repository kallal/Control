Private Sub cmdCalculate_Click()

Dim myDataOK As Boolean

Me.txtDiscPrice1 = Null
Me.txtDiscPrice2 = Null
Me.txtSetup = Null
Me.txtPricePerM = Null
Me.txtPricePerPiece = Null
Me.txtPostageRate = Null
Me.txtFinalQty = Null
Me.txtPrintTotal = Null
Me.txtMailTotal = Null
Me.txtPostageTotal = Null
Me.txtTotalGST = Null
Me.txtGrandTotal = Null

 
 
'check to make sure we have a full set of data.
myDataOK = True
If Nz(Me.txtTotalBudget, 0) = 0 Then myDataOK = False
If Nz(Me.txtVol1, 0) = 0 Then myDataOK = False
If Nz(Me.txtVol2, 0) = 0 Then myDataOK = False
If Nz(Me.txtPrice1, 0) = 0 Then myDataOK = False
If Nz(Me.txtPrice2, 0) = 0 Then myDataOK = False
Me.txtPostageRate = Val(Me.Postage) / 100

If Nz(Me.txtPostageRate, 0) = 0 Then myDataOK = False

If myDataOK = False Then
    'MsgBox "Need more info..", vbCritical
    Exit Sub
End If

'Got all the goods... let's do some work.

'Deduce the setup and price per piece numbers from the data points provided.

'Method- Algebraic notation to deduce X and Y from the equations
'X = Setup Charge, Y = Cost Per Piece
'Two Equations Provided
'X + Y(txtVol1) = TxtPrice1
'X + Y(txtVol2) = TxtPrice2

'Solve for X and Y

'Y = (TxtPrice2 - txtPrice1) / (txtVol2-TxtVol1)
'X = txtPrice1 -(Y * txtVol1) 'Can be solved after we know Y

Me.txtDiscPrice1 = Nz((Me.txtPrice1 + Me.txtMailPrice1) * (1 + Me.txtDisc))
Me.txtDiscPrice2 = Nz((Me.txtPrice2 + Me.txtMailPrice2) * (1 + Me.txtDisc))

myPricePerPiece = (((Me.txtPrice2 + Me.txtMailPrice2) * (1 + Me.txtDisc)) - ((Me.txtPrice1 + Me.txtMailPrice1) * (1 + Me.txtDisc))) / (Me.txtVol2 - Me.txtVol1)
myPricePerM = myPricePerPiece * 1000
mySetupCharge = ((Me.txtPrice1 + Me.txtMailPrice1) * (1 + Me.txtDisc)) - (myPricePerPiece * Me.txtVol1)

Me.txtSetup = mySetupCharge
Me.txtPricePerPiece = myPricePerPiece
Me.txtPricePerM = myPricePerM

'Now calculate what would fit for this job.
myTotalBudget = Me.txtTotalBudget
If Me.frmGSTIncluded = 1 Then
    myTotalBudget = myTotalBudget * 0.952381
End If


myFinalQty = Int((myTotalBudget - mySetupCharge) / (myPricePerPiece + (Me.Postage / 100)))
Me.txtFinalQty = myFinalQty
myPrintAndMailTotal = mySetupCharge + (myFinalQty * myPricePerPiece)

myPrintPercentage = Me.txtPrice1 / (Me.txtPrice1 + Me.txtMailPrice1)
myMailPercentage = Me.txtMailPrice1 / (Me.txtPrice1 + Me.txtMailPrice1)

Me.txtPrintTotal = myPrintAndMailTotal * myPrintPercentage
Me.txtMailTotal = myPrintAndMailTotal * myMailPercentage

Me.txtPostageTotal = myFinalQty * Me.txtPostageRate


If Me.frmGSTIncluded = 1 Then
    Me.txtTotalGST = (myPrintAndMailTotal + Me.txtPostageTotal) * 0.05
Else
    Me.txtTotalGST = 0
End If

Me.txtGrandTotal = Me.txtPrintTotal + Me.txtMailTotal + Me.txtPostageTotal + Me.txtTotalGST


'MsgBox "test"





End Sub

