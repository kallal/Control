Private Sub cboPaperFinish_AfterUpdate()

'if there is no Paper Finish, don't show anything.
Me.cboThicknessPoints.visible = False

'populate the combo box with available options once paper is chosen.

If Nz(Me.cboPaperFinish, "") <> "" Then
    mysql = "Select ID, ThicknessPts, WeightLbs FROM dbo_PaperBasicSizeThickness WHERE [Finish] = " & Chr(34) & Me.cboPaperFinish.Column(1) & Chr(34)
    Me.cboThicknessPoints.visible = True
    Me!cboThicknessPoints.RowSource = mysql
    Me.cboThicknessPoints = Null
End If

If Nz(Me.cboPaperFinish.Column(1), 0) > 0 Then
    If Me.cboPaperFinish.Column(0) <> Me.cboPaperType Then
        'MsgBox Me.cboPaperFinish.Column(0)
        'MsgBox Me.cboPaperType
        Me.cboPaperType = Null
        Me.BasicX = Null
        Me.BasicY = Null
        Me.txtWeightGSM = Null
        Me.txtWeightLbs = Null
    End If
End If


'Find the matching point size if it exists
mycriteria = "[Finish] = " & Chr(34) & Me.cboPaperFinish.Column(1) & Chr(34) & " AND [WeightLbs] = " & Val(Nz(Me.txtWeightLbs, 0))

myresult = DLookup("[ThicknessPts]", "dbo_PaperBasicSizeThickness", mycriteria)

If Nz(myresult, 0) <> 0 Then
    Me.cboThicknessPoints = DLookup("[ID]", "dbo_PaperBasicSizeThickness", mycriteria)
Else
    Me.cboThicknessPoints = Null
End If


End Sub

Private Sub cboPaperType_AfterUpdate()

If Nz(Me.cboPaperType, 0) <> 0 Then
    Me.BasicX = Me.cboPaperType.Column(2)
    Me.BasicY = Me.cboPaperType.Column(3)
    myOldFactor = Me.ConvFactor
    Me.ConvFactor = Me.cboPaperType.Column(4)
    myNewFactor = Me.ConvFactor
    Me.cboPaperFinish = Null
    Me.cboThicknessPoints = Null
    
End If
 'dsaf
 
'Recalc  from old type to new type if there is already a GSM in place.
If Nz(Me.txtWeightGSM, 0) > 0 Then
    Call txtWeightGSM_AfterUpdate
End If

Call cmdCalcSheet_Click
End Sub

Private Sub cboSheetSize_AfterUpdate()
Me.txtSheetSizeX = Me.cboSheetSize.Column(2)
Me.txtSheetSizeY = Me.cboSheetSize.Column(3)

Call cmdCalcSheet_Click

End Sub

Private Sub cboThicknessPoints_AfterUpdate()
'Check if you have both Paper Finish and Points... if so, you can update all of the top information.

If Nz(Me.cboPaperFinish, "") <> "" And Nz(Me.cboThicknessPoints, "") <> "" Then
    mypapertype = Me.cboPaperFinish.Column(0) 'Grab the numbered Paper Type
    Me.cboPaperType = mypapertype
    Me.BasicX = Me.cboPaperType.Column(2)
    Me.BasicY = Me.cboPaperType.Column(3)
    Me.ConvFactor = Me.cboPaperType.Column(4)
    
    Me.txtWeightLbs = Me.cboThicknessPoints.Column(2)
    Me.txtWeightGSM = Round(Me.txtWeightLbs * Me.ConvFactor)
    


End If

Call cmdCalcSheet_Click
    

End Sub

Private Sub cmdCalculateBasicSheet_Click()

If Nz(Me.cboPaperType, 0) = 0 Then Exit Sub 'no paper type defined yet.


End Sub



Private Sub cmdCalcSheet_Click()
Me.txtReamWeight = ((Me.txtSheetSizeX * Me.txtSheetSizeY) * Me.txtWeightLbs) / (Me.BasicX * Me.BasicY)
Me.txtReamWeightRounded = Round(Me.txtReamWeight)
Me.txtMWeight = Round(Me.txtReamWeight * 2) 'Weight of 1000 Sheets
Me.txtSheetsPerCWT = 1000 / (Me.txtMWeight / 100) ' sheets in 100lbs of paper

If Nz(Me.txtPriceCWT, 0) > 0 Then
    'Calculate price per 1000 Sheets)
    Me.txtPricePerM = Round(Me.txtPriceCWT * (1000 / Me.txtSheetsPerCWT), 2)
End If

End Sub

Private Sub txtPriceCWT_AfterUpdate()
If Nz(Me.txtPriceCWT, 0) > 0 Then
    'Calculate price per 1000 Sheets)
    Me.txtPricePerM = Round(Me.txtPriceCWT * (1000 / Me.txtSheetsPerCWT), 2)
End If

End Sub

Private Sub txtPricePerM_AfterUpdate()
If Nz(Me.txtPricePerM, 0) > 0 Then
    'Calculate price per 1000 Sheets)
    Me.txtPriceCWT = Round(Me.txtPricePerM * (Me.txtSheetsPerCWT / 1000), 2)
End If
End Sub

Private Sub txtSheetSizeX_AfterUpdate()
Me.cboSheetSize = Null
Call cmdCalcSheet_Click

End Sub

Private Sub txtSheetSizeY_AfterUpdate()
'Check if we have enough data  - if so...
Me.cboSheetSize = Null
Call cmdCalcSheet_Click


End Sub

Private Sub txtWeightGSM_AfterUpdate()
Me.txtWeightLbs = Round(Me.txtWeightGSM * (1 / Me.ConvFactor))
Call cmdCalcSheet_Click

End Sub

Private Sub txtWeightLbs_AfterUpdate()
Me.txtWeightGSM = Round(Me.txtWeightLbs * Me.ConvFactor)
Call cmdCalcSheet_Click

End Sub