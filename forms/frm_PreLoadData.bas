Private Sub cmdProceedToLogin_Click()

'Call relink code and set systemwide constants


If Me!SelectData = 1 Then
    Call cmdLinkData_Click
    constPDQXMLFiles = Me!PrimaryPDQXMLFiles
    constQuotePDFFiles = Me!PrimaryQuotePDFFiles
    constInvoicePDFFiles = Me!PrimaryInvoicePDFFiles
    
Else
    Call cmdLinkTestData_Click
    constPDQXMLFiles = Me!TestPDQXMLFiles
    constQuotePDFFiles = Me!TestQuotePDFFiles
    constInvoicePDFFiles = Me!TestInvoicePDFFiles
    
End If
'MsgBox constPDQXMLFiles



DoCmd.OpenForm "frmLogin"



End Sub


Private Sub Form_Load()
Forms!frm_preloaddata.visible = False

Call cmdProceedToLogin_Click
End Sub