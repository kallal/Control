Private Sub cmdLinkData_Click()

Dim MYDB As Database
Dim myTdf As TableDef
Dim myTdfs As TableDefs
Dim myDboDataLocation As String
Dim myPdqDataLocation As String

Set MYDB = CurrentDb()
Set myTdfs = MYDB.TableDefs

myDboDataLocation = Me.PrimaryDboDataLocation
myPdqDataLocation = Me.PrimaryPDQDataLocation


'MsgBox "Relinking data to Primary databases... Please wait", vbInformation, ProgramName
For Each myTdf In myTdfs

    'MsgBox " inside loop"
    
         If myTdf.SourceTableName <> "" Then 'If the table source is other than a base table
             
             If Left(myTdf.SourceTableName, 3) = "tbl" Then
                myTdf.connect = ";DATABASE=" & myPdqDataLocation 'Update PDQ Database locations only for databases starting with tbl
                Debug.Print "PDQ : " & myTdf.SourceTableName
             Else
                myTdf.connect = ";DATABASE=" & myDboDataLocation 'Update AxisMIS Database locations
                Debug.Print "DBO : " & myTdf.SourceTableName
            End If
         myTdf.RefreshLink 'Refresh the link
         End If
Next

'MsgBox "Done with relinking - proceed to AxisMIS", vbInformation, ProgramName


End Sub

