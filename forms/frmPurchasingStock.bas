Private Sub cboShowOnly_AfterUpdate()
Me.lblOrderinfo.visible = False
Me.cmdOrder.visible = False
Me.cmdRelease.visible = False
Me.tmpExpectedDate.visible = False
Me.cboVendorID.visible = False
Me.cboVendorNameID.visible = False
Me.myVendorOrderNumber.visible = False

Select Case cboShowOnly
    Case "READY TO ORDER"
        Me.Filter = "ORDEROPTION = 4 and (NZ(ORDERDATE,0)) = 0"
        Me.cmdOrder.visible = True
        Me.cmdOrder.caption = "ORDER"
        Me.tmpExpectedDate.visible = True
        Me.cboVendorID.visible = True
        Me.cboVendorNameID.visible = True
        Me.myVendorOrderNumber.visible = True
        Me.lblOrderinfo.visible = True
        Me.cmdOrder.visible = True
        
        
    Case "READY TO PRE-ORDER AND HOLD"
        Me.Filter = "ORDEROPTION = 5 AND (NZ(ORDERDATE,0)) = 0"
        Me.cmdOrder.visible = True
        Me.cmdOrder.caption = "PRE-ORDER"
        Me.cboVendorID.visible = True
        Me.cboVendorNameID.visible = True
        Me.myVendorOrderNumber.visible = True
        Me.tmpExpectedDate.visible = False
        Me.lblOrderinfo.visible = True
        Me.cmdOrder.visible = True
        
     Case "WAITING FOR RELEASE AT VENDOR"
        Me.Filter = "ORDEROPTION = 5 AND (NZ(ORDERDATE,0)) <> 0 and (nz(expecteddate,0)) = 0"
        Me.cmdOrder.visible = False
        Me.tmpExpectedDate.visible = True
        Me.cmdRelease.visible = True
        Me.tmpExpectedDate.visible = True
        
    Case "NOT PLANNED"
        Me.Filter = "ORDEROPTION = 1"
        Me.cmdOrder.visible = False
    Case "WAITING FOR CONFIRMATION"
        Me.Filter = "ORDEROPTION = 2"
        Me.cmdOrder.visible = False
    Case "INVENTORY REQUIRED" 'Will show up until invoiced.
        Me.Filter = "ORDEROPTION = 3"
        Me.cmdOrder.visible = False
    Case "ORDERED (BUT NOT IN)" 'Has Expected Date but no Arrival Date
        Me.Filter = "Nz(ExpectedDate, 0) <> 0 And Nz(ArrivalDate, 0) = 0"
        Me.cmdOrder.visible = False
        
    Case "ORDERED (IN, NOT INVOICED)" 'HAS EXPECTED DATE AND ARRIVAL DATE BUT NOT INVOICED (INVOICED DON'T COME THROUGH TO THIS QUERY)
        Me.Filter = "NZ(EXPECTEDDATE,0) <> 0 AND NZ(ArrivalDate, 0) <> 0"
        Me.cmdOrder.visible = False

End Select
Me.FilterOn = True
