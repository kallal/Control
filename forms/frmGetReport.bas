Private Sub Form_Load()

   ' if no passed report, then only show reports that are NOT
   ' set to a given printer type
   
   If Nz(Me.OpenArgs, "") = "" Then
      Me.chkAllReports = False
   End If
   
   FillListBox
   If (Nz(Me.OpenArgs, "") <> "") And (Nz(Me.OpenArgs, "") <> "ShowAll") Then
      ' this value is to highlight the list box
      Me.lstReports = Me.OpenArgs
   End If
   
      
End Sub


Public Function FillListBox()

   ' empty out array by re-dim.
   
   ReDim sReports(1, 0)
   
   ' first row is heading
   
   sReports(0, 0) = "Report"
   sReports(1, 0) = "Report Type"
   
   Dim i       As Integer
   Dim r       As AccessObject
   Dim srName  As String
   i = 1
   
   Dim rstR       As DAO.Recordset
   Set rstR = CurrentDb.OpenRecordset("tblReportPrinter", dbOpenDynaset, dbSeeChanges)
   
   For Each r In CurrentProject.AllReports
   
      srName = r.name
      If IsNull(Me.txtSearch) = False Then
         ' we have a text box filter - check it
         If InStr(r.name, Me.txtSearch) = 0 Then
            ' filter fail - blank out report name
            srName = ""
         End If
      End If
      
      ' skip any report that exists in current report list
      ' (again, if we find a match, blow out srName)
      If (Me.chkAllReports = False) And (srName <> "") Then
         ' we must skip any reprot that ALREADY has a setting (unless chkAllReports = true)
         rstR.FindFirst "Report = '" & srName & "'"
         If rstR.NoMatch = False Then
            srName = ""
         End If
      End If
      
      ' at this point if srName is blank, then it failed above
      ' options and we DO NOT add to list
      
      If srName <> "" Then
         ReDim Preserve sReports(1, i)
         sReports(0, i) = r.name
         i = i + 1
      End If
      
   Next
      
   ' at this point first column has report names.
   'now sort the first column, and skip sort of row 0 (that is heading)
   QuickSort sReports, 1, UBound(sReports(), 2)
   
   ' now this is sorted, grab any existing report that has settings
   ' we do this seperate since the sort above ONLY works on 1st column
   
   ' and only do this if we are to include reports all ready set
   
   If Me.chkAllReports = True Then
      For i = 1 To UBound(sReports(), 2)
         
         rstR.FindFirst "Report = '" & sReports(0, i) & "'"
         If rstR.NoMatch = False Then
            sReports(1, i) = rstR!PrinterType
         End If
      Next i
   End If
   
   rstR.Close
   
   
   Me.lstReports.RowSourceType = "comboload"       ' set rowsource type to function
   'Me.lstReports.Requery
   

End Function

' function for combo box
' this is a COMBO CALL BACK!!!

Function comboload(fld As control, ID As Variant, row As Variant, col As Variant, Code As Variant) As Variant


   Dim intOffset As Integer
   Select Case Code
      Case acLBInitialize           ' Initialize.
         comboload = True
         'MyTour.TourId = Me.id

      Case acLBOpen                 ' Open.
         comboload = Timer        ' Unique ID.
      Case acLBGetRowCount          ' Get rows.
         
         comboload = UBound(sReports(), 2) + 1
         
         
      Case acLBGetColumnCount    ' Get columns.
      
         comboload = UBound(sReports(), 1) + 1
               
      Case acLBGetColumnWidth    ' Get column width.

         Select Case col
            Case 0: comboload = 3600
            Case 1: comboload = 1500
            Case 2: comboload = 3000
         End Select
            
         
      Case acLBGetValue             ' Get the data.
         
         comboload = sReports(col, row)

   End Select
   
   
End Function

'---------------------------------------------------------------------------------------
' Procedure : QuickSort
' Author    : AlbertKallal
' Date      : 2016-12-14
' Purpose   : Quick Sort This sorted by decending the FIRST column of a given array
'             Only 1st row of data is perserved
'
'---------------------------------------------------------------------------------------
'

Sub QuickSort(arr, Lo As Long, Hi As Long)
    
   ' RECURsive quick sort.
   ' Lo (starting point of sort)
   ' Hi (ending point of sort)
   
    Dim varPivot        As Variant
    Dim varTmp          As Variant
    Dim tmpLow          As Long
    Dim tmpHi           As Long
    
    tmpLow = Lo
    tmpHi = Hi
    varPivot = arr(0, (Lo + Hi) \ 2)
    Do While tmpLow <= tmpHi
        Do While arr(0, tmpLow) < varPivot And tmpLow < Hi
            tmpLow = tmpLow + 1
        Loop
        Do While varPivot < arr(0, tmpHi) And tmpHi > Lo
            tmpHi = tmpHi - 1
        Loop
        If tmpLow <= tmpHi Then
            varTmp = arr(0, tmpLow)
            arr(0, tmpLow) = arr(0, tmpHi)
            arr(0, tmpHi) = varTmp
            tmpLow = tmpLow + 1
            tmpHi = tmpHi - 1
        End If
    Loop
    
    If Lo < tmpHi Then QuickSort arr, Lo, tmpHi
    If tmpLow < Hi Then QuickSort arr, tmpLow, Hi
End Sub

