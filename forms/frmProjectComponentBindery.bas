Private Sub cmdAllComplete_Click()
myResponse = vbNo

myResponse = MsgBox("Mark all Bindery tasks as complete. Are you sure?", vbYesNo + vbDefaultButton2, ProgramName)


    Dim MYDB As Database
    Dim myRST As Recordset
    Set MYDB = CurrentDb()
    Dim mysql As String
    mysql = "SELECT * FROM dbo_CompanyEmployeeTask WHERE [ProjectComponentHeaderID] = " & Me.ProjectComponentID
    Set myRST = MYDB.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
    
    mycriteria = "[ProjectComponentHeaderID] = " & Me.ProjectComponentID & " and NZ([endTime],0) = 0 AND [CATEGORY] = 8" '8 IS BINDERY
    myRST.FindFirst mycriteria
    If myRST.NoMatch = False Then
        'found somebody has a task open.
        MsgBox "A task is still in progress. Please end all BINDERY tasks in progress before clicking on this button.", vbCritical, ProgramName
        Exit Sub
    End If
    
    DoCmd.OpenForm "frmSelectNextOption"
    
    Dim myRST2 As Recordset
    Set myRST2 = MYDB.OpenRecordset("dbo_NextStatusOptions", dbOpenDynaset, dbSeeChanges)
    myRST2.MoveFirst
    myOptionNum = 1
    Do Until myRST2.EOF
        If myRST2!FinishLocation = 8 Then ' BINDERY = 8
            MyField = "Opt" & Trim(myOptionNum)
            myCaption = "Set " & DLookup("[TaskCategory]", "dbo_TaskCategory", "[ID] =" & myRST2!NextLocation) & " to " & myRST2!NextStatus
            
            Forms!frmSelectNextOption(MyField).caption = myCaption
            Forms!frmSelectNextOption(MyField).visible = True
            
            Forms!frmSelectNextOption(MyField).tag = myRST2!ID 'put the ID in the tag for ease of set afterwards.
            myOptionNum = myOptionNum + 1
        End If
        myRST2.MoveNext
    Loop
    
    
    myStatus = "COMP (" & Format(Date, "MMM D") & ")"
    Me.cboStatus = myStatus
    Parent.StBindery = myStatus

If Me.Dirty Then Me.Dirty = False
End Sub

