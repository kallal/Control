Private Sub Form_Open(Cancel As Integer)

   ' setup qtry and discount controls
    Dim strC      As String
    Dim s         As Integer
    
    If Nz(Me.extQuoteSelect, 0) > 0 Then
       s = Me.extQuoteSelect
       Me.ExtQuoteQty0.ControlSource = "ExtQuoteQty" & s
       Me.ExtQuoteAmt0.ControlSource = "ExtQuoteAmt" & s
       Me.ExtQuoteDisc0.ControlSource = "ExtQuoteDisc" & s
       Me.ExtQuoteSell0.ControlSource = "ExtQuoteSell" & s
       Me.ExtQuoteQtyTax0.ControlSource = "ExtQuoteQtyTax" & s
       Me.ExtQuoteQtyNonTax0.ControlSource = "ExtQuoteQtyNonTax" & s
       ' the comrate controls etc. do NOT need to be toucned, since they
       ' operate on above controls - and we changed the data source values of above
    End If

    ' now setup slider control (max/min)
    
    Dim curMin       As Currency
    Dim curmax       As Currency
    
    
    For s = 1 To 5
       strC = "ExtQuoteAmt" & s
       If Me(strC) < curMin Then
          curMin = Me(strC)
       End If
       
       If Me(strC) > curmax Then
          curmax = Me(strC)
       End If
       
   Next s
Debug.Print "max found = " & curmax

   ' we have min/max, setup slider
   With Me.GUISliderControl.Form
    'Geometry properties:
    .SlideWidth = 50
    .SlideHeight = 100
    .TrackLength = Me.ExtQuoteSell0.Width - 110
    .TrackHeight = 10
    .TickMarkThickness = 1 'points
    .MajorTickMarkLength = 120
    .MinorTickMarkLength = 80
    'IMPORTANT NOTE: set geometry properties before setting SliderValue.
    .MaxVal = Fix(curmax)
    .MinVal = Fix(curMin)
    '.SliderValue = Fix(Me.ExtQuoteSell0)       ' can't do this in on-open event
    .UpdateOnMove = True
    .bolIntOnly = True
    .intRoundTo = 5
    .ArrowKeyMovePerCent = 10
    
    'The next 2 lines of code below attempt to size the subform of the slider control.
    'This creates the border around the slider and is a bit problematic. On the parent
    'form you will probably need to play around with the size of the subform/subreport
    'control that contains the slider. It seems to helps if the subform/subreport control
    'height is a bit greater than the height of the slider.
    
    Me.GUISliderControl.Width = Me.ExtQuoteSell0.Width
    Me.GUISliderControl.Height = .InsideHeight
    
End With
   
    
    
    
End Sub

