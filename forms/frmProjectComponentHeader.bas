Dim mySQL4 As String
Dim myPDQProcessFlags As Recordset

mySQL4 = "SELECT * FROM qryDCMtoProcessFlags"
Set myPDQProcessFlags = CurrentDb.OpenRecordset(mySQL4, dbOpenDynaset, dbSeeChanges)

Dim mySubTable As Recordset

If Not (myComponentDetail.BOF And myComponentDetail.EOF) Then

    'Run loop to process flags.
    myComponentDetail.MoveFirst
    Do Until myComponentDetail.EOF 'Start of loop through myComponentDetail
        'Next line ignores Totals (99), Stock (1) as they do not get added to control's process flag areas.

        If myComponentDetail!ProjectComponentCategory <> 99 And myComponentDetail!ProjectComponentCategory <> 1 Then
            
            'Find match in myPDQProcessFlags database. If no match, offer to add it if you are system administrator. Otherwise offer to add it to the internal notes.

            mySearchString = "[ProcessName] = '" & myComponentDetail!name & "'"
            
            myPDQProcessFlags.FindFirst mySearchString
            
                        
            If myPDQProcessFlags.NoMatch = True Then
                
                'Ignore nomatch
                
                'msgbox "ProcessName = " & myComponentDetail!Name
                'msgbox "You are using a service which is no longer in the master database. Please open and resave your quote with a new number, and then try to assign the quote again to ensure all services are accounted for.", vbInformation, ProgramName
                'Add unhandled process to Internal Notes in case new DCM's are added without being accounted for.

            Else

                'Look for services that are not ignored, but yet have nothing to do - those should be added to internal notes as they are unaccounted for.
                If Nz(myPDQProcessFlags!IgnoreThisService, False) <> True And Nz(myPDQProcessFlags!SubDatabase, "") = "" And Left(myPDQProcessFlags!ProcessName, 4) <> "LF |" Then
                    Me.Notes = Me.Notes & myComponentDetail!name & vbCrLf
                    'MsgBox "hit"
                    'MsgBox "Added " & myComponentDetail!Name & " to internal Notes - Ignore service:" & myPDQProcessFlags!IgnoreThisService & " - Database:" & myPDQProcessFlags!SubDatabase
                Else
 
 


'If this is large format, add it to the table
                If Left(myPDQProcessFlags!ProcessName, 4) = "LF |" Then  'this is a large format item... add it to the list.
                    'make sure there is a matching record in ProjectComponentLFFinish
                    Dim myLFF_RS As Recordset
                    myLFF_SQL = "SELECT * from dbo_ProjectComponentLFFinish WHERE [ProjectComponentID] = " & Me.ID
                    Set myLFF_RS = CurrentDb.OpenRecordset(myLFF_SQL, dbOpenDynaset, dbSeeChanges)
