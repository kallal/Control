                                myWorkList!PrintRunNum = !PrintRunNum
                                
                                myWorkList!Status = 1 'Set Status to Not Started (1 =Not Started, 2 = In Process, 3 = Complete)
                               
                                myWorkList.Update
                            End With
                        End If ' end of ignore inside large format procedure
  
                Else 'Process other than Large format items


            
                    If Nz(myPDQProcessFlags!SubDatabase, "") <> "" Then
                        mysql = "SELECT * FROM " & myPDQProcessFlags!SubDatabase & " WHERE [ProjectComponentID] = " & Me.ProjectComponentID
                        If Left(myPDQProcessFlags!ProcessName, 4) = "PR |" And Nz(myComponentDetail!PrintRunNum, 999) <> 999 Then
                            'Add SectionName to the search line if it is a press run
                            mysql = mysql & " AND [SectionName] = '" & myComponentDetail!SectionName & "'"

                        End If 'is a print run.
                        
    
                        Set mySubTable = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
                    
                    
                    
                        If Not (mySubTable.EOF And mySubTable.BOF) Then
                            
                                mySubTable.MoveFirst
                                Do Until mySubTable.EOF
        
                                    mySubTable.Edit
                                    If Nz(myPDQProcessFlags!SubFieldName1, "") <> "" And Nz(myPDQProcessFlags!SubFieldData1, "") <> "" Then
                                        If mySubTable(myPDQProcessFlags!SubFieldName1) <> "Notes" Then
                                            mySubTable(myPDQProcessFlags!SubFieldName1) = myPDQProcessFlags!SubFieldData1
                                        Else
                                            mySubTable(myPDQProcessFlags!SubFieldName1) = mySubTable(myPDQProcessFlags!SubFieldName1) & myPDQProcessFlags!SubFieldData1 & vbCrLf
                                        End If
                                    End If
                                    
                                    If Nz(myPDQProcessFlags!SubFieldName2, "") <> "" And Nz(myPDQProcessFlags!SubFieldData2, "") <> "" Then
                                        If mySubTable(myPDQProcessFlags!SubFieldName2) <> "Notes" Then
                                            mySubTable(myPDQProcessFlags!SubFieldName2) = myPDQProcessFlags!SubFieldData2
                                        Else
                                            mySubTable(myPDQProcessFlags!SubFieldName2) = mySubTable(myPDQProcessFlags!SubFieldName2) & myPDQProcessFlags!SubFieldData2 & vbCrLf
                                        End If
                                    End If
                                    
                                    If Nz(myPDQProcessFlags!SubFieldName3, "") <> "" And Nz(myPDQProcessFlags!SubFieldData3, "") <> "" Then
                                        If mySubTable(myPDQProcessFlags!SubFieldName3) <> "Notes" Then
