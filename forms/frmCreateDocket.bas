Private Sub cmdConfirm_Click()
Dim myrs As Recordset

If Nz(Me.DocketArtworkExpectedDate, 0) = 0 Then
    MsgBox "You need to specify an expected artwork arrival date before opening the docket.", vbCritical, ProgramName
    Exit Sub
End If


'check for errors on form.
If Me.frmSendEmailConfirm = Null Then
    MsgBox "You need to select whether or not to send the email confirmation first.", vbCritical, ProgramName
    Exit Sub
End If

If Me.SpecifiedDocketDueDate < Me.AssignedDocketDueDate And Me.lblRushJob.visible = True And Me.IsRush = False Then
    MsgBox "You need to accept the Rush Job terms", vbCritical, ProgramName
    Exit Sub
End If

'Check if any of the components have multiple quantites, and then issue warning if they do

mysql = "SELECT * FROM dbo_projectComponentheader WHERE [ProjectHeaderID] = " & Me.ID

Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenSnapshot)

myMultipleQty = False

If myrs.RecordCount > 0 Then
    myrs.MoveFirst
    Do Until myrs.EOF
        If Nz(myrs!ExtQuoteQty2, 0) > 0 Then
            myMultipleQty = True
            
        End If
        myrs.MoveNext
    Loop
    myrs.Close
End If


If myMultipleQty = True Then
    myResponse = vbNo
    myResponse = MsgBox("NOTE: One or more of the components in this project have multiple quantity options (indicated above with a red !). Please confirm that you have selected the correct quantity." & vbCrLf & vbCrLf & "Is this order correct?", vbQuestion + vbYesNo, ProgramName)
    If myResponse = vbNo Then
        Exit Sub
    End If
End If

    
    




'check to see if they selected to send confirmation email.


'get next Docket number
Dim myRST As Recordset
Dim MYDB As Database
Set MYDB = CurrentDb()
Set myRST = MYDB.OpenRecordset("dbo_NextNumber", dbOpenDynaset, dbSeeChanges)
myDocketnum = myRST!NextDocketNum
myRST.Edit

myRST!NextDocketNum = myDocketnum + 1
myRST.Update
myRST.Close

Me.DocketNum = myDocketnum
Me.DocketDate = Date
Me.ProjectStatusID = 21


'Update components with their respective dates.

