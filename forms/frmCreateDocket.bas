Private Sub Form_Current()
'calculate the Estimated Date based on the info you have thus far.
'calculate Assigned Date if available.
Dim MYDB As Database
Dim myrs As Recordset
Dim myAssignedDate As Date
Dim myTotalPrintDays As Integer
Dim myTotalMailDays As Integer




Set MYDB = CurrentDb
Set myrs = MYDB.OpenRecordset("dbo_ProjectComponentHeader", dbOpenDynaset, dbSeeChanges)



myTotalPrintDays = 0
myTotalMailDays = 0

With myrs

.FindFirst ("[ProjectHeaderID] = " & Me.ID)
If .NoMatch Then Exit Sub
done = False

Do Until done = True
    tmpPrintDays = 1 + Nz(!DYStock, 0) + Nz(!DyCreative, 0) + Nz(!DYPrepress, 0) + Nz(!DYPrint, 0) + Nz(!DYOutsideSrv, 0) + Nz(!DYLFFinish, 0) + Nz(DYBindery, 0)
    
    If Hour(Time) >= 12 Then tmpPrintDays = tmpPrintDays + 1 'Add a day if the current time is after noon.
    
    tmpMailDays = Nz(!DYMailData, 0) + Nz(!DYMailShop, 0)
    
    
    If tmpPrintDays > myTotalPrintDays Then myTotalPrintDays = tmpPrintDays
    If tmpMailDays > myTotalMailDays Then myTotalMailDays = tmpMailDays

    .FindNext ("[ProjectHeaderID] = " & Me.ID)
    If .NoMatch Then done = True

Loop


End With
Me.myCalculatedMailDays = myTotalMailDays
Me.myCalculatedPrintDays = myTotalPrintDays

tmpPrintDueDate = fncWorkday(IIf(Me!DocketArtworkExpectedDate > Date, Me!DocketArtworkExpectedDate, Date), myTotalPrintDays)
tmpMailDueDate = fncWorkday(IIf(Me!DocketDataExpectedDate > Date, Me!DocketDataExpectedDate, Date), myTotalMailDays)


tmpDate1 = fncWorkday(IIf(Me!DocketDataExpectedDate > Date, Me!DocketDataExpectedDate, Date), myTotalMailDays)

If Nz(Me.DocketArtworkExpectedDate, 0) <> 0 Then

    Me.AssignedDocketDueDate = fncWorkday(IIf(Me!DocketArtworkExpectedDate > Date, Me!DocketArtworkExpectedDate, Date), myTotalPrintDays + myTotalMailDays)
End If

If tmpDate1 > Me.AssignedDocketDueDate Then Me.AssignedDocketDueDate = tmpDate1




If Nz(Me.SpecifiedDocketDueDate, 0) <> 0 Then


    If Me.SpecifiedDocketDueDate < Me.AssignedDocketDueDate Then
    
        Me.lblRushJob.visible = True
        Me.lblRushText.visible = True
        Me.IsRush.visible = True
        Me.lblRushIUnderstand.visible = True
        'MsgBox "Selecting this date will trigger a RUSH JOB.", vbCritical, ProgramName
    Else
        Me.lblRushJob.visible = False
        Me.lblRushText.visible = False
        Me.IsRush.visible = False
        Me.lblRushIUnderstand.visible = False
    End If
End If

If Me.FirmDueDate = True Then
    'Check to make sure there is a specified due date - if not, grab the assigned one.

    
    Me.SpecifiedDocketDueDate.FontBold = True
    Me.SpecifiedDocketDueDate.ForeColor = vbRed
    Me.SpecifiedDocketDueDate.BackColor = vbYellow
    
Else
    Me.SpecifiedDocketDueDate.FontBold = False
    Me.SpecifiedDocketDueDate.ForeColor = vbBlack
    Me.SpecifiedDocketDueDate.BackColor = vbWhite
    
End If

'Figure out if rush notations should be on based on calc date vs specified date.
If Me.Dirty Then Me.Dirty = False
End Sub



Private Sub SpecifiedDocketDueDate_AfterUpdate()


If Me.SpecifiedDocketDueDate < Me.AssignedDocketDueDate Then

    Me.lblRushJob.visible = True
    Me.lblRushText.visible = True
    Me.IsRush.visible = True
    Me.lblRushIUnderstand.visible = True
    MsgBox "Selecting this date will trigger a RUSH JOB.", vbCritical, ProgramName
    DoCmd.GoToControl "IsRush"
Else
    Me.lblRushJob.visible = False
    Me.lblRushText.visible = False
    Me.IsRush.visible = False
    Me.lblRushIUnderstand.visible = False
    
End If

End Sub

Private Sub SpecifiedDocketDueDate_BeforeUpdate(Cancel As Integer)
If Nz(Me.SpecifiedDocketDueDate, 0) <> 0 Then
    If Me.SpecifiedDocketDueDate < Date Then
        MsgBox "You can't specify a date before today...Unless, of course, you have a silver DeLorean and fuel for the Flux Capacitor.", vbCritical, "Hullo, McFly?"
        Me.Undo
        Cancel = True
    End If
    
    If Me.SpecifiedDocketDueDate < Nz(Me.DocketArtworkExpectedDate, 0) Then
        MsgBox "You can't specify a date before the artwork is set to arrive... Need to have something to work on...", vbCritical, ProgramName
        Me.Undo
        Cancel = True
    End If
    
    If Me.SpecifiedDocketDueDate < Nz(Me.DocketDataExpectedDate, 0) Then
        MsgBox "You can't specify a date before the Data is set to arrive... Kristine will hate you for that...", vbCritical, ProgramName
        Me.Undo
        Cancel = True
    End If
    
    
End If

End Sub

