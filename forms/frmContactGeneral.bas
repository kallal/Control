Private Sub cboSalesRepID_BeforeUpdate(Cancel As Integer)
'check if user has rights to enter new clients - if so, go ahead and allow it, if not, send back rejection message and cancel the change.

If DLookup("[CanAddContactGeneralYN]", "dbo_CompanyEmployee", "[ID] = " & cLoginID) = False Then
    MsgBox "You do not have rights to change the Sales Rep assigned to this client. Cancelling.", vbCritical, ProgramName
    Cancel = True
    Me.cboSalesRepID.Undo
Else
    Cancel = False
End If

End Sub