Private Sub DaysFromStart_AfterUpdate()

If Me.ExpectedDate <> "" Then
    tmpresponse = vbNo
    tmpresponse = MsgBox("You already have an Expected Date for this milestone... do you want to change it to a Days From Start milestone?", vbYesNo, ProgramName)
    If tmpresponse = vbYes Then
        Me.ExpectedDate = ""
        Me.ExpectedDateStatic = False
    Else
        Me.DaysFromStart = ""
    End If
    
End If

If Me.DaysFromStart <> "" Then
    DoCmd.GoToRecord , , acNext
    DoCmd.GoToControl "Type"
End If


End Sub

Private Sub ExpectedDate_AfterUpdate()

If Me.DaysFromStart <> "" Then
    tmpresponse = vbNo
    tmpresponse = MsgBox("You already have a Days From Start setting for this milestone... do you want to change it to an Expected Date milestone?", vbYesNo, ProgramName)
    If tmpresponse = vbYes Then
        Me.DaysFromStart = ""


    Else
        Me.DaysFromStart = ""
    End If
    
End If



End Sub

Private Sub ExpectedDateStatic_AfterUpdate()
If Nz(Me.ExpectedDate, "") = "" And Me.ExpectedDateStatic = True Then
    MsgBox "You cannot assign an Expected Date as Static if you have not yet assigned an Expected Date.", vbCritical, ProgramName
    Me.ExpectedDateStatic = False
End If

End Sub