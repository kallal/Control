Private Sub cmdEnd_Click()
If Nz(Me.StartTime, 0) = 0 Then
    MsgBox "This Task has not been started yet. You cannot end a task that hasn't started.", vbCritical, ProgramName
    Exit Sub
End If

If Nz(Me.EndTime, 0) <> 0 Then
    MsgBox "This Task has already been closed.", vbCritical, ProgramName
    Exit Sub
End If

myPrompt = "Ending task " & Me.Description & ". Please type any additional production notes regarding this task (i.e. machine problems, stock issues, etc.)"
myresp = ""
myresp = InputBox(myPrompt, ProgramName)


Me.EndTime = Format(Now, "short time")
Me.Notes = myresp
Call EndTime_AfterUpdate
If Me.Dirty Then Me.Dirty = False

End Sub





Private Sub cmdStart_Click()
If Nz(Me.StartTime, 0) <> 0 Then
    MsgBox "This Task has already been started. Please start a new line.", vbCritical, ProgramName
    Exit Sub
End If

myresp = ""
myresp = InputBox("Please enter the name of the task you are performing (i.e. Press run, Coil Bind, Insert, etc.)", ProgramName)

If myresp = "" Then Exit Sub

Me.StartTime = Format(Now, "short time")

Me.Description = myresp
If Me.Dirty Then Me.Dirty = False
    
End Sub

Private Sub EndTime_AfterUpdate()
If Me.Dirty Then Me.Dirty = False

If Nz(Me.StartTime, 0) = 0 Or Nz(Me.EndTime, 0) = 0 Then Exit Sub

If Me.EndTime < Me.StartTime Then
    MsgBox "You have entered incorrect times... the End time must be later than the Start time. Re-Enter the End Time.", vbCritical, ProgramName
    Me.EndTime = Null
    Exit Sub
End If

mytime = (Me.EndTime - Me.StartTime) * 24 ' result in fraction of day X 24 hrs



Me.TotalTime = mytime
If Me.Dirty Then Me.Dirty = False


End Sub



