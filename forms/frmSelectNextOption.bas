Private Sub Opt1_Click()
Dim MYDB As Database
Dim myRST As Recordset

Set MYDB = CurrentDb()
Set myRST = MYDB.OpenRecordset("dbo_NextStatusOptions", dbOpenDynaset, dbSeeChanges)

myRST.FindFirst "[ID] = " & Me.Opt1.tag

Select Case myRST!NextLocation

Case 1
    MyField = "StStock"
Case 2
    MyField = "StCreative"
Case 3
    MyField = "StPrepress"
Case 4
    MyField = "StMailData"
Case 5
    MyField = "StPrint"
Case 6
    MyField = "StOutsideSrv"
Case 7
    MyField = "stLFFinish"
Case 8
    MyField = "StBindery"
Case 9
    MyField = "StMailShop"

End Select



Forms!navmaster.Form!navSubform!NavProject(MyField) = myRST!NextStatus


DoCmd.Close


End Sub

