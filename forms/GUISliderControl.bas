Option Explicit

'       a Slide - The thing you drag with the mouse. I used a command button to allow for
'                 more style variations.
'       a Track - What the slide travels on. It's a rectangle object. I chose this over a
'                 line for flexibility. Changing its height changes the track thickness.
'       Tick Marks - Visually subdivide the track. These are just lines.
'
' Properties:
'    (r) indicates read, (w) indicates write
'
'    Unless otherwise noted all values are in twips (1 twip = 1/1440 inch = 1/566.93 cm).
'
'    SlideHeight (w, Integer) - Sets height of slide.
'    SlideWidth (w, Integer) - Sets width of slide.
'    TrackHeight (w, Integer) - Sets height (thickness) of track.
'    TrackLength (w, Integer) - Sets the length of the track.
'    TickMarkThickness (w, Integer) - Sets thickness of tick marks, units are points.
'    MajorTickMarkLength (w, Integer) - Length of tick marks that occur at 0, 50 and 100%.
'    MinorTickMarkLength (w, Integer) - Length of remaining tick marks.

'    MaxVal (w, Double) - Maximum value for  slider control. Default is 100.
'    MinVal (w, Double) - Minimum value for  slider control. Default is 0.

'    SliderValue (r/w, Double) - Reads or sets the value of the slider control. Note this should be
'                         set after the geometry functions above or the control may not size properly.
'    UpdateOnMove (w, Boolean) - If true, the value of the slider control is updated as the slide is
'                        moved (on the MouseMove event). If false, the value is only updated on the
'                        slide's MouseUp event.
'    ArrowKeyMovePerCent (w, Double) - Percent of slider track that slider moves when pressing
'                                      the left arrow or right arrow key. Default is 1%.
'
'--------------------------------------------------------------------------------------------------

Private mdblAbsPosX        As Double
Private mblnUpdateOnMove   As Boolean
Private mdblMaxVal         As Double
Private mdblMinVal         As Double
Private mdblRange          As Double
Private mdblKeyMovePerCent As Double
Private mblnActive         As Boolean

Public bolIntOnly          As Boolean  ' set true the only int values
Public intRoundTo          As Integer  ' set round to value - only works with bolIntOnly

Private Declare PtrSafe Sub WaitABit Lib "kernel32" Alias "Sleep" (ByVal dwMilliseconds As Long)

Public Property Let SlideHeight(ByVal intSlideHeight As Integer)
    'Set the height slide. Value is in twips.
    Me.Slide.Height = intSlideHeight
    RebuildVertical
End Property

Public Property Let SlideWidth(ByVal intSlideWidth As Integer)
    'Set the Width slide. Value is in twips.
    Me.Slide.Width = intSlideWidth
    RebuildHorizontal
End Property

Public Property Let TrackHeight(ByVal intTrackHeight As Integer)
    'Set the length of the slider track. Value is in twips.
    Me.Track.Height = intTrackHeight
    RebuildVertical
End Property

Public Property Let TrackLength(ByVal intTrackWidth As Integer)
    'Set the length of the slider track. Value is in twips.
    Me.Track.Width = intTrackWidth
    RebuildHorizontal
End Property

Public Property Let TickMarkThickness(ByVal bytPoints As Byte)
'Sets thickness of tick marks. Valid values are 0 to 6. 0 is hairline,
'numbers 1 to 6 are thickness in points.
Dim ctrl As control
If bytPoints >= 0 And bytPoints <= 6 Then
    For Each ctrl In Me.Controls
        With ctrl
            If TypeName(ctrl) = "Line" Then
                ctrl.BorderWidth = bytPoints
            End If
        End With
    Next
End If
Set ctrl = Nothing
End Property

Public Property Let MajorTickMarkLength(ByVal intLength As Integer)
'Sets length of major tick marks. Major tick marks occur at 0, 50 and 100%. Units are twips.
Dim ctrl As control
If intLength >= 0 Then
    For Each ctrl In Me.Controls
        With ctrl
            If TypeName(ctrl) = "Line" And .name Like "MajorTick*" Then
                .Height = intLength
            End If
        End With
    Next
End If
RebuildVertical
Set ctrl = Nothing
End Property

Public Property Let MinorTickMarkLength(ByVal intLength As Integer)
'Sets length of minor tick marks. Units are twips.
Dim ctrl As control
If intLength >= 0 Then
    For Each ctrl In Me.Controls
        With ctrl
            If TypeName(ctrl) = "Line" And .name Like "Tick*" Then
                .Height = intLength
            End If
        End With
    Next
End If
RebuildVertical
Set ctrl = Nothing
End Property

Public Property Let MaxVal(ByVal dblMax As Double)
    mdblMaxVal = dblMax
    mdblRange = mdblMaxVal - mdblMinVal
End Property

Public Property Let MinVal(ByVal dblMin As Double)
    mdblMinVal = dblMin
    mdblRange = mdblMaxVal - mdblMinVal
End Property

Public Property Get SliderValue() As Double
   
    SliderValue = mdblMinVal + dblTrackPosition * mdblRange
    
   If bolIntOnly Then
      SliderValue = Int(SliderValue)
      SliderValue = Fix(SliderValue - (SliderValue Mod 5))
   End If

End Property

Public Property Let SliderValue(ByVal dblSliderValue As Double)
'Set the value and position of the slide. SliderValue is a Double that can range from mdblMinVal
'to mdblMaxVal, inclusive.
On Error GoTo Proc_Err


If dblSliderValue >= mdblMinVal And dblSliderValue <= mdblMaxVal And mdblMinVal < mdblMaxVal Then
    With Me
        Slide.Move CInt((dblTrackLength * (dblSliderValue - mdblMinVal)) / (mdblRange) _
                         + CDbl(Track.Left) - dblSlideOffset)
    End With
End If
'Calls UpdateValues function on parent form. UpdateValues then reads the values of the slider
'controls. Somewhat contorted, but necesary because Access doesn't recognize event on the subform.
If mblnActive Then Me.Parent.UpdateValues
Proc_Exit:
    Exit Property
Proc_Err:
    'Ignore reference error - occurs when frmSliderControl opened directly.
    If Err.Number = 2455 Or Err.Number = 2452 Or Err.Number = 2467 Then
        Resume Proc_Exit
    Else
        MsgBox "Error " & Err.Number & " " & Err.Description
        Resume Proc_Exit
    End If

End Property

Public Property Let UpdateOnMove(ByVal bln As Boolean)
    'If true, the value of the slider control is updated as the slide is moved.
    mblnUpdateOnMove = bln
End Property

Public Property Let ArrowKeyMovePerCent(ByVal dblIncrement As Double)
'Sets amount slide will move when the left or right arrow key is pressed.
    mdblKeyMovePerCent = dblIncrement
End Property

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
'Allows use of Left and Right Arrow keys to move slider.

   Dim dblNewVal        As Double
   Dim dblIncrement     As Double
   Dim bolTouch         As Boolean
   
   'dblIncrement = mdblRange * mdblKeyMovePerCent / 100
   dblIncrement = mdblKeyMovePerCent       ' this is not % value anymore
   Debug.Print dblIncrement
   
   With Me
       If KeyCode = vbKeyLeft Then
           dblNewVal = .SliderValue - dblIncrement
           bolTouch = True
       ElseIf KeyCode = vbKeyRight Then
           dblNewVal = .SliderValue + dblIncrement
           bolTouch = True
       End If
       If (dblNewVal >= mdblMinVal) And (dblNewVal <= mdblMaxVal) And (bolTouch = True) Then
           .SliderValue = dblNewVal
       ElseIf dblNewVal < mdblMinVal Then
            .SliderValue = mdblMinVal
       ElseIf dblNewVal > mdblMaxVal Then
            .SliderValue = mdblMaxVal
       End If
   End With
End Sub

Private Sub Form_Open(Cancel As Integer)
    'Default values:
    mdblMinVal = 0
    mdblMaxVal = 100
    mdblRange = mdblMaxVal - mdblMinVal
    mdblKeyMovePerCent = 1
    'Rebuild geometry:
    Me.SliderValue = 0 'Needed so form resizes properly.
    RebuildHorizontal
    RebuildVertical
    mblnActive = True
End Sub

Private Sub RebuildHorizontal()
'Rebuilds slider geometry in horizontal (x) direction.
Const cintLeftSidePadding As Integer = 30 'Space from left side of form to left edge of slide
                                          'when slide is at 0%, twips.
Dim ctrl As control
Dim strMultiplier As String
'Position track:
Me.Track.Left = CInt(dblSlideOffset) + cintLeftSidePadding
'Position tick marks:
For Each ctrl In Me.Controls
    With ctrl
        If TypeName(ctrl) = "Line" Then
            strMultiplier = .name
            Do Until IsNumeric(strMultiplier)
                strMultiplier = Mid$(strMultiplier, 2)
            Loop
            .Left = Me.Track.Left + (CInt(strMultiplier) * Me.Track.Width) / 10
        End If
    End With
Next
'Size form width:
With Me
    .Width = 2 * .Track.Left + .Track.Width
End With
Set ctrl = Nothing
End Sub

Private Sub RebuildVertical()
'Rebuilds slider geometry in vertical (y) direction.

Dim ctrl As control
Const cintTopPadding As Integer = 50 'Space from top of slide to top of form, twips.
Const intTopPadding As Integer = 30 'Space between top of tick marks and bottom of Slider.
Const cintBottomPadding As Integer = 30 'Space between bottom of tick marks and end of form.

'Position track and slide:
With Me
    .Track.Top = (Slide.Height - Track.Height) / 2 + cintTopPadding
    .Slide.Top = Track.Top + (Track.Height - Slide.Height) / 2 'Center slide verticaly.
End With
'Position tick marks:
For Each ctrl In Me.Controls
    With ctrl
        If TypeName(ctrl) = "Line" Then
            .Top = Me.Track.Top + Me.Track.Height + Me.Slide.Height / 2 + intTopPadding
        End If
    End With
Next
'Size form height:
With Me
    .Detail.Height = .MajorTick0.Top + .MajorTick0.Height + cintBottomPadding
End With

Set ctrl = Nothing
End Sub

Private Sub Slide_MouseDown(Button As Integer, Shift As Integer, x As Single, Y As Single)
Dim mdblAbsPosX As Double
    If Button = acLeftButton Then
        'Center slide under cursor by moving the slide under the cursor.
        With Me.Slide
            mdblAbsPosX = CDbl(.Left) + CDbl(x) - dblSlideOffset
            'Conditions prevent slide from moving off either end of track.
            If mdblAbsPosX <= dblHighLimit And mdblAbsPosX >= dblLowLimit Then
                .Move (mdblAbsPosX)
            ElseIf mdblAbsPosX > dblHighLimit Then
                .Move (dblHighLimit)
            ElseIf mdblAbsPosX < dblLowLimit Then
                .Move (dblLowLimit)
            End If
        End With
    End If
End Sub

Private Sub Slide_MouseMove(Button As Integer, Shift As Integer, x As Single, Y As Single)
On Error GoTo Proc_Err
If Button = acLeftButton Then
    With Slide
        mdblAbsPosX = CDbl(.Left) + CDbl(x) - dblSlideOffset 'twips - reference is center of Slider.
        'Conditions prevent slide from moving off either end of track.
        If mdblAbsPosX <= dblHighLimit And mdblAbsPosX >= dblLowLimit Then
            .Left = (mdblAbsPosX)
        ElseIf mdblAbsPosX > dblHighLimit Then
            .Left = dblHighLimit
        ElseIf mdblAbsPosX < dblLowLimit Then
            .Left = dblLowLimit
        End If
        DoEvents 'Tracks cursor more closely.
        WaitABit (100) 'Improves flickering a bit. Parameter is pause time in msec.
    End With
    If mblnUpdateOnMove Then
        Me.Parent.UpdateValues
    End If
End If
Proc_Exit:
    Exit Sub
Proc_Err:
    'Ignore reference error - occurs when frmSliderControl opened directly.
    If Err.Number = 2455 Or Err.Number = 2452 Or Err.Number = 2467 Then
        Resume Proc_Exit
    Else
        MsgBox "Error " & Err.Number & " " & Err.Description
        Resume Proc_Exit
    End If
End Sub

Private Sub Slide_MouseUp(Button As Integer, Shift As Integer, x As Single, Y As Single)
On Error GoTo Proc_Err
    If Not mblnUpdateOnMove Then
        Me.Parent.UpdateValues
    End If
Proc_Exit:
    Exit Sub
Proc_Err:
    If Err.Number = 2452 Then
        Resume Proc_Exit
    Else
        MsgBox "Error " & Err.Number & " " & Err.Description
        Resume Proc_Exit
    End If
End Sub

Private Function dblHighLimit() As Double
'Reference is to center of Slider, which corresponds to the ends of the Track.
    dblHighLimit = Me.Track.Left + Me.Track.Width - dblSlideOffset
End Function

Private Function dblLowLimit() As Double
'Reference is to center of Slider, which corresponds to the ends of the Track.
    dblLowLimit = Me.Track.Left - dblSlideOffset
End Function

Private Function dblSlideOffset() As Double
    dblSlideOffset = Me.Slide.Width / 2 'Distance to center of Slider, twips.
End Function

Private Function dblTrackLength() As Double
'Reference is to center of Slider, which corresponds to the ends of the Track.
    dblTrackLength = Me.Track.Width
End Function

Private Function dblTrackPosition() As Double
    dblTrackPosition = (CDbl(Slide.Left) + dblSlideOffset - CDbl(Track.Left)) / dblTrackLength
End Function