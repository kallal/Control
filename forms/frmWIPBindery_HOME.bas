Private Sub cmdFilter_Click()


myfilter = ""

'MsgBox "Show only: " & Me.chkShowOnlyReady

If Nz(Me.frmShowOnly, 0) <> 0 Then
    'set the filter by the bindery work to be done.
    Select Case Me.frmShowOnly
    
    Case 1 'Trim
        myfilter = myfilter & " WHERE [IsTrim] = True"
    
    Case 2 'Pad
        myfilter = myfilter & " WHERE [PadInYN] = true"
    
    Case 3 'Drill
        myfilter = myfilter & " WHERE [DrillYN] = true"
    
    Case 4 'Score
        myfilter = myfilter & " WHERE [IsScore] = true"
        
    Case 5 'Fold
        myfilter = myfilter & " WHERE [FoldYN] = true"
        
    Case 6 'Saddle Stitch
        myfilter = myfilter & " WHERE [BindSaddleYN] = true"
        
    Case 7 'Perfect Bind
        myfilter = myfilter & " WHERE [BindPerfectYN] = true"
        
    Case 8 'Coil Bind
        myfilter = myfilter & " WHERE [BindCoilYN] = true"
        
    Case 9 'Wire-O
        myfilter = myfilter & " WHERE [BindWireOYN] = true"
  
  End Select
End If



mysql = "SELECT * FROM qryScheduleBindery " & myfilter


Me.rptWIPBindery.Report.RecordSource = mysql



Application.Echo True
End Sub

