Private Sub SpecifiedDocketDueDate_AfterUpdate()

Dim MYDB As Database
Dim myrs As Recordset
Set MYDB = CurrentDb
Set myrs = MYDB.OpenRecordset("qryProjectComponentHeader", dbOpenDynaset, dbSeeChanges)

With myrs
.MoveFirst
mycriteria = "[ProjectHeaderID] = " & Me.ID 'Update all components to the new Specified Date.

.FindFirst mycriteria

If .NoMatch <> True Then
    done = False
    Do Until done = True
    .Edit
    !SpecifiedDueDate = Me.SpecifiedDocketDueDate
    .Update
    .FindNext mycriteria
    If .NoMatch Then done = True
    Loop
End If

End With

If Me.SpecifiedDocketDueDate < Me.AssignedDocketDueDate Then
    Me.lblRush.visible = True
    Me.IsRush = True
Else
    Me.lblRush.visible = False
    Me.IsRush = False
End If

AddContactHistory Nz(ContactGeneralID), Nz(BillOrderedBy), Nz(ContactNameID), ID, 1, rsLocSS!FN & " " & rsLocSS!LN & " changed the delivery date to " & Me.SpecifiedDocketDueDate

If Me.Dirty Then Me.Dirty = False
Me.Refresh

End Sub

Private Sub SpecifiedDocketDueDate_BeforeUpdate(Cancel As Integer)

If Me.SpecifiedDocketDueDate < Date Then
    MsgBox "You cannot specify a delivery date before today. Unless, of course, you have a silver DeLorean and fuel for the Flux Capacitor.", vbCritical, "Hullo? McFly?"
    Me.Undo
    Cancel = True
    Exit Sub
End If

myPrompt = "You have chosen a new Specified (NO FAIL) Delivery Date of " & Format(Me.SpecifiedDocketDueDate, "Medium Date") & ". Are you sure?"
myResponse = vbNo
myResponse = MsgBox(myPrompt, vbQuestion + vbYesNo + vbDefaultButton2, ProgramName)
If myResponse = vbNo Then
    Me.Undo
    Cancel = True
Else
    Cancel = False
End If
End Sub

Private Sub togWatchList_Click()
constOrange = 967423
constGrey = 15527148


    Dim MYDB As Database
    Dim myRST As Recordset
    
    Set MYDB = CurrentDb()
    Set myRST = MYDB.OpenRecordset("dbo_ProjectHeaderWatchList", dbOpenDynaset, dbSeeChanges)
    
If Me.togWatchList.BackColor = constGrey Then
    'get reason to watch
    a = ""
    a = InputBox("Reason to watch this project?", ProgramName)
       
    'add record to watch list

    myRST.AddNew
    myRST!ProjectHeaderID = Me.ID
    myRST!EmployeeID = cLoginID
    myRST!WatchNotes = a
    myRST.Update
    Me.togWatchList.BackColor = constOrange
    
    
Else
    'confirm removal from watch list
    a = vbNo
    a = MsgBox("Are you sure you want to remove this project from your Watch List?", vbQuestion + vbYesNo + vbDefaultButton2, ProgramName)
    If a = vbYes Then
     
    'delete record from watch list

        myRST.MoveFirst
        Do Until myRST.EOF
               
        If myRST!ProjectHeaderID = Me.ID And myRST!EmployeeID = cLoginID Then
        
            myRST.Delete
            Me.togWatchList.BackColor = constGrey
            
        End If
        myRST.MoveNext
        Loop
        
     End If
     
        
End If


End Sub



