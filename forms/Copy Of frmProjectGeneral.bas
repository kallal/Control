Me.AllowEdits = False


myResponse = vbNo

If Me.ProjectStatusID > 10 Then
    myResponse = MsgBox("This quote has already been sent. Send Again?", vbYesNo, ProgramName)
    If myResponse = vbNo Then
        If Me.Dirty Then Me.Dirty = False
        Exit Sub
    End If
End If



myResponse = ""
myResponse = InputBox("Ready to send this estimate (" & Me.QuoteNum & ")... If you want to add additional Quotes to this email, please enter the additional Estimate Numbers (separated by commas) below.", ProgramName)

myMatrix(1, 1) = Me.ID
myMatrix(1, 2) = Me.QuoteNum

Me.Requery

Me.ProjectStatusID = 11 ' Change status to live
Me.EstimateSent = Now
Me.EstimateSentByID = cLoginID
Me.LastContact = Now
Me.LastContactByID = cLoginID
Me.LastContactMethod = "Email"

i = 1
If Len(Nz(myResponse, "")) > 0 Then

    
    done = False
    
    Do Until done = True  'parse the response
    
    i = i + 1
    mymarker = InStr(myResponse, ",")

    
    If mymarker = 0 Then
        mymarker = Len(myResponse)
        done = True
    Else
        mymarker = mymarker - 1
    End If
    
    On Error Resume Next
    myMatrix(i, 1) = DLookup("[ID]", "dbo_projectHeader", "[QuoteNum] = " & Nz(Val(Left(myResponse, mymarker)), 0))
    If myMatrix(i, 1) = 0 Then
        MsgBox "You have entered one or more invalid quote numbers... Please retry.", vbCritical, ProgramName
        If Me.Dirty Then Me.Dirty = False
        Exit Sub
    End If
    myMatrix(i, 2) = Val(Left(myResponse, mymarker))
    
    If mymarker + 1 < Len(myResponse) Then
        myResponse = Right(myResponse, Len(myResponse) - mymarker - 1)
    End If
    
    Loop
End If



    
    


