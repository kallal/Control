If Me.Dirty Then Me.Dirty = False
'Check for PO Required first

myPORequired = DLookup("[PORequired]", "dbo_ContactGeneral", "[id] = " & Me.ContactGeneralID)

If myPORequired = True And Nz(Me.PONumber, 0) = 0 Then
    MsgBox "This client requires a PO number on all Dockets. Please Enter a Purchase Order number and retry.", vbCritical, ProgramName
    Exit Sub
End If


DoCmd.OpenForm "frmCreateDocket", acNormal, , "[ID] = " & Me.ID, acFormEdit, acDialog

'MsgBox "At this point, validate all data to be sure that complete information is provided. Also check if client is allowed to order and/or ship (warn only if ship issue)"
'Figure out the forecasted due date and the forecasted proof ok dates, if we have artwork.



'If Me.Dirty Then Me.Dirty = False
Me.Refresh        ' disk write + refresh
Parent.Refresh
Call Form_Current

End Sub

