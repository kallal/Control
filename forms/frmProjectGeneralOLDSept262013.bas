Private Sub cmdReadyToSend_Click()


Dim myRecipient As String
Dim mySubject As String
Dim MyMessage As String


Me.Refresh
myResponse = vbNo
myResponse = MsgBox("Mark this Estimate as Ready to Send. Are you sure?", vbQuestion + vbYesNo + vbDefaultButton2, ProgramName)
If myResponse = vbYes Then
    'Check how is this estimate to be handled. Do I send directly?
    If Me.EstimateTo = 1 Then 'Send email
        Call cmdEmailEstimate_Click
        Me.Requery
        
       
        Me.ProjectStatusID = 11
        Me.EstimateSent = Now
        Me.EstimateSentByID = cLoginID
        Me.LastContact = Now
        Me.LastContactByID = cLoginID
        Me.LastContactMethod = "Email"

    Else
        'Otherwise, mark as ready to send.
        'Send email to Sales/CSR? check with them.
        myMsgbox = "Estimate is Ready for Sales/CSR to send to client, and an automated email has been sent to the "
        Me.ProjectStatusID = 3
        
        If Me.EstimateTo = 2 Then
            myRecipient = Me.CsrID.Column(5)
            myMsgbox = myMsgbox & "Inside Salesperson (CSR)."
        Else
            myRecipient = Me.SalesRepID.Column(5)
            myMsgbox = myMsgbox & "Sales Representative."
        End If
        
        MsgBox myMsgbox, vbInformation, ProgramName
        
        mySubject = ""
        If Nz(Me.QuoteNum) > 0 Then mySubject = "E" & Me.QuoteNum
        If Nz(Me.DocketNum) > 0 Then mySubject = "D" & Me.DocketNum
        If Nz(Me.InvoiceNum) > 0 Then mySubject = "I" & Me.InvoiceNum
        mySubject = mySubject & " : " & Me.ContactGeneralID.Column(1) & " : " & Me.ProjectName
        MyMessage = "Estimate is ready to send to client. Please go to the 'Ready to Send' list and forward the Estimate to your client."
        
        SendMailCDO myRecipient, mySubject, MyMessage
        
    End If
    
    
    Me.cmdReadyToSend.enabled = False
    Me.cmdReadyToSend.visible = False
End If
If Me.Dirty Then Me.Dirty = False

