End Sub

Private Sub cmdCall_Click()
mySubject = InputBox("Calling " & Me.BillOrderedBy.Column(1) & " at " & Format(Me.BillOrderedBy.Column(4), "(000) 000-0000") & vbCrLf & "What is the nature of this call?", ProgramName)
If mySubject = "" Then
    MsgBox "No subject, call cancelled.", vbCritical, ProgramName
    Exit Sub
End If

fncMakePhoneCall Format(Me.BillOrderedBy.Column(4), "(000) 000-0000")

AddContactHistory Nz(Me.ContactGeneralID), Nz(Me.BillAddLocationID), Me.BillOrderedBy, Nz(Me.ID), 3, "Phone (Direct) Call - Subject: " & mySubject
Me.LastContact = Now
Me.LastContactByID = cLoginID
Me.LastContactMethod = "Phone"
If Me.Dirty Then Me.Dirty = False
End Sub

