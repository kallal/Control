Private Sub ShipListFlag_AfterUpdate()
If Me.ShipListFlag = True And Me.Waybill <> "" Then
    myResponse = vbNo
    myResponse = MsgBox("Warning... Tagging this Packing List will overwrite the waybill information previously stored. Do you wish to continue?", vbYesNo + vbExclamation, ProgramName)
    If myResponse = vbNo Then
        Me.ShipListFlag = False
    End If
End If

End Sub

Private Sub txtID_Click()

myReportName = "frmPackingList"
DoCmd.OpenForm myReportName, acNormal, , "[id] = " & Me.ID, acFormEdit, acDialog
End Sub

Private Sub Waybill_Click()
If Nz(Me.ShipmentHeaderID, 0) = 0 Then Exit Sub

myReportName = "frmShipmentHeader"

DoCmd.OpenForm myReportName, acNormal, , "[id] = " & Me.ShipmentHeaderID, acFormEdit, acDialog

End Sub