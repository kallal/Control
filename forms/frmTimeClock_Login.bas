Public Sub txtPIN_AfterUpdate()


'We've got a user and a PIN that's verified - do the work now.
Dim myEmployeeID As Long
Dim mysql As String
Dim myUsername As String
Dim myrs As Recordset
Dim MYRS_CS As Recordset
Dim myEmployeeName As String

Dim myWeekday As Integer
Dim myStartFieldname As String
Dim myEndFieldname As String


myUsername = Me.txtUsername

If Left(myUsername, 3) = "emp" Then
    myUsername = Right(myUsername, Len(myUsername) - 3)
End If

myEmployeeID = Val(myUsername)

mysql = "SELECT * from dbo_CompanyEmployee WHERE ID = " & myEmployeeID
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)


myEmployeeName = myrs![LN] & ", " & myrs![FN]
Me.txtEmployeeName = myEmployeeName


'Get Current Status - will dictate which buttons are available.
mysql = "SELECT * from dbo_CompanyEmployeeCurrentStatus WHERE CompanyEmployeeID = " & myEmployeeID
Set MYRS_CS = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)

If MYRS_CS.EOF Then 'No current record found
    Me.cmdLogin.enabled = True
    Me.cmdLogout.enabled = False
    Me.txtCurrentStatus = ""
Else
    Me.txtCurrentStatus = MYRS_CS!CurrentStatus
    Me.cmdLogin.enabled = False
    Me.cmdLogout.enabled = True
    If MYRS_CS!CurrentStatus = "ON BREAK" Then
        Me.cmdStartBreak.enabled = False
        Me.cmdEndBreak.enabled = True
        Me.cmdLogout.enabled = False
    Else
        Me.cmdStartBreak.enabled = True
        Me.cmdEndBreak.enabled = False
    End If
End If

'Grab the current shift expectation and show it on screen
mysql = "SELECT * from dbo_CompanyEmployeeShiftManagement WHERE CompanyEmployeeID = " & myEmployeeID
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenSnapshot)
'Get what is today's day of the week
myWeekday = Weekday(Date)
myStartFieldname = "Start_" & Trim(str(myWeekday))
myEndFieldname = "End_" & Trim(str(myWeekday))

Me.txtExpectedShift = myrs(myStartFieldname) & " to " & myrs(myEndFieldname)

Set myrs = Nothing 'cleanup
Set MYRS_CS = Nothing

Me.cmdCancel.SetFocus


End Sub

