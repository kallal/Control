Private Sub txtUsername_AfterUpdate()

Dim myEmployeeID As Integer
Dim mysql As String
Dim myrs As Recordset
Dim myUsername As String
Dim myPin As String
Dim mydash As Integer
Dim myEmployeeName As String

myUsername = Me.txtUsername
myPin = ""
If Left(myUsername, 3) = "emp" Then
    myUsername = Right(myUsername, Len(myUsername) - 3)
End If

mydash = InStr(myUsername, "-")

If mydash > 0 Then
    myPin = Right(myUsername, Len(myUsername) - mydash)
    myUsername = Left(myUsername, mydash - 1)

End If
Me.txtUsername = myUsername
myEmployeeID = Val(myUsername)

mysql = "SELECT * from dbo_CompanyEmployee WHERE ID = " & myEmployeeID
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
If myrs.EOF = False Then

    myEmployeeName = myrs![LN] & ", " & myrs![FN]
    Me.txtEmployeeName = myEmployeeName
End If

If Nz(myPin, "") <> "" Then
    Me.txtPIN = myPin
    Call Me.txtPIN_AfterUpdate
End If


'What to do AFTER data is known to be good.


End Sub

Private Sub txtUsername_BeforeUpdate(Cancel As Integer)
Dim myEmployeeID As Long
Dim mysql As String
Dim myUsername As String
Dim myrs As Recordset

Cancel = False

Me.txtPIN = ""

Me.txtPIN.enabled = True

If Nz(Me.txtUsername, "") <> "" Then

    'Find the match
    myUsername = Me.txtUsername
    
    If Left(myUsername, 3) = "emp" Then
        myUsername = Right(myUsername, Len(myUsername) - 3)
    End If
    
    myEmployeeID = Val(myUsername)
    If Nz(myEmployeeID, 0) = 0 Then
        MsgBox "No employee number found. Employee barcodes begin with EMP and are followed by a four digit number.", vbCritical, ProgramName
        Cancel = True
        Me.txtPIN.enabled = False
        Me.txtUsername.Undo
        Exit Sub
    End If
    
    mysql = "SELECT * from dbo_CompanyEmployee WHERE ID = " & myEmployeeID
    Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
    If myrs.EOF Then
        MsgBox "Invalid, or No match found for this Employee ID.", vbCritical, ProgramName
        Cancel = True
        Me.txtPIN.enabled = False
        Me.txtUsername.Undo

    Else
        If myrs!IsActive = False Then
            MsgBox "This user is not an active production user. Please have your manager check the Active status of this employee.", vbCritical, ProgramName
            Cancel = True
            Me.txtUsername.Undo
            Me.txtPIN.enabled = False
        End If
    End If
    


End If
Set myrs = Nothing

End Sub