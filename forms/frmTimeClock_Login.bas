    myEmployeeID = Val(myUsername)

mysql = "SELECT * from dbo_CompanyEmployee WHERE ID = " & myEmployeeID
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)

If Me.txtPIN <> Nz(myrs!PIN, 0) Then
