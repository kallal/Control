Private Sub cmdChangeMasterCompany_Click()
Dim myrsLocation As Recordset
Dim myrsName As Recordset
Dim myrsProject As Recordset

myOldCompanyID = Me.ContactGeneralID
myNewContactID = ""
myNewContactID = InputBox("Which Company ID should I move these records to?: ")
If myNewContactID = "" Then
    MsgBox "Cancelling change - exiting."
    Exit Sub
End If

mysql = "SELECT * FROM dbo_ContactLocation WHERE [ContactGeneralID] = " & Me.ContactGeneralID
Set myrsLocation = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)


mysql = "SELECT * FROM dbo_ContactName WHERE [ContactGeneralID] = " & Me.ContactGeneralID
Set myrsName = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)

mysql = "SELECT * FROM dbo_ProjectHeader WHERE [ContactGeneralID] = " & Me.ContactGeneralID
Set myrsProject = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)


myresult = vbNo
myresult = MsgBox("About to change " & myrsLocation.RecordCount & " location records, " & myrsName.RecordCount & "Name Records and " & myrsProject.RecordCount & "Projects. Continue?", vbQuestion + vbYesNo, ProgramName)
If myresult = vbNo Then
    MsgBox "Cancelling at Location change - exiting."
    Exit Sub
End If

Do Until myrsLocation.EOF
    myrsLocation.Edit
    myrsLocation!ContactGeneralID = myNewContactID
    myrsLocation.Update
    myrsLocation.MoveNext
Loop

    
Do Until myrsName.EOF
    myrsName.Edit
    myrsName!ContactGeneralID = myNewContactID
    myrsName.Update
    myrsName.MoveNext
Loop
    
Do Until myrsName.EOF
    myrsName.Edit
    myrsName!ContactGeneralID = myNewContactID
    myrsName.Update
    myrsName.MoveNext
Loop

Do Until myrsProject.EOF
    myrsProject.Edit
    myrsProject!ContactGeneralID = myNewContactID
    myrsProject.Update
    myrsProject.MoveNext
Loop

MsgBox "Changes made."


End Sub

