  End If
  If Me.Dirty Then Me.Dirty = False
  
  If cControlCountry = "US" Or cControlCountry = "USA" Then
        Me.lblProv.caption = "State: "
            Else
        Me.lblProv.caption = "Prov: "
  End If
    
    Me.Prov.DefaultValue = "'" & cControlProv & "'"
    Me.City.DefaultValue = "'" & cControlCity & "'"
    
End Sub

