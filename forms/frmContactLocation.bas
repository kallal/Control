Private Sub Form_Current()
'If you move to a new record, make sure it's locked first.

Dim ctl As control
    Me.cmdEdit.caption = "LOCKED"
    Me.cmdEdit.BackColor = vbRed
    For Each ctl In Me.Controls
        If ctl.tag = "L" Then
            ctl.enabled = False
            ctl.Locked = True

        End If
    Next

' Provide a record counter for using with
' custom navigation buttons (when not using
' Access built in navigation)

On Error Resume Next
    Dim rst As DAO.Recordset
    Dim lngCount As Long

    Set rst = Me.RecordsetClone
    
    With rst
        .MoveFirst
        If Me.ID = rst!ID Then
            cmdPrev.enabled = False
        Else
            cmdPrev.enabled = True
        End If
        
        .MoveLast
        
        If Me.ID = rst!ID Then
            cmdNext.enabled = False
        Else
            cmdNext.enabled = True
        End If
        
        lngCount = .RecordCount
    End With
    
'Show the result of the record count in the text box (txtRecordNo)
    If Me.CurrentRecord > lngCount Then
    Me.txtRecordNo = "NO RECORD"
Else

    Me.txtRecordNo = "Location " & Me.CurrentRecord & " of " & lngCount
End If

If Me.Country <> "Canada" Then
    Me.PC.InputMask = Null
    
End If

If Me.Dirty Then Me.Dirty = False
End Sub

