Private Sub cmdKomoriSummaryReport_Click()
'Procedure to show breakdown of this entire project

'Copy pertinent data from dbo_T_StatusRecord to loc_T_StatusRecord, and add in the Interval time as we go.

Dim MYDB As Database
Dim myInputTbl As Recordset
Dim myOutputTbl As Recordset
Set MYDB = CurrentDb()




sqlcmd = "Delete * FROM loc_T_Statusrecord" 'Clean out temporary table
CurrentDb.Execute sqlcmd, dbSeeChanges


Dim myRecordset As Recordset
Set myRecordset = Me.Recordset.Clone

myRecordset.MoveFirst

Do Until myRecordset.EOF
'Start loop to collect all viewed records
    
    
    sqlcmd = "SELECT * FROM [dbo_T_Statusrecord] WHERE RecordDate BETWEEN " & quDate(myRecordset!StartDate) & " AND " & quDate(myRecordset!FinishDate)
    
    'This seems to be the only way to find matching records - by date/time
    
    MYDB.QueryDefs("qry_T_Statusrecord").sql = sqlcmd
    
    
    
        Set myInputTbl = MYDB.OpenRecordset("qry_T_Statusrecord", dbOpenDynaset, dbSeeChanges)
        Set myOutputTbl = MYDB.OpenRecordset("loc_T_Statusrecord", dbOpenDynaset, dbSeeChanges)
        
        myInputTbl.MoveFirst
    
        done = False
               Do Until done = True
                    
                    myInputTbl.MoveNext
                    
                    If myInputTbl.EOF Then
                        done = True
                    Else
                        mynext = myInputTbl!RecordDate
                    End If
                    
                    myInputTbl.MovePrevious
                    
                    myOutputTbl.AddNew
    
                     For Each fld In myInputTbl.Fields
                         myOutputTbl(fld.name) = myInputTbl(fld.name)
        
    
                     Next
                     myOutputTbl!interval = (mynext - myInputTbl!RecordDate) * 24 * 60
                         
    
                     myOutputTbl.Update
                
                    
                    myInputTbl.MoveNext
    
                Loop

 Loop
 
        
 DoCmd.OpenForm "frmTmpKomoriParameters", acNormal, , , , acHidden
 
With Forms!frmTmpKomoriParameters.Form

.OrderNo = Me.OrderNo

.JobNo = Me.JobNo
.StartDate = Me.StartDate
.FinishDate = Me.FinishDate
.SpeedAve = Me.SpeedAve
.PrintSheets = Me.PrintSheets

End With

        

DoCmd.OpenReport "rptKomoriDetail", acViewPreview



End Sub

