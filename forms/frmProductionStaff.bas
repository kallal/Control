Private Sub cmdEndShift_Click()

'First check if any tasks are still happening for this individual.

Dim mysql As String
Dim myRST As Recordset

mysql = "SELECT * FROM dbo_CompanyEmployeeTask WHERE [companyEmployeeID] = " & Me.CompanyEmployeeID & " and nz([endtime],0) = 0"
Set myRST = CurrentDb.OpenRecordset(mysql, dbOpenSnapshot)


If myRST.EOF = False Then
    MsgBox "You are still working on a task. You must end all of your tasks before ending your shift.", vbCritical, ProgramName
    MsgBox "There are still open items on Docket " & DLookup("[DocketNum]", "dbo_ProjectHeader", "[ID] = " & myRST!ProjectHeaderID)
    
    Exit Sub
End If
    
        
    

myResponse = vbNo
myResponse = MsgBox("Ending Shift for " & Me.FN & " " & Me.LN & ". Are you sure?", vbQuestion + vbYesNo + vbDefaultButton2, ProgramName)
If myResponse = vbYes Then
    Me.ShiftEnd = Now
    Me.Requery
End If

End Sub

Private Sub cmdStartShift_Click()
myResponse = ""
myResponse = InputBox("Please scan or enter your Employee ID.", ProgramName)
If myResponse = "" Then
    Exit Sub
End If


If Left(myResponse, 3) = "EMP" And Len(myResponse) = 7 Then
    'Login this user
    Dim MYDB As Database
    Dim myRST As Recordset
    Dim myRST2 As Recordset
    Set MYDB = CurrentDb()
    Set myRST = MYDB.OpenRecordset("dbo_CompanyEmployee", dbOpenDynaset, dbSeeChanges)
    myEmployeeID = Val(Right(myResponse, 4))
    myRST.FindFirst "[ID] = " & myEmployeeID
    If myRST.NoMatch Then 'Check if employee ID ok
        MsgBox "Employee not found. Please try again.", vbCritical, ProgramName
        Exit Sub
    End If
    
    Set myRST2 = MYDB.OpenRecordset("dbo_CompanyEmployeeShift", dbOpenDynaset, dbSeeChanges)
    myRST2.FindFirst "[CompanyEmployeeID] = " & myEmployeeID & " AND isnull([shiftend])" 'Check if this user is already logged in to a shift anywhere.
    If myRST2.NoMatch = True Then
    
        myRST2.AddNew
        myRST2!CompanyEmployeeID = myRST!ID
        myRST2!ShiftBegin = Now
        myRST2!StationID = rsLocSS!tmpsTationid
        myRST2.Update
    Else
        myResponse = vbNo
        mystationname = DLookup("StationName", "dbo_Station", "[ID] = " & myRST2!StationID)
        
        myResponse = MsgBox("You are still logged in at the " & mystationname & " Station." & vbCrLf & "Do you want to move to this station?", vbQuestion + vbYesNo + vbDefaultButton2, ProgramName)
        If myResponse = vbNo Then
            Exit Sub
        Else
            myRST2.Edit
            myRST2!StationID = rsLocSS!tmpsTationid
            myRST2.Update
        End If
    End If
    
Else
    MsgBox "Employee not found. Please try again.", vbCritical, ProgramName
End If

Me.Requery

End Sub