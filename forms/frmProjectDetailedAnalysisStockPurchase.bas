Option Compare Database

Private Sub Form_Load()
Dim mysql As String
Dim myrs As Recordset
Dim myPlateCount As Integer
Dim myPlateValue As Double

mysql = "SELECT * FROM qryPlateCount WHERE [ProjectHeaderID] = " & Parent.ID


Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)

myPlateCount = 0
myPlateValue = 0


If myrs.RecordCount = 0 Then Exit Sub

myrs.MoveFirst

Do Until myrs.EOF
    If myrs!DepartmentID = 1 Then
        myPlateCount = myPlateCount + Nz(myrs!NetPlates, 0)
        myPlateValue = myPlateValue + Nz(myrs!NetPlates, 0) * Nz(myrs!PlateCost, 0)
    End If
    
    myrs.MoveNext
Loop

'myPlateValue = myPlateCount * 11.5 ' not used - plate value is now added up as the loop above runs

Me.myPlateCount = myPlateCount
Me.myPlateValue = myPlateValue


End Sub