Private Sub Form_Dirty(Cancel As Integer)
If Parent!cmdEdit.caption = "&LOCKED" Then
    myResponse = vbNo
    myResponse = MsgBox("This project is locked. Do you want to unlock?", vbYesNo + vbDefaultButton2 + vbQuestion, ProgramName)
    If myResponse = vbYes Then
        Call Parent.cmdEdit_Click
        Cancel = False
    Else
        Cancel = True
    End If
       
End If

End Sub

Private Sub txtHrsEnter_AfterUpdate()
    myhrloc = InStr(Me!txtHrsEnter, "h")
    myminloc = InStr(Me!txtHrsEnter, "m")

If myhrloc > 0 Or myminloc > 0 Then

    If myhrloc > 0 Then
        myhr = Val(Left(Me!txtHrsEnter, myhrloc - 1))
        mymin = Val(Right(Me!txtHrsEnter, Len(Me!txtHrsEnter) - myhrloc))

    End If
    
    If myminloc > 0 Then
        mymin = Val(Mid(Me!txtHrsEnter, myhrloc + 1, myminloc - myhrloc - 1))
    End If

    mytimedec = myhr + (mymin / 60)
    Me!HoursEst = mytimedec
Else
    mytimedec = Val(Me!txtHrsEnter)
    Me.HoursEst = Round(mytimedec, 2)
    
End If

Me!txtHrsEnter = Round(mytimedec, 2)
If Me.Dirty Then Me.Dirty = False
End Sub

