Private Sub cboStatus_AfterUpdate()

myStatus = Me!cboStatus
    
If Left(Me!cboStatus, 4) = "COMP" Then
    myStatus = "COMP (" & Date & ")"
End If

If Left(Me!cboStatus, 3) = "ORD" Then
    myStatus = "ORD (" & Date & ")"
End If

Me!cboStatus = myStatus
        
Parent!StMailShop = myStatus
If Me.Dirty Then Me.Dirty = False
If Parent.Dirty Then Parent.Dirty = False
End Sub

Private Sub cboStatus_BeforeUpdate(Cancel As Integer)
If Left(Parent!StMailShop, 4) = "COMP" Then
    myResponse = vbNo
    myResponse = MsgBox("This set of tasks has already been marked complete - do you wish to reopen this set of tasks?", vbYesNo + vbCritical, ProgramName)
    If myResponse = vbNo Then
        Cancel = True
    Else
        Cancel = False
    End If
End If

End Sub


Private Sub cmdAllComplete_Click()
myResponse = vbNo

myResponse = MsgBox("Mark all Mail Data Processing tasks as complete. Are you sure?", vbYesNo + vbDefaultButton2, ProgramName)
If myResponse = vbYes Then
    Dim MYDB As Database
    Dim myRST As Recordset
    Set MYDB = CurrentDb()
    Dim mysql As String
    mysql = "SELECT * FROM dbo_CompanyEmployeeTask WHERE [ProjectComponentHeaderID] = " & Me.ProjectComponentID
    Set myRST = MYDB.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
    mycriteria = "[ProjectComponentHeaderID] = " & Me.ProjectComponentID & " and NZ([endTime],0) = 0 AND [CATEGORY] = 9" 'MAIL SHOP
    myRST.FindFirst mycriteria
    If myRST.NoMatch = False Then
        'found somebody has a task open.
        MsgBox "A task is still in progress. Please end all MAIL SHOP tasks in progress before clicking on this button.", vbCritical, ProgramName
        Exit Sub
    End If
    
    DoCmd.OpenForm "frmSelectNextOption"
    
    Dim myRST2 As Recordset
    Set myRST2 = MYDB.OpenRecordset("dbo_NextStatusOptions", dbOpenDynaset, dbSeeChanges)
    myRST2.MoveFirst
    myOptionNum = 1
    Do Until myRST2.EOF
        If myRST2!FinishLocation = 9 Then ' MAILSHOP = 9
            MyField = "Opt" & Trim(myOptionNum)
            myCaption = "Set " & DLookup("[TaskCategory]", "dbo_TaskCategory", "[ID] =" & myRST2!NextLocation) & " to " & myRST2!NextStatus
            
            Forms!frmSelectNextOption(MyField).caption = myCaption
            Forms!frmSelectNextOption(MyField).visible = True
            
            Forms!frmSelectNextOption(MyField).tag = myRST2!ID 'put the ID in the tag for ease of set afterwards.
            myOptionNum = myOptionNum + 1
        End If
        myRST2.MoveNext
    Loop
    myStatus = "COMP (" & Format(Date, "MMM D") & ")"
    Me.cboStatus = myStatus
    Parent.StMailShop = myStatus
End If
If Me.Dirty Then Me.Dirty = False
End Sub


Private Sub cmdTask_Click()
'Create Criteria for filtering Task form
mycriteria = "[ProjectHeaderID] = " & Parent.Parent.ID
mycriteria = mycriteria & " and [ProjectComponentHeaderID] = " & Me.ProjectComponentID
mycriteria = mycriteria & " and [Category] = 9"
mycriteria = mycriteria & " and [CompanyEmployeeID] =" & cLoginID
'MsgBox mycriteria

'Create OpenArgs for passing default values to Task form.
myArgs = "2ProjectHeaderID|" & Parent.Parent.ID & "|"
myArgs = myArgs & "2ProjectComponentHeaderID|" & Me.ProjectComponentID & "|"
myArgs = myArgs & "2Category" & "|9|"
myArgs = myArgs & "2CompanyEmployeeID|" & cLoginID & "|"


DoCmd.OpenForm "frmProjectComponentTask", acNormal, , mycriteria, acFormEdit, acDialog, myArgs
Parent.Refresh

Call Form_Current

If Me.Dirty Then Me.Dirty = False
End Sub

