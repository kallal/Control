Private Sub cmdBuildFriendlyDescription_Click()
Dim myDesc As String
Dim myrs As Recordset
Dim mysql As String
Dim myEstAmt As Double
If Parent.Dirty Then Parent.Dirty = False

myDesc = ""
myEstAmt = 0

mysql = "Select * FROM dbo_MailPricingDetail WHERE [ProjectComponentID] = " & Parent.ID & " ORDER BY [CategoryID]"

Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)

myrs.MoveFirst

Do Until myrs.EOF

    myDesc = myDesc & IIf(Len(myrs!ShortDesc) > 0, myrs!ShortDesc & vbCrLf, "")
    
    
    myEstAmt = myEstAmt + (Nz(myrs![BasePrice], 0) + ((myrs![Qty] * Nz(myrs![SellPrice], 0)) / IIf(Nz(myrs![Unit], 0) = 1, 1, 1000)))
    myrs.MoveNext
Loop
myrs.Close

Parent.FriendlyDescription = myDesc
Parent.ExtQuoteAmt1 = myEstAmt
Parent.ExtQuoteSell1 = Round(Parent.ExtQuoteAmt1 + (Parent.ExtQuoteAmt1 * (Parent.ExtQuoteDisc1 / 100)))


End Sub

