    Select Case Me.frmShowQueue
    Case 1
        mySubFilter = mySubFilter & " [QueueID] = 2 AND"
    Case 2
        mySubFilter = mySubFilter & " [QueueID] = 3 AND"
    Case 3
        mySubFilter = mySubFilter & " [QueueID] = 5 AND"
    End Select
End If

'** add filters for Job Ticket Status
    
If Me.frmShowPrinted <> 3 Then
    Select Case Me.frmShowPrinted
    Case 1
        mySubFilter = mySubFilter & " nz([TrackingCode],'') = '' AND"
    Case 2
        mySubFilter = mySubFilter & " nz([TrackingCode],'') <> '' AND"
    End Select
End If

        


If Len(mySubFilter) > 0 Then
    mySubFilter = Left(mySubFilter, Len(mySubFilter) - 3)
    
    mySubFilter = "SELECT * from ssQryOrderDetails WHERE " & mySubFilter
Else
    mySubFilter = "SELECT * from ssQryOrderDetails"
End If


mySubFilter = mySubFilter & " ORDER BY [ProductID]"

Set myQDF = CurrentDb.QueryDefs("ssQryOrderDetailsForReport")
myQDF.sql = mySubFilter

'MsgBox mySubFilter ' for debugging



myfilter = ""
If Len(Me.txtCustomerName) > 0 Then
    myfilter = " [CustomerName] LIKE '*" & Trim(Me.txtCustomerName) & "*' AND"
End If

If Len(Me.txtOrderNumber) > 0 Then
    myfilter = " [Numerator] LIKE '*" & Trim(Me.txtOrderNumber) & "*' AND"

End If

myJobNumber = Val(Nz(Me.txtJobNumber, 0))

If myJobNumber > 0 Then
    myfilter = " [JobNumber] = " & myJobNumber & " AND"

End If


If Len(myfilter) > 0 Then
    myfilter = Left(myfilter, Len(myfilter) - 3)
    
    myfilter = "SELECT * from ssQryOrdersHeader WHERE " & myfilter
Else
    myfilter = "SELECT * from ssQryOrdersHeader"
End If

Set myQDF = CurrentDb.QueryDefs("ssQryOrdersHeaderForReport")
myQDF.sql = myfilter


'Redraw everything!!


Me.ssRptOrders.Report.RecordSource = myfilter






 














End Sub

