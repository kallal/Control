Private Sub cmdShipManifest_Click()
'Check to see if any Packing Lists are tagged to ship.

Dim MYDB As Database
Dim mySource As Recordset
Dim myTarget As Recordset

Set MYDB = CurrentDb()

Set myTarget = MYDB.OpenRecordset("dbo_ShipmentHeader", dbOpenDynaset, dbSeeChanges)


'Check if any packing lists are available and/or flagged

myCount = 0
mymatch = False



Dim myContactGeneralID As Integer
Dim myContactLocationID As Integer
Dim myAttn As String
Dim myAdd1 As String
Dim myAdd2 As String
Dim myCity As String
Dim myProv As String
Dim myPC As String
Dim myCountry As String

mySQLCmd = "SELECT * FROM dbo_PackingListHeader WHERE [ProjectHeaderID] = " & Me.ID

Set mySource = MYDB.OpenRecordset(mySQLCmd, dbOpenDynaset, dbSeeChanges)

mySource.MoveFirst
Do Until mySource.EOF
    If mySource!ProjectHeaderID = Me.ID Then
        myCount = myCount + 1
        If mySource!ShipListFlag = True Then
            mymatch = True
            On Error Resume Next
            myContactGeneralID = mySource!ContactGeneralID
            myContactLocationID = mySource!ContactLocationID
            myAttn = mySource!ATTN
            myAdd1 = mySource!Add1
            myAdd2 = mySource!Add2
            myCity = mySource!City
            myProv = mySource!Prov
            myPC = mySource!PC
            myCountry = mySource!Country
            On Error GoTo 0
            
                      
        End If
    End If
    mySource.MoveNext
Loop


If Nz(myCount) = 0 Then
    MsgBox "You have no Packing Lists available. Please create your Packing List before using the Quickship button.", vbCritical, ProgramName
    Exit Sub
End If


If mymatch = False Then
    MsgBox "You have no Packing Lists flagged to ship. Please click the 'Tag to Ship' box next to the Packing List(s) you want to ship before using the Quickship button.", vbCritical, ProgramName
    Exit Sub
End If

'Create new ShipmentHeader and populate the fields
myTarget.AddNew
myTarget!ShipViaID = Me.ShipVia
myTarget!ShipmentDate = Now()
myTarget!EnteredBy = cLoginID
myTarget!EstCost = 0

On Error Resume Next
myTarget!ContactGeneralID = myContactGeneralID
myTarget!ContactLocationID = myContactLocationID
myTarget!ATTN = myAttn
myTarget!Add1 = myAdd1
myTarget!Add2 = myAdd2
myTarget!City = myCity
myTarget!Prov = myProv
myTarget!PC = myPC
myTarget!Country = myCountry
On Error GoTo 0

myTarget.Update
myTarget.Bookmark = myTarget.LastModified          ' ADK force + get SQL last ID
myNewID = myTarget![ID]

'Assign new ShipmentHeaderID to tagged Packing Lists and untag them.
mySource.MoveFirst

Do Until mySource.EOF
    If mySource!ProjectHeaderID = Me.ID Then
        If mySource!ShipListFlag = True Then
            mySource.Edit
            mySource!ShipmentHeaderID = myNewID
            mySource!ShipListFlag = False
            mySource.Update
        End If
    End If
    mySource.MoveNext
Loop

' Open the ShipmentHeader form and assign the waybill info, print labels.
DoCmd.OpenForm "frmShipmentHeader", acNormal, , "[ID] = " & myNewID, acFormEdit, acDialog

Me.Refresh
Me.Repaint

End Sub

Private Sub cmdShipNow_Click()

'Create a new Packing List Header and populate the required fields

Dim MYDB As Database

Dim mySource As Recordset
Dim myTarget As Recordset
Dim NewPackingListID As String


Set MYDB = CurrentDb()

Set myTarget = MYDB.OpenRecordset("dbo_PackingListHeader", dbOpenDynaset, dbSeeChanges)

myTarget.AddNew

myTarget!ContactGeneralID = Me.ContactGeneralID
myTarget!ContactLocationID = Me.ShipAddLocationID
myTarget!CompanyName = DLookup("CompName", "dbo_ContactGeneral", "[ID] = " & Me.ContactGeneralID)
myTarget!Add1 = Me.ShipAdd1
myTarget!Add2 = Me.ShipADD2
myTarget!City = Me.ShipCITY
myTarget!Prov = Me.ShipPROV
myTarget!PC = Me.ShipPC
myTarget!Country = Me.ShipCountry
myTarget!PONumber = Me.PONumber
myTarget!ShipVia = Me.ShipVia
myTarget!SpecialInstructions = Me.ShipInstruction ' don't want this printed on this form.
myTarget!ATTN = Me.ShipAttn

myTarget!NumPkg = 0
myTarget!NumSkid = 0
myTarget!NumMono = 0
myTarget!NumLC = 0
myTarget!NumTub = 0
myTarget!ShipmentType = 1
myTarget!IsCollect = False
myTarget!IsPickup = False
myTarget!PackingListDate = Now()
myTarget!CompanyEmployeeID = cLoginID
myTarget!ProjectHeaderID = Me.ID

myTarget.Update

myTarget.Bookmark = myTarget.LastModified       ' ADK force ID create for SQL server
NewPackingListID = myTarget!ID 'Grab the newly assigned Packing List ID number.

'Prepopulate the detail lines for the new shipment.


Set myTarget = MYDB.OpenRecordset("dbo_PackingListDetail", dbOpenDynaset, dbSeeChanges)

mySQLCmd = "SELECT * FROM dbo_ProjectComponentHeader WHERE [ProjectheaderID] = " & Me.ID & " AND [ShipFlag] = True"

Set mySource = MYDB.OpenRecordset(mySQLCmd, dbOpenDynaset, dbSeeChanges)

If Not (mySource.BOF And mySource.EOF) Then

    mySource.MoveFirst
End If





Do Until mySource.EOF
    
    myTarget.AddNew
    myTarget!PackingListHeaderID = NewPackingListID
    myTarget!ProjectComponentHeaderID = mySource!ID
    myTarget!Qty = mySource!ExpectedQty
    myTarget!Description = mySource!ComponentName
    myTarget.Update
    
    mySource.Edit
    mySource!ShipFlag = False
    mySource.Update
        
    mySource.MoveNext
Loop



Me!frmProjectCriticalPathPackingList.Requery
'Call the Packing List form using the ID we just got and give it the focus.
DoCmd.OpenForm "frmPackingList", acNormal, , "[ID] = " & NewPackingListID, acFormEdit, acDialog


Me.Refresh

End Sub

