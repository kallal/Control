Private Sub cmdMarkAsPrinted_Click()

Dim myrs As Recordset



myProductid = Val(Nz(Me.CurrentProductID, 0))

'Can't update query - need to update individual tables

mysql = "SELECT * FROM ssd_OrderDetails WHERE [ProductID] = " & myProductid

'Update 'OrderedProductStatusID to 190, ShippingStatusID to 190, QueueID = 3, StatusInQueue = 4, PrintedDate = NOW

Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)

If myrs.EOF Then
    MsgBox "Job Not Found in Order Details - Exiting update routine.", vbCritical, ProgramName
    Exit Sub
End If

myrs.Edit
myrs!OrderedProductStatusID = 190
myrs!PrintedDate = Now()
myrs.Update

Set myrs = Nothing

mysql = "SELECT * FROM ssd_JobInQueue WHERE [ProductID] = " & myProductid
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
myrs.Edit
myrs!QueueID = 3
myrs!StatusInQueue = 4
myrs.Update

Set myrs = Nothing

mysql = "SELECT * FROM ssd_Delivery WHERE [ProductID] = " & myProductid
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
myrs.Edit
myrs!StatusID = 190
myrs.Update
Set myrs = Nothing

'Cleanup, and ready for the next one.

Me.ssRptOrderDetails.visible = False
Me.CurrentProductID = ""
Me.txtProductID = ""
MsgBox "Job has been marked as READY TO SHIP.", vbInformation, ProgramName

DoCmd.GoToControl "txtProductID"


End Sub

Private Sub cmdPrint_Click()

myProductid = Val(Nz(Me.CurrentProductID, 0))

DoCmd.OpenReport "rptSS_JobLabel", acViewNormal, , "[ProductID] = " & myProductid

End Sub





Private Sub Form_Current()
DoCmd.GoToControl ("txtProductID")
End Sub

Private Sub txtProductID_AfterUpdate()
Dim myrs As Recordset

'Make sure it is an Integer


myProductid = Val(Nz(Me.txtProductID, 0))

'Check to see if it is still available
mysql = "SELECT * FROM ssQryOrderDetails WHERE [ProductID] = " & myProductid




Set myrs = CurrentDb.OpenRecordset(mysql)

If myrs.EOF Then
    MsgBox "Incorrect Entry, or Job is no longer in work queues.", vbInformation, ProgramName
End If

If myProductid = 0 Or myrs.EOF Then
    Me.txtProductID = ""
    Me.ssRptOrderDetails.visible = False
    Exit Sub
End If


If myrs!OrderedProductStatusID = 200 Then
    myresult = vbNo
    myresult = MsgBox("This job has already been marked as SHIPPED. Do you wish to continue?", vbQuestion + vbYesNo, ProgramName)
    If myresult = vbNo Then
        Me.txtProductID = ""
        Me.ssRptOrderDetails.visible = False
        Exit Sub
    End If
End If


Me.CurrentProductID = myProductid

'Tell the report to pull up that particular job

Me.ssRptOrderDetails.Report.RecordSource = mysql

Me.ssRptOrderDetails.visible = True

If Me.chkAutoPrintLabel = True Then
Call cmdPrint_Click
'Call cmdPrint_Click 'Enable if we want two copies

End If

Me.txtProductID = ""

End Sub