'    Reports(myReportName).Tag = ""
'End If


'DoCmd.Close acReport, myReportName


Dim qdf As DAO.QueryDef
Set qdf = CurrentDb.QueryDefs("qrySchedulingSummaryforReport")
qdf.sql = "Select * from qrySchedulingSummaryWithShipping WHERE " & strWhere

