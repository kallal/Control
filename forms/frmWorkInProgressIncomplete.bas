Private Sub cmdSearch_Click()
Application.Echo False

'Check for invalid entries - i.e. ' sign in search

myAbort = False

If InStr(Me.txtCompanyID, "'") > 0 Then myAbort = True

If myAbort = True Then
    MsgBox "Please do not use a quote symbol in the search fields.", vbCritical, ProgramName
    Application.Echo True
    Me.txtCompanyID = ""
    Exit Sub
End If


If Me.Dirty Then Me.Dirty = False




Dim strWhere As String
Dim tmpdatestart As Date
Dim tmpdateend As Date
Dim myTag As String
myTag = ""



Me.rptWorkInProgressSummary.SourceObject = ""


strWhere = ""

strWhere = strWhere & " NZ([DocketNum],0) > 0 AND"

myDate = False
mySortByFlag = False 'Flag for if to show the "Sort by" line.
'create filter criteria

'if only put date filter on if either of the dates have entries...

If Nz(Me.txtDateStart, "") <> "" And Nz(Me.txtDateEnd, "") <> "" Then

    If Nz(Me.txtDateStart, "") = "" Then
        tmpdatestart = "01/01/1900"
    Else
        tmpdatestart = Me.txtDateStart
        myDate = True
    
    End If
    
    If Nz(Me.txtDateEnd, "") = "" Then
        tmpdateend = "12/31/2199"
    Else
        tmpdateend = Me.txtDateEnd
        myDate = True
    End If
    
    If myDate = True Then
        mySortByFlag = True
        myTag = myTag & " Date From: " & Nz(Me.txtDateStart, "ANY") & " to " & Nz(Me.txtDateEnd, "ANY")
    End If
    
   
    strWhere = strWhere & " (([DueDate] >= " & Format$(tmpdatestart, "\#mm\/dd\/yyyy\#") & ") AND([DueDate] <= " & Format$(tmpdateend, "\#mm\/dd\/yyyy\#") & ") OR NZ([DUEDATE],0) = 0) AND"

End If

If Len(Me.txtCompanyID) <> 0 Then
    mycomp = Me.txtCompanyID
    

    done = False
        
    Do Until done = True 'create as many criteria as there are spaces
        mymark = InStr(Trim(mycomp), " ")
        
        If mymark = 0 Then done = True
        If mymark = 0 Then mymark = Len(mycomp)
        
        myComp1 = Trim(Left(mycomp, mymark))
        mycomp = Right(mycomp, Len(mycomp) - mymark)
        strWhere = strWhere & " [COMPNAME] like '*" & myComp1 & "*' AND"
        mySortByFlag = True
        myTag = myTag & " | Company: " & myComp1
        
    Loop

End If



If Len(Me.txtProjectName) <> 0 Then
    strWhere = strWhere & " [ProjectName] like '*" & Me.txtProjectName & "*' AND"
    myTag = myTag & " | Project: " & Me.txtProjectName
    mySortByFlag = True
End If

myDocketnum = Val(Nz(Me.txtDocketNum, 0))

If myDocketnum > 0 Then
    If myDocketnum <= 99999 Then 'Pad it with 3xxxxx before running query.
        myDocketnum = myDocketnum + 300000
        Me.txtDocketNum.Value = str(myDocketnum)
    End If
    
    strWhere = strWhere & " [DocketNum] = " & myDocketnum & " AND"
    myTag = myTag & " | Docket Number: " & str(myDocketnum)
    mySortByFlag = True
End If

If Len(Me.txtSalesRepID) <> 0 Then
    strWhere = strWhere & " [SalesRepID] = " & Me.txtSalesRepID & " AND"
    myTag = myTag & " | Sales Rep: " & Me.txtSalesRepID.Column(1)
    mySortByFlag = True
End If

If Len(Me.txtCsrID) <> 0 Then
    strWhere = strWhere & " [CsrID] = " & Me.txtCsrID & " AND"
    myTag = myTag & " | CSR: " & Me.txtCsrID.Column(1)
    mySortByFlag = True
End If




If Nz(Me.chkShowRTSandRTI, False) = False Then
    strWhere = strWhere & " nz([completiondate],0) = 0 AND [ProjectStatusID] < 35 AND" 'Hide Completed
    'strWhere = strWhere & " [ProjectStatusID] < 30 AND" 'Hide RTI and above
Else
 strWhere = strWhere & " [ProjectStatusID] < 35 AND" 'Never show ready to Invoice
End If


'Set up which plant to view - default to all (99)
' If Me.frmPlant = 99 then show all - no line required
If Me.frmPlant >= 1 And Me.frmPlant <= 9 Then
     strWhere = strWhere & " [CompanyLocationID] = " & Me.frmPlant & " AND"
End If





If Len([strWhere]) >= 4 Then
    strWhere = Left([strWhere], Len([strWhere]) - 3)
End If





'APPLY FILTER

Select Case fraShowBy
    Case 1
        myReportNameFull = "Report.rptWIPByDueDateOnScreen"
        myReportName = "rptWIPByDueDateOnScreen"
    Case 2
        myReportNameFull = "Report.rptWIPByClientOnScreen"
        myReportName = "rptWIPByClientOnScreen"
End Select

rsLocSS.Edit
If mySortByFlag = True Then
    rsLocSS!txtSortByDisplay = "Sort by - " & myTag
Else
    rsLocSS!txtSortByDisplay = ""
End If
rsLocSS.Update

