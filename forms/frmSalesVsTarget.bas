Private Sub Form_Load()

   Dim strFMonth     As String
   Dim strSQL        As String
   Dim dt            As Date
   Dim i             As Integer
   
   'Stop
   
   strSQL = CurrentDb.QueryDefs("qryRptSalespersonTargetVsActual").sql
   
   ' set [ThisMonthTarget]
   
   strFMonth = "[estAmt" & Format(Month(Date), "00") & "]"
   
   strSQL = Replace(strSQL, "[ThisMonth]", strFMonth)
   
   
   ' set ThreeMonths
   
   dt = DateSerial(Year(Date), Month(Date), 1)
   
   strFMonth = "([estAmt" & Format(Month(DateAdd("m", 1, dt)), "00") & "] + " & _
               "[estAmt" & Format(Month(DateAdd("m", 2, dt)), "00") & "] + " & _
               "[estAmt" & Format(Month(DateAdd("m", 3, dt)), "00") & "])"
   
   strSQL = Replace(strSQL, "[ThreeMonths]", strFMonth)
   
   ' set total est (year start) to end of last month
   strFMonth = ""
   If Month(Date) > 1 Then
      For i = 1 To Month(Date) - 1
         
         If strFMonth <> "" Then strFMonth = strFMonth & " + "
         
         strFMonth = strFMonth & "[estAmt" & Format(i, "00") & "]"
      Next i
      
      strFMonth = "(" & strFMonth & ")"
      strSQL = Replace(strSQL, "[TotalEst]", strFMonth)
   Else
      strSQL = Replace(strSQL, "[TotalEst]", "[EstAmt01]")
   End If
   

   ' set new client expectation
   strFMonth = "[newclt" & Format(Month(dt), "00") & "]"
   strSQL = Replace(strSQL, "[newclt01]", strFMonth)
   
   
   Debug.Print strSQL
   
   Me.RecordSource = strSQL

   
End Sub