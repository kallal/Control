Private Sub Form_Current()


Me.txtWelcome = DLookup("[LoginMessage]", "dbo_Company", "[ID] = " & 1)

Dim MYDB As Database
Dim myRST As Recordset

Set MYDB = CurrentDb()
Set myRST = MYDB.OpenRecordset("dbo_Quotes", dbOpenDynaset, dbSeeChanges)
Randomize

'-- this section is to show what the current database is on the window

Dim myTdf As TableDef
Dim myTdfs As TableDefs


Set myTdfs = MYDB.TableDefs

done = False

For Each myTdf In myTdfs

    'MsgBox " inside loop"
    
         If Left(myTdf.SourceTableName, 3) <> "tbl" Then 'If the table source is other than a PDQ table
         
                Me.txtDataFileLocation = myTdf.connect
                done = True
                
         End If
Next






myRST.MoveLast

myRST.MoveFirst

myreccount = myRST.RecordCount


myRndID = Int(Rnd() * myreccount)



myRST.Move myRndID

myQuote = myRST!quote
myauthor = myRST!Author

Me!txtQuote = myQuote & " - " & myauthor


'Set up press schedule for the correct press. - Not working yet..
'mymachineid = Forms!frmLogin!cboStationID.Column(5)
'MsgBox "New Press ID is " & mymachineid

'Me.frmWIPPrint_HOME.Form!cboSelectPress = mymachineid
'Me.frmWIPPrint_HOME.Requery




If Me.Dirty Then Me.Dirty = False

End Sub



