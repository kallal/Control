Private Sub cmdEnd_Click()
Dim mytime As Double
mytime = 0


If Nz(Me.StartTime, 0) = 0 Then
    MsgBox "This Task has not been started yet. You cannot end a task that hasn't started.", vbCritical, ProgramName
    Exit Sub
End If

If Nz(Me.EndTime, 0) <> 0 Then
    MsgBox "This Task has already been closed.", vbCritical, ProgramName
    Exit Sub
End If

myPrompt = "Ending task " & Me.InfoLine & ". Please type any additional production notes regarding this task (i.e. machine problems, stock issues, etc.)"
myresp = ""
myresp = InputBox(myPrompt, ProgramName)

'MsgBox myresp


'Me.EndTime = Format(Now, "short time")


If Me.Dirty Then Me.Dirty = False

Me.EndTime = Time
Me.enddate = Date


mytime = mytime + (Me.EndTime - Nz(Me.StartTime, 0)) * 24 ' result in fraction of day X 24 hrs

mytime = mytime + (Me.enddate - Nz(Me.StartDate, 0)) * 24 'add in number of days if it went overnight

'MsgBox mytime


Me.TotalTime = mytime





Me.Notes = myresp
Me.Requery
End Sub

