Private Sub AddNumber_AfterUpdate()
If Len(AddNumber) <> 6 Then
    MsgBox "You must enter a six digit number of an Invoice. Cancelling.", vbCritical, ProgramName
    Me.AddNumber = ""
    Exit Sub
End If

Dim mySQLin As String
Dim mySQLout As String
Dim myRSin As Recordset
Dim myRSout As Recordset

mySQLin = "SELECT * FROM dbo_ProjectHEader WHERE [Invoicenum] = " & Me.AddNumber
Set myRSin = CurrentDb.OpenRecordset(mySQLin, dbOpenSnapshot)

If myRSin.BOF And myRSin.EOF Then
    MsgBox "Invoice not found. Cancelling.", vbCritical, ProgramName
    Set myRSin = Nothing
    Me.AddNumber = ""
    Exit Sub
End If

'I have a match... lets populate the local table.
mySQLout = "SELECT * from loc_BatchPrint"
Set myRSout = CurrentDb.OpenRecordset(mySQLout, dbOpenDynaset, dbSeeChanges)

myRSin.MoveFirst
myRSout.AddNew

myRSout!ProjectHeaderID = myRSin!ID
myRSout!QuoteNum = myRSin!QuoteNum
myRSout!DocketNum = myRSin!DocketNum
myRSout!InvoiceNum = myRSin!InvoiceNum

myRSout!CompanyName = DLookup("[CompName]", "dbo_ContactGeneral", "[ID] = " & myRSin!ContactGeneralID)
myRSout!JobName = myRSin!ProjectName

myRSout.Update

Set myRSout = Nothing
Set myRSin = Nothing

Me.AddNumber = ""

Form.Requery


End Sub

Private Sub cmdClearList_Click()

Dim mysql As String

mysql = "DELETE * from loc_BatchPrint"

DoCmd.SetWarnings False

DoCmd.RunSQL mysql

DoCmd.SetWarnings True

Form.Requery

End Sub

Private Sub cmdPDF_Click()

'Create folder on desktop if it doesn't exist, then dump the PDF's to there.
strUserName = Environ("UserName")
strPath = "C:\Documents And Settings\" & strUserName & "\Desktop"


Dim myrs As Recordset
Dim mysql As String

mysql = "SELECT * from loc_BatchPrint"

Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenSnapshot)

If myrs.BOF And myrs.EOF Then
    MsgBox "Nothing to print. Cancelling.", vbCritical, ProgramName
    Exit Sub
End If

myrs.MoveFirst
Do Until myrs.EOF
myFileName = strPath & "\I" & Format(myrs!InvoiceNum, "000000") & ".pdf"

Application.Echo False

myReportName = "rptInvoice" & IIf(cControlCompanyCode <> "BG" And cControlCompanyCode <> "DPI", cControlCompanyCode, "")


DoCmd.OpenReport myReportName, acViewPreview, , "[ID] = " & myrs!ProjectHeaderID


On Error GoTo FileInUse:
DoCmd.OutputTo acOutputReport, "", acFormatPDF, myFileName, False



DoCmd.Close acReport, myReportName


myrs.MoveNext
Loop
Set myrs = Nothing

Application.Echo True

Exit Sub

FileInUse:
    
    DoCmd.Close acReport, myReportName
    Application.Echo True
    MsgBox "Cannot recreate one of the PDF File(s) - You cannot replace a file if you have the file open already. Please close all of the PDF Invoices on your desktop and try again.", vbCritical, ProgramName
    
End Sub

