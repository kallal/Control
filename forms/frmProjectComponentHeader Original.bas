Public Sub ProofApprovals()
Dim myFSCApproved As String
Dim myProofApproved As String
Dim myHardApproved As String
Dim myMailApproved As String
Dim myStatus As String

Dim myrs As Recordset
Dim mysql As String
Me.Refresh

If cBypassPrepressAIYN = True Then
    Exit Sub
End If

 
If Nz(Parent.InvoiceNum, 0) > 0 Then
    'MsgBox "don't bother - invoice:" & Parent.InvoiceNum & " Docket: " & Parent.DocketNum
    
    Exit Sub
End If

If Me.Dirty Then Me.Dirty = False

If Nz(Me.ArtArrivedDate, 0) = 0 Then
    'If we have no art, no point in proceeding with any other tests for proofing or work to be done.
    myStatus = "WAIT ARTWORK"
    Me.StPrepress = myStatus
    Exit Sub
End If

If Nz(Me.ArtArrivedDate, 0) <> 0 And (Nz(Me.ProofSentDate, 0) = 0 And Nz(Me.ProofSentDate, 0) = 0 And Nz(Me.HardProofSentDate, 0) = 0) Then
    'Art is in, but no proofs have been generated yet. - status should be set to what type of work is to be done in prepress
    mysql = "SELECT * from dbo_ProjectComponentPrepress WHERE [ProjectComponentID] = " & Me.ID
    Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenSnapshot)
    myStatus = "ARTWORK ARRIVED"
    If myrs!SetNewYN = True Then myStatus = "SET NEW"
    If myrs!TypeChangesYN = True Then myStatus = "TYPE CHANGES"
    If myrs!PrepYN = True Then myStatus = "PREP"
    If myrs!StandingReimpoYN = True Then myStatus = "REIMPO"
    If myrs!ExactRepeatYN = True Then myStatus = "EXACT REPEAT"
    Me.StPrepress = myStatus
        
    Exit Sub
End If


'Check in reverse order so that the most pertinent approval status is shown

If Me.HardProofSentDate.enabled = True Then
    If Nz(Me.HardProofApprovedDate, 0) > 0 Then
        myHardApproved = "Yes"
    Else
        myHardApproved = "No"
        
        'If Nz(Me.HardProofSentDate, 0) > 0 Then
            myStatus = "WAIT HARDPRF"
        'End If
    End If
Else
    myHardApproved = "NR"
End If

If Me.ProofSentDate.enabled = True Then
    If Nz(Me.ProofApprovedDate, 0) > 0 Then
        myProofApproved = "Yes"
    Else
        myProofApproved = "No"
        'If Nz(Me.ProofSentDate, 0) > 0 Then
            myStatus = "WAIT PDFPRF"
        'End If
        
    End If
Else
    myProofApproved = "NR"
End If

If Me.FSCProofSentDate.enabled = True Then
    If Nz(Me.FSCProofApprovedDate, 0) > 0 Then
        myFSCApproved = "Yes"
    Else
        myFSCApproved = "No"
        'If Nz(Me.FSCProofSentDate, 0) > 0 Then
            myStatus = "WAIT FSC"
        'End If
    End If
Else
    myFSCApproved = "NR"
End If

If Me.MailApprovalRequired = True Then
    If Nz(Me.MailProofApprovedDate, 0) > 0 Then
        myMailApproved = "Yes"
    Else
        myMailApproved = "No"
        myStatus = "WAIT MAIL"

    End If
Else
    myMailApproved = "NR"
End If

'If we're not waiting for anything, and at least one of the client or FSC approvals has been done, then it must be approved...
If myStatus = "" And (myHardApproved = "Yes" Or myProofApproved = "Yes" Or myFSCApproved = "Yes" Or myMailApproved = "YES") Then
    'You can update the status to approved now
    myStatus = "APPROVED"
    
    Me.StPrepress = "APPROVED"

Else
    If Nz(myStatus, "") <> "" Then
        Me.StPrepress = myStatus
    End If
    
    'You can't change status as no proofs have been sent yet
End If


Dim myRecipient As String
Dim mySubject As String
Dim MyMessage As String
   
'find out what the stock situation is... is it in, or on hold at vendor?

If Nz(Me.ProofApprovedDate, 0) <> 0 Then 'You can trigger any required emails now.
Dim MYDB As Database
Dim myRST As Recordset
Set MYDB = CurrentDb()
Set myRST = MYDB.OpenRecordset("dbo_ProjectComponentStock", dbOpenSnapshot)

myRST.FindFirst ("[ProjectComponentID] = " & Me.ID)
done = False
Do Until done = True
    If myRST!OrderOption = 5 Then
        ' job is set to hold at vendor - send a release email.
        

        myCC = cReleasePaperAtVendorEmail
       
        myRecipient = DLookup("[email]", "qryContactIDEmail", "[id] = " & myRST!VendorNameID)
        If Nz(myRecipient, "") <> "" Then
            myRecipient = myRecipient & "; " & myCC
        Else
            myRecipient = myCC
        End If
        
        
        mySubject = "D" & DLookup("DocketNum", "dbo_projectHeader", "[ID] = " & Me.ProjectHeaderID) & " - Proof Approved - Release stock at vendor."
        MyMessage = "Proof has been approved for this job. The order was originally placed as Order and Hold... Be sure to confirm stock and quantity, and release the stock."
        
        SendMailCDO myRecipient, mySubject, MyMessage
        
    End If
    
    If myRST!OrderOption = 4 And Nz(myRST!ArrivalDate, 0) <> 0 Then 'paper is here... release it to the cutter or press
    
        myRecipient = cReleaseStockToCutterEmail
        mySubject = "D" & DLookup("DocketNum", "dbo_projectHeader", "[ID] = " & Me.ProjectHeaderID) & " - Proof Approved - Release stock to production."
        MyMessage = "Proof has been approved for this job that you have already received stock for. Please mark the yellow sheet as Proof Approved, and release the stock to production, either to the press or to cutting if required."
        
        SendMailCDO myRecipient, mySubject, MyMessage
        
    End If
    myRST.FindNext ("[ProjectComponentID] = " & Me.ID)
    If myRST.NoMatch Then done = True
    
Loop

End If

    
    
AddContactHistory Parent!ContactGeneralID, Parent!BillAddLocationID, Parent!BillOrderedBy, Parent!ID, 1, "Soft Proof Approved Date Changed to " & Me.ProofApprovedDate & " for component " & Me.ComponentName


If Me.Dirty Then Me.Dirty = False



End Sub

