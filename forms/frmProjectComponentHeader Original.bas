Private Sub cmdAssignQuote_Click()


'check to see if there is already a quote loaded
Dim myrs As Recordset
Dim mysql As String
Dim mySelectQuote As Integer
Dim myReplaceOption As Integer
Dim mySelectPriceOption As Integer


'Set default options for these so all code runs for a new import
mySelectQuote = 2 '1 = same number as before, 2 = new number
myReplaceOption = 1 '1 = replace all tabs, 2 = replace only stock and press tabs.

If Nz(Me.ExtQuoteNum, 0) > 0 Then 'Check if a quote has already been attached.
    'Call new form, set the constants to know what to do with this as we bring them in.
    DoCmd.OpenForm "frmReplaceQuoteOption", , , , , acDialog
    'get results
    mysql = "SELECT * from tmpDboReplaceQuoteOption where [ID] = 1"
    Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset)
    myReplaceOption = myrs!frmReplaceOption
    mySelectQuote = Nz(myrs!frmSelectQuote, 0)
    mySelectPriceOption = Nz(myrs!frmSelectPriceOption, 0)
    If myReplaceOption = 3 Then 'Cancel replace
        Exit Sub
    End If

    
End If



Application.Echo False


Dim myCurrentComponentID As Double
myCurrentComponentID = Nz(Me.ComponentID)


cPDQImportResult = "Success" ' Set the constant before calling my bad code.

'Should be able to skip by now.


If mySelectQuote = 2 Then 'Call the Assign quote browser only if we're looking for a new number
    Me.extQuoteSelect = 1
    
    DoCmd.OpenForm "frmPDQQuote", acNormal, , , , acDialog
    
    If cPDQImportResult = "FAILED" Then
        Application.Echo True
        Exit Sub
    End If
End If


Call extQuoteSelect_Update(myReplaceOption, mySelectPriceOption)



If cPDQImportResult = "FAILED" Then
    Application.Echo True
    Exit Sub
End If


'Parent.Refresh
'Me.Refresh

done = False
Do Until done = True

If myCurrentComponentID = 0 Then
    DoCmd.GoToRecord , , acLast
    done = True
Else

    If Me.ComponentID <> myCurrentComponentID Then
        DoCmd.GoToRecord , , acNext
    Else
        done = True
    End If
End If

Loop

myResponse = vbYes
myResponse = MsgBox("Update Estimate Date to Today's Date?", vbQuestion + vbYesNo, ProgramName)
If myResponse = vbYes Then

    Dim myRecordset As Recordset
    mysql = "SELECT * FROM dbo_ProjectHeader WHERE [ID] = " & Me.Parent.ID
    Set myRecordset = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
    
    myRecordset.Edit
    myRecordset!QuoteDate = Date
    myRecordset.Update
    
    myRecordset.Close
    
    
    
    
   ' Me.Parent!QuoteDate = Format(Date, "mm/dd/yyyy")

    
End If
If Me.Dirty Then Me.Dirty = False
Me.Refresh






Application.Echo True



End Sub

