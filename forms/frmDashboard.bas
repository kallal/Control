Private Sub optShowMe_AfterUpdate()
If Me!optShowMe = True Then
    Me!cboGroupSelect = ""
Else
    If Len(Nz(Me!cboGroupSelect, "")) = 0 Then
        Me!optShowMe = True
        MsgBox "To Select a Group, use the box to the right. Otherwise, your own To-Do's will show.", vbCritical, ProgramName
    End If
End If

If Me.Dirty Then Me.Dirty = False
End Sub