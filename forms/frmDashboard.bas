Private Sub cboGroupSelect_AfterUpdate()
If Len(Nz(Me!cboGroupSelect, "")) > 0 Then
    Me!optShowMe = False
Else
    Me!optShowMe = True
End If
If Me.Dirty Then Me.Dirty = False

End Sub

Private Sub cmdAddToDo_Click()

