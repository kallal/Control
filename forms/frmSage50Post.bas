Sub PostTestHold()
   ' first process list of  customers to ensure they exist
   
   If MsgBox("Post all 'Ready' invoices to sage 50?", vbQuestion + vbDefaultButton2 + vbYesNoCancel, "Post invoices?") <> vbYes Then
      Exit Sub
   End If
   
   
   Dim lngCustomerMissing        As Long
   
   lngCustomerMissing = CheckToAddCustomers(Me.RecordsetClone)
   
   If lngCustomerMissing <> 0 Then
      ' user canceled, or missing customers remain
      Exit Sub
   End If
      
      
   
   
   ' next process list of sales reps (employees) to ensure they exist
   Call CheckToAddSalesReps(Me.RecordsetClone)
   
   
   ' post each record, marking date of success
   Call AddInvoices(Me.RecordsetClone)

   Me.Requery

End Sub

Sub PostInvoices(rst As Recordset)
   
   ' pass list of records to post as invoices (rst)
   
   ' first check + deal with customers
   
   Dim intCustomers        As Integer

   If AddCustomers(rst) <> 0 Then
      ' there are customers NOT addded to sage, we cannot post
      Exit Sub
   End If

  If AddSalesReps(rst) <> 0 Then
     ' there are sales reps to add, cannot post
     Exit Sub
  End If
       
   ' post each record, marking date of success

   Call AddInvoices(rst)

   Me.Requery
  

End Sub


Private Sub cmdPostOne_Click()

   Dim strSQL        As String
   Dim rst           As DAO.Recordset

   Dialog.DefaultBeepAllowed = False
   Dialog.DefaultCopyToClipboardAllowed = False
   Dialog.DefaultSavedTextFileFolder = False
   Dialog.DefaultSaveToFileAllowed = False
   
   
   Dialog.DefaultButtonDelay = 0
   
   Select Case Nz(Me!PostToResult, "")
   
      Case "Ok"
         
         'MsgBox "Already posted", vbExclamation, "Posted"
         
         If Box("Already posted, post again?", vbDefaultButton2 + vbQuestion + vbYesNo, "Post?") = vbYes Then
            CurrentDb.Execute "update dbo_ProjectHeader set PostToResult = null where id = " & Me.ID, dbSeeChanges
            GoSub postsub
         End If
         
         
      Case "Fail"
         
         'MsgBox "Was unable to post - requires manual entry", , "Fail"
         If Box("Was unable to post - requires manual entry" & vbCrLf & _
                   "Try again?", vbDefaultButton2 + vbQuestion + vbYesNo, "Post?") = vbYes Then
            CurrentDb.Execute "update dbo_ProjectHeader set PostToResult = null where id = " & Me.ID, dbSeeChanges
            GoSub postsub
         End If
         
         
      Case "", "Skip", "Ready for Repost"
         Dim lngChoice As Long
         
         lngChoice = Box("Post invoice #" & Me.InvoiceNum, vbCustom + vbDefaultButton3 + vbQuestion, _
                "Post this invoice?", _
                , , 0, 0, 0, 0, _
                "Skip", _
                "No", _
                "Yes")
                
         If lngChoice = vbbt3 Then
            ' we want to post
            ' if status = Skip we have to set = nothing since posting routines ONLY
            ' allow a status of null, or Read To Repost
            If Me!PostToResult = "Skip" Then
               CurrentDb.Execute "update dbo_ProjectHeader set PostToResult = Null where id = " & Me.ID, dbSeeChanges
            End If
            
            GoSub postsub
         ElseIf lngChoice = vbbt1 Then
            ' set as skip
            CurrentDb.Execute "update dbo_ProjectHeader set PostToResult = 'Skip',PostToAccDate = Date() where id = " & Me.ID, dbSeeChanges
            Me.Requery
            'Me.PostToResultD.Requery
            
         End If
         
      Case Else
         MsgBox "Cannot automatic post", , "Unable to post"
         
   End Select
   
   On Error Resume Next
   
   SageEmployee.CloseDatabase: ' Set SageEmployee = Nothing
   SageCustomer.CloseDatabase: ' Set SageCustomer = Nothing
   SageInvoice.CloseDatabase:  ' Set SageInvoice = Nothing
   
   Exit Sub
   
postsub:

   strSQL = "select * from qryProjectSage where ID = " & Me.ID
   Set rst = CurrentDb.OpenRecordset(strSQL, dbOpenDynaset, dbSeeChanges)
   Call PostInvoices(rst)
   
   rst.Close
   Set rst = Nothing
   
   Return
   
   
End Sub

Private Sub cmdSearch_Click()


Dim strWhere As String
Dim tmpdatestart As Date
Dim tmpdateend As Date


Application.Echo False ' - allow updates while debugging

Dim strSQL As String

strSQL = "select qryProjectSage.*, GetComponentNames([ID]) AS myComponentName from qryProjectSage " & _
                  "where ( (PostToAcc = True) and ((PostToResult is null) or (PostToResult = 'Ready for Repost') )  ) " & _
                  "or    ( (PostToAccDate >=  Date()) ) " & _
                  "ORDER BY [InvoiceDate] DESC"


Me.RecordSource = strSQL





strWhere = ""
'create filter criteria

If Nz(Me.txtDateStart, "") = "" Then tmpdatestart = "01/01/1900" Else tmpdatestart = Me.txtDateStart
If Nz(Me.txtDateEnd, "") = "" Then tmpdateend = "12/31/2199" Else tmpdateend = Me.txtDateEnd


' Show Invoices Only
    
      strWhere = strWhere & "(NZ([InvoiceNum],0) > 0)  AND"
      
      strWhere = strWhere & " ([InvoiceDate] >= " & Format$(tmpdatestart, "\#mm\/dd\/yyyy\#") & ") AND"
      strWhere = strWhere & " ([InvoiceDate] <= " & Format$(tmpdateend, "\#mm\/dd\/yyyy\#") & ") AND"


If Len(Me.txtDocketNum) <> 0 Then
    strWhere = strWhere & "[DocketNum] = " & Me.txtDocketNum & " AND"
End If

If Len(Me.txtInvoiceNum) <> 0 Then
    strWhere = strWhere & "[InvoiceNum] = " & Me.txtInvoiceNum & " AND"
Else
      strWhere = strWhere & " ([PostToAcc] = True) AND"
End If



If Len(Me.txtCompanyID) <> 0 Then
    
    
    mycomp = Me.txtCompanyID
    

    done = False
        
    Do Until done = True 'create as many criteria as there are spaces
        mymark = InStr(Trim(mycomp), " ")
        
        If mymark = 0 Then done = True
        If mymark = 0 Then mymark = Len(mycomp)
        
        myComp1 = Trim(Left(mycomp, mymark))
        mycomp = Right(mycomp, Len(mycomp) - mymark)
        strWhere = strWhere & "[COMPNAME] like '*" & myComp1 & "*' AND"
    Loop
    Else
End If


If Len(Me.txtProjectName) <> 0 Then
    strWhere = strWhere & "[ProjectName] like '*" & Me.txtProjectName & "*' AND"
End If


If Len(Me.txtOrderedBy) <> 0 Then
    strWhere = strWhere & "[OrderedBy] like '*" & Me.txtOrderedBy & "*' AND"
End If

If Len(Me.txtPONumber) <> 0 Then
    strWhere = strWhere & "[PONumber] like '*" & Me.txtPONumber & "*' AND"
End If

If Len(Me.txtSalesRepID) <> 0 Then
    strWhere = strWhere & "[SalesRepID] = " & Me.txtSalesRepID & " AND"
End If



