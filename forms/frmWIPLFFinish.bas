Private Sub Form_Load()
'Set Up Location Radio Buttons
Dim myLoc As Recordset
Set myLoc = CurrentDb.OpenRecordset("dbo_CompanyLocation", dbOpenSnapshot)
myLoc.MoveFirst

For i = 1 To 9
    myradiobut = "Loc" & Trim(i)
    mylabel = "lblLoc" & Trim(i)
    If Nz(myLoc!LocationCode, "") <> "" Then
        Me(myradiobut).enabled = True
        Me(mylabel).caption = myLoc!LocationCode
        Me(mylabel).BackColor = myLoc!ReportColor
        Me(myradiobut).visible = True
        Me(mylabel).visible = True
    Else
        Me(myradiobut).enabled = False
        Me(myradiobut).visible = False
        Me(mylabel).visible = False
    End If
    myLoc.MoveNext
Next i
    
Set myLoc = Nothing
End Sub

