Private Sub cmdFilter_Click()


myfilter = "WHERE 1 + 1 = 2 AND"



'MsgBox "Show only: " & Me.chkShowOnlyReady

If Nz(Me.frmShowOnly, 0) <> 0 Then
    'set the filter by the LFFinish work to be done.
    Select Case Me.frmShowOnly
    
    Case 1 'FL Cut
        myfilter = myfilter & " [MinOfHasCutter] = True AND"
    
    Case 2 'RL Cut
        myfilter = myfilter & " [MinOfHasRollCutter] = true AND"
    
    Case 3 'Router
        myfilter = myfilter & " [MinOfHasRouter] = true AND"
    
    Case 4 'Lam
        myfilter = myfilter & " [MinOfHasLaminate] = true AND"
        
    Case 5 'Drill
        myfilter = myfilter & " [MinOfHasDrill] = true AND"
        
    Case 6 'Tape
        myfilter = myfilter & " [MinOfHasTaping] = true AND"
        
    Case 7 'Kit
        myfilter = myfilter & " [MinOfHasKitting] = true AND"
        
    Case 8 'HEm
        myfilter = myfilter & " [MinOfHasSewnHem] = true AND"
        
    Case 9 'Other
        myfilter = myfilter & " [MinOfHasOthersrv] = true AND"
  
  End Select
End If


'Set up which plant to view - default to all (99)
' If Me.frmPlant = 99 then show all - no line required
If Me.frmPlant >= 1 And Me.frmPlant <= 9 Then
     myfilter = myfilter & " [CompanyLocationID] = " & Me.frmPlant & " AND"
End If

If Len(myfilter) > 5 Then myfilter = Left(myfilter, Len(myfilter) - 4)


mysql = "SELECT * FROM qryRptWIP_LFF_Summary " & myfilter


Me.rptWIPBindery.Report.RecordSource = mysql



Application.Echo True
End Sub

