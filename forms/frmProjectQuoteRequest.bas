Private Sub cmdAttachments_Click()
Dim strPath As String
Dim varItem As Variant
Dim filePicker As FileDialog

Set filePicker = Application.FileDialog(msoFileDialogFilePicker)

strPath = DLookup("FolderEstimate", "dbo_Company", "[id] = " & 1)

'add files & eNumber
strPath = strPath & "\Files\E" & Format(Me.QuoteNum, "000000")

'Create directory if it doesn't already exist
If Dir(strPath, vbDirectory) = "" Then
    MkDir (strPath)
End If



With filePicker
   .AllowMultiSelect = False
    .InitialView = msoFileDialogViewList
    .InitialFileName = strPath & "\"
    .ButtonName = "Paste or Drag Files in, Click CANCEL to close =>"
    .Title = "Estimate E" & Format(Me.QuoteNum, "000000") & " Files"
    .Filters.Clear
    .Show
End With


Dim FSO As Object
Dim fldr As Object
Dim lngCount As Long

Set FSO = CreateObject("Scripting.FileSystemObject")
Set fldr = FSO.getfolder(strPath)
folderfilecount = fldr.Files.Count
    
Me.cmdAttachments.caption = "Attachments (" & Trim(folderfilecount) & ")"

    If Me.Dirty Then Me.Dirty = False


End Sub

