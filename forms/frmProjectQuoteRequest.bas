Private Sub cmdSubmit_Click()
Dim myrs As Recordset

'check to see if they have picked a destination for the quote
If Nz(Me.EstimateTo, 0) = 0 Then
    MsgBox "You need to select where to send this quote from the box above. Request is NOT submitted.", vbCritical, ProgramName
    Exit Sub
End If

'add history and date stamp the quote request onscreen.
AddContactHistory Nz(Me.ContactGeneralID), , Me.BillOrderedBy, Nz(Parent.ProjectID), IIf(Parent.ProjectID, 3, 2), "Request for Quote Sent to Estimating: " & mySubject
Me.QuoteSubmittedBy = cLoginID
Me.QuoteSubmittedWhen = Now()
Me.ProjectStatusID = 1

mysql = "SELECT * from dbo_ProjectQuoteRequestDetail WHERE [ProjectHeaderID] = " & Me.ID

Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
Do Until myrs.EOF
    myrs.Edit
    myrs!Locked = True
    myrs.Update
    myrs.MoveNext
Loop

If Me.Dirty = True Then Me.Dirty = False

MsgBox "You have submitted this quote to Estimating.", vbInformation, ProgramName

