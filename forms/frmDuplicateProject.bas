DoCmd.Close

End Sub

Private Sub cmdProceed_Click()

Dim rstIn As Recordset
Dim rstOut As Recordset

Dim sqlIn As String
Dim sqlOut As String
Dim myQuoteNum As Double

Dim myPreviousDocketDate As Date
Dim myPreviousDocketNum As Integer


myResponse = vbNo
myResponse = MsgBox("Run Duplicate Docket procedure. Are You Sure?", vbYesNo + vbDefaultButton2, ProgramName)

If myResponse = vbNo Then Exit Sub


'Questions to ask?
    'Do you want to repeat all components of this project, or only certain ones? Identify the components to duplicate.
    
    
'Duplicate Header
    sqlIn = "SELECT * from dbo_ProjectHeader WHERE [ID] = " & Me.ID
    sqlOut = "Select * from dbo_ProjectHeader"
    
    Set rstIn = CurrentDb.OpenRecordset(sqlIn, dbOpenSnapshot)
    Set rstOut = CurrentDb.OpenRecordset(sqlOut, dbOpenDynaset, dbSeeChanges)
    
    rstOut.AddNew
    
    For Each fld In rstIn.Fields
        'bypass ID fields as it will be autonumbered
        If fld.name <> "ID" And fld.name <> "SSMA_TimeStamp" Then
        
            rstOut(fld.name) = rstIn(fld.name)
        End If
        
        
        If fld.name = "ProjectName" Then
            If Me.frmReason = 1 Then
                'rstOut(fld.Name) = "DUPE - " & rstIn(fld.Name) & ""
            Else
                'rstOut(fld.Name) = "RERUN - " & rstIn(fld.Name) & ""
            End If
            
        End If
        
            
        
        
    Next fld
    

    
    'Remove Existing estimate/docket/invoice nums and dates
    
    'get next quote number
    Dim NewEstimate As Recordset
    Set NewEstimate = CurrentDb.OpenRecordset("dbo_NextNumber", dbOpenDynaset, dbSeeChanges)
    myQuoteNum = Val(NewEstimate!NextEstimateNum)
    NewEstimate.Edit
    NewEstimate!NextEstimateNum = myQuoteNum + 1
    NewEstimate.Update
    NewEstimate.Close
    
    rstOut!QuoteNum = myQuoteNum
    rstOut!QuoteDate = Date
    
    'Get rid of previous docket and invoice numbers
    rstOut!DocketNum = Null
    rstOut!InvoiceNum = Null
    rstOut!DocketDate = Null
    rstOut!InvoiceDate = Null
    
    'Set last edited / created , by whom
    rstOut!LastEditedBy = Forms!frmLogin!ID
    rstOut!LastEdited = Now()
    rstOut!CreatedBy = Forms!frmLogin!ID
    rstOut!Created = Now()
    rstOut!AssignedDocketDueDate = Null
    rstOut!SpecifiedDocketDueDate = Null
    rstOut!DocketArtworkExpectedDate = Null
    rstOut!DocketDataExpectedDate = Null
    
    rstOut!PostToAcc = 0
    rstOut!PostToAccDate = Null
    rstOut!PostToResult = Null

    
    
    'Make sure new one is not voided
    rstOut!VOID = False
    
    'Set new project status
    rstOut!ProjectStatusID = 11
    
    'Clean off old notes
    rstOut!ProjectNotes = Null
    
    'Grab PreviousDocket Info
    myPrevDocketDate = rstIn!DocketDate
    myPrevDocketNum = rstIn!DocketNum
    
    'Clean off quote request dates
    rstOut!QuoteSubmittedWhen = Null
    rstOut!QuoteSubmittedBy = Null
    rstOut!EstimateTo = Null
    rstOut!EstimatorID = Null
    rstOut!EstimateSentByID = Null
    rstOut!EstimateSent = Null
    rstOut!LastContact = Null
    rstOut!LastContactByID = Null
    rstOut!LastContactMethod = Null
    rstOut!LostEstimateReason = Null
    rstOut!CODPaidYN = Null
    rstOut!CODPaidDate1 = Null
    rstOut!OriginalDueDate = Null
    rstOut!CODPaidAmt1 = Null
    

    
