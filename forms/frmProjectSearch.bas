Private Sub cmdSearch_Click()


Dim strWhere As String
Dim tmpdatestart As Date
Dim tmpdateend As Date


'Application.Echo False ' - allow updates while debugging



strWhere = ""
'create filter criteria

If Nz(Me.txtDateStart, "") = "" Then tmpdatestart = "01/01/1900" Else tmpdatestart = Me.txtDateStart
If Nz(Me.txtDateEnd, "") = "" Then tmpdateend = "12/31/2199" Else tmpdateend = Me.txtDateEnd



Select Case Me.SearchWhat 'Sets string for type of project and sets date criteria
    Case 1 ' show All
        

        strWhere = strWhere & " (([QuoteDate] >= " & Format$(tmpdatestart, "\#mm\/dd\/yyyy\#") & ") AND"
        strWhere = strWhere & " ([QuoteDate] <= " & Format$(tmpdateend, "\#mm\/dd\/yyyy\#") & ") OR"

        strWhere = strWhere & " ([DocketDate] >= " & Format$(tmpdatestart, "\#mm\/dd\/yyyy\#") & ") AND"
        strWhere = strWhere & " ([DocketDate] <= " & Format$(tmpdateend, "\#mm\/dd\/yyyy\#") & ") OR"
        
        strWhere = strWhere & " ([InvoiceDate] >= " & Format$(tmpdatestart, "\#mm\/dd\/yyyy\#") & ") AND"
        strWhere = strWhere & " ([InvoiceDate] <= " & Format$(tmpdateend, "\#mm\/dd\/yyyy\#") & ")) AND"
    
    
    Case 2 ' Show Estimates Only
    
        strWhere = strWhere & "(NZ([QuoteNum],0) > 0 AND NZ([DocketNum],0) = 0)  AND"
        
        strWhere = strWhere & " ([QuoteDate] >= " & Format$(tmpdatestart, "\#mm\/dd\/yyyy\#") & ") AND"
        strWhere = strWhere & " ([QuoteDate] <= " & Format$(tmpdateend, "\#mm\/dd\/yyyy\#") & ") AND"
        
    Case 3 ' Show Dockets Only

        strWhere = strWhere & "(NZ([DocketNum],0) > 0 AND NZ([InvoiceNum],0) = 0)  AND"
        
        strWhere = strWhere & " ([DocketDate] >= " & Format$(tmpdatestart, "\#mm\/dd\/yyyy\#") & ") AND"
        strWhere = strWhere & " ([DocketDate] <= " & Format$(tmpdateend, "\#mm\/dd\/yyyy\#") & ") AND"
    
    Case 4 ' Show Invoices Only
    
        strWhere = strWhere & "(NZ([InvoiceNum],0) > 0)  AND"
        
        strWhere = strWhere & " ([InvoiceDate] >= " & Format$(tmpdatestart, "\#mm\/dd\/yyyy\#") & ") AND"
        strWhere = strWhere & " ([InvoiceDate] <= " & Format$(tmpdateend, "\#mm\/dd\/yyyy\#") & ") AND"

End Select

    

If Len(Me.txtQuoteNum) <> 0 Then
    strWhere = strWhere & "[QuoteNum] = " & Me.txtQuoteNum & " AND"
End If



If Len(Me.txtDocketNum) <> 0 Then
    strWhere = strWhere & "[DocketNum] = " & Me.txtDocketNum & " AND"
End If

If Len(Me.txtInvoiceNum) <> 0 Then
    strWhere = strWhere & "[InvoiceNum] = " & Me.txtInvoiceNum & " AND"
End If



If Len(Me.txtCompanyID) <> 0 Then
    Me.txtComponentName.enabled = True
    
    mycomp = Me.txtCompanyID
    

    done = False
        
    Do Until done = True 'create as many criteria as there are spaces
        mymark = InStr(Trim(mycomp), " ")
        
        If mymark = 0 Then done = True
        If mymark = 0 Then mymark = Len(mycomp)
        
        myComp1 = Trim(Left(mycomp, mymark))
        mycomp = Right(mycomp, Len(mycomp) - mymark)
        strWhere = strWhere & "[COMPNAME] like '*" & myComp1 & "*' AND"
    Loop
    Else
        Me.txtComponentName = ""
        Me.txtComponentName.enabled = False
End If


If Len(Me.txtProjectName) <> 0 Then
    strWhere = strWhere & "[ProjectName] like '*" & Me.txtProjectName & "*' AND"
End If

If Len(Me.txtComponentName) <> 0 Then
    strWhere = strWhere & "[myComponentName] like '*" & Me.txtComponentName & "*' AND"
End If

If Len(Me.txtOrderedBy) <> 0 Then
    strWhere = strWhere & "[OrderedBy] like '*" & Me.txtOrderedBy & "*' AND"
End If

If Len(Me.txtPONumber) <> 0 Then
    strWhere = strWhere & "[PONumber] like '*" & Me.txtPONumber & "*' AND"
End If

If Len(Me.txtSalesRepID) <> 0 Then
    strWhere = strWhere & "[SalesRepID] = " & Me.txtSalesRepID & " AND"
End If

If Len(Me.txtCsrID) <> 0 Then
    strWhere = strWhere & "[CsrID] = " & Me.txtCsrID & " AND"
End If


'If Len(Me.txtLN) <> 0 Then
'    strWhere = strWhere & "[LN] like '*" & Me.txtLN & "*' AND"
'End If

'If Len(Me.txtTitle) <> 0 Then
'    strWhere = strWhere & "[TITLE] like '*" & Me.txtTitle & "*' AND"
'End If

'If Len(Me.txtPhone) <> 0 Then
'    strWhere = strWhere & "[PhDirect] like '*" & Me.txtPhone & "*' AND"
'End If

'If Len(Me.txtEmail) <> 0 Then
'    strWhere = strWhere & "[Email] like '*" & Me.txtEmail & "*' AND"
'End If

'If Len(Me.txtSalesRepID) <> 0 Then
'    strWhere = strWhere & "[SalesRepID] = " & Me.txtSalesRepID & " AND"
'End If

'If Me.chkIsActive = True Then
'    strWhere = strWhere & "[IsActive] = TRUE and"
'End If

'If Me.chkClient = True Then
'    strWhere = strWhere & "[isclient] = TRUE and"
'End If

'If Me.chkVendor = True Then
'    strWhere = strWhere & "[isvendor] = TRUE and"
'End If

'If Me.chkCompetitor = True Then
'    strWhere = strWhere & "[iscompetitor] = TRUE and"
'End If

If Len([strWhere]) >= 4 Then
    strWhere = Left([strWhere], Len([strWhere]) - 3)
End If

'APPLY FILTER

Me.Filter = strWhere
Me.FilterOn = True
If Len([strWhere]) = 0 Then Me.FilterOn = False


'Following two lines are required to ensure the entire database loads and doesn't leave tables in "SUSPEND" on SQL server.
If Me.Recordset.RecordCount > 0 Then
    Me.Recordset.MoveLast
    Me.Recordset.MoveFirst
End If




'Application.Echo True

End Sub



Private Sub cmdSelect_Click()

'GRAB Project ID
Dim tmpProjectID As Long

tmpProjectID = Me!ID

Call OpenProject(tmpProjectID)

'Bypass balance of code -now calls public function
Exit Sub



tmpControlNum = 0
tmpcontrolnumavail = 0


'skip by the 5 tab layout

GoTo skip5layout:

'FIND OUT IF project IS ALREADY OPEN - IF IT IS, KILL IT, so it will reappear at top.


For i = 1 To 5
    strcontrolname = "navProject" & Format(i, "00")
    If Val(Forms!navmaster(strcontrolname).tag) = Val(tmpProjectID) Then
        Forms!navmaster(strcontrolname).tag = ""
        Forms!navmaster(strcontrolname).caption = Forms!navmaster(strcontrolname).name
        Forms!navmaster(strcontrolname).visible = False
        
        
        For j = i To 4 'if you delete one, suck them up from below
        strcontrolname1 = "navProject" & Format(j, "00")
        strcontrolname2 = "navProject" & Format(j + 1, "00")
        
        Forms!navmaster(strcontrolname1).visible = Forms!navmaster(strcontrolname2).visible
        Forms!navmaster(strcontrolname1).tag = Forms!navmaster(strcontrolname2).tag
        Forms!navmaster(strcontrolname1).caption = Forms!navmaster(strcontrolname2).caption
        Forms!navmaster(strcontrolname1).NavigationWhereClause = Forms!navmaster(strcontrolname2).NavigationWhereClause
        Next
        

    
        
    End If
Next


 ' bump everybody down one onscreen.

    For i = 5 To 2 Step -1
        strcontrolname1 = "navProject" & Format(i, "00")
        strcontrolname2 = "navProject" & Format(i - 1, "00")
        
        Forms!navmaster(strcontrolname1).visible = Forms!navmaster(strcontrolname2).visible
        Forms!navmaster(strcontrolname1).tag = Forms!navmaster(strcontrolname2).tag
        Forms!navmaster(strcontrolname1).caption = Forms!navmaster(strcontrolname2).caption
        Forms!navmaster(strcontrolname1).NavigationWhereClause = Forms!navmaster(strcontrolname2).NavigationWhereClause

        
    Next


skip5layout:

mytab = "" 'set up the tab information.
If Me.InvoiceNum > 0 Then
    mytab = "INV " & Me.InvoiceNum
    Else
        If Me.DocketNum > 0 Then
            mytab = "DKT " & Me.DocketNum
        Else
            mytab = "EST " & Me.QuoteNum
    End If
End If
mytab = mytab & vbCrLf
mytab = mytab & Trim(Left(Me.CompName, 15)) & IIf(Len(Me.CompName) > 15, "...", "") & vbCrLf
mytab = mytab & Trim(Left(Me.ProjectName, 15)) & IIf(Len(Me.ProjectName) > 15, "...", "")




' go to the first tab and turn it on.

tmpControlNum = 1
strcontrolname = "navProject" & Format(tmpControlNum, "00")
Forms!navmaster(strcontrolname).visible = True
Forms!navmaster(strcontrolname).caption = mytab
Forms!navmaster(strcontrolname).tag = tmpProjectID


tmpWhereClause = "[id] = " & Val(tmpProjectID)

Forms!navmaster(strcontrolname).NavigationWhereClause = tmpWhereClause




DoCmd.BrowseTo acBrowseToForm, "navProject", , tmpWhereClause

'Forms!navmaster(strcontrolname).Form!NavProject!ContactGeneralID.SetFocus 'does this work to start the ribbon? - nope.


End Sub

