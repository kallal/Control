Private Sub cmdEdit_Click()
If Me!cmdEdit.caption = "LOCKED" Then
    Me!cmdEdit.caption = "EDITABLE"
    Me!cmdEdit.BackColor = vbGreen
    
    On Error Resume Next
    AddContactHistory Me.ContactGeneralID, , Me.ID, , 1, "Unlocked Contact Name Page for Editing"
    AddContactHistory
    
    Dim ctl As control

    For Each ctl In Me.Controls
        If ctl.tag = "L" Then
            ctl.enabled = True
            ctl.Locked = False

            
        End If
    Next
Else

    
    Call Form_Current
End If

    
End Sub

