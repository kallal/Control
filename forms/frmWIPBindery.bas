Private Sub cmdFilter_Click()


myfilter = "WHERE 1=1 AND"

'MsgBox "Show only: " & Me.chkShowOnlyReady

If Nz(Me.frmShowOnly, 0) <> 0 Then
    'set the filter by the bindery work to be done.
    Select Case Me.frmShowOnly
    
    Case 1 'Trim
        myfilter = myfilter & " [IsTrim] = True AND"
    
    Case 2 'Pad
        myfilter = myfilter & " [PadInYN] = true AND"
    
    Case 3 'Drill
        myfilter = myfilter & " [DrillYN] = true AND"
    
    Case 4 'Score
        myfilter = myfilter & " [IsScore] = true AND"
        
    Case 5 'Fold
        myfilter = myfilter & " [FoldYN] = true AND"
        
    Case 6 'Saddle Stitch
        myfilter = myfilter & " [BindSaddleYN] = true AND"
        
    Case 7 'Perfect Bind
        myfilter = myfilter & " [BindPerfectYN] = true AND"
        
    Case 8 'Coil Bind
        myfilter = myfilter & " [BindCoilYN] = true AND"
        
    Case 9 'Wire-O
        myfilter = myfilter & " [BindWireOYN] = true AND"
  
          
    Case 10 'Is UV
        myfilter = myfilter & " [IsUV] = true AND"
        
                
    Case 11 'Lam
        myfilter = myfilter & " [IsLam] = true AND"
        
                
    Case 12 'Shrinkwrap
        myfilter = myfilter & " [IsSW] = true AND"
  
    Case 13 'oTHER
        myfilter = myfilter & " [IsOther] = true AND"
        
  End Select
End If

'Set up which plant to view - default to all (99)
' If Me.frmPlant = 99 then show all - no line required
If Me.frmPlant >= 1 And Me.frmPlant <= 9 Then
     myfilter = myfilter & " [CompanyLocationID] = " & Me.frmPlant & " AND"
End If

If Len(myfilter) > 4 Then myfilter = Left(myfilter, Len(myfilter) - 4)


mysql = "SELECT * FROM qryScheduleBindery " & myfilter


Me.rptWIPBindery.Report.RecordSource = mysql



Application.Echo True
End Sub

