Private Sub cmdRunFilter_Click()
Criteria = ""
If Len(Me.filterCategoryID) > 0 Then Criteria = Criteria & "CategoryID = " & Me.filterCategoryID & " and "
'If Len(Me.filterType) > 0 Then Criteria = Criteria & "Type = """ & Me.filterType & """ and "
If Len(Me.filterLocationID) > 0 Then Criteria = Criteria & "LocationID = " & Me.filterLocationID & " and "
If Len(Me.filterMachineTypeID) > 0 Then Criteria = Criteria & "MachineTypeID = " & Me.filterMachineTypeID & " and "
If Len(Me.filterVendorID) > 0 Then Criteria = Criteria & "(Vendor1ID = " & Me.filterVendorID & " or Vendor2ID = " & Me.filterVendorID & " or Vendor3ID = " & Me.filterVendorID & ") and "

If Len(Me.filterDescription) > 0 Then
    myDesc = Me.filterDescription
    

    done = False
        
    Do Until done = True 'create as many criteria as there are spaces
        mymark = InStr(Trim(myDesc), " ")
        
        If mymark = 0 Then done = True
        If mymark = 0 Then mymark = Len(myDesc)
        
        mydesc1 = Trim(Left(myDesc, mymark))
        myDesc = Right(myDesc, Len(myDesc) - mymark)
        Criteria = Criteria & "[DESCRIPTION] like '*" & mydesc1 & "*' AND "
    Loop
    
 
End If

If Len(Me.filterCode) > 0 Then
    Criteria = Criteria & "[CODE] like '*" & Me.filterCode & "*' AND "
End If




If Len(Criteria) < 1 Then
    Me.FilterOn = False
Else
    Criteria = Left(Criteria, Len(Criteria) - 5)
    Me.Filter = Criteria
    Me.FilterOn = True
End If


End Sub

