Private Sub txtLockStatus_Click()
If cLockQuoteRequestsYN = True Then
    If [Locked] = True Then
        If cCanUnlockQuoteRequestsYN = True Then
            myResponse = ""
            myResponse = InputBox("Unlocking this quote request. You must enter a note as to the reason for unlocking this request.", ProgramName)
            If myResponse = "" Then
                MsgBox "Canceling - this request remains locked. You must enter a reason for removing the lock.", vbCritical, ProgramName
                Exit Sub
            End If
            
            [Locked] = False
            AddContactHistory Me.ContactGeneralID, , , Me.ProjectHeaderID, 2, "Quote Request " & Me.ComponentName & " unlocked by " & cCurrentUserName & " - " & myResponse

        Else
            MsgBox "You do not have permission to unlock a previously locked quote request. Please request that your estimator unlock this quote request.", vbCritical, ProgramName
        End If
        
    
    End If
    
End If
 Me.Requery
 
End Sub

