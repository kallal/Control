Private Sub cmdClose_Click()

If Len(Nz(Me.ToDo, "")) = 0 Then
    MsgBox "You can't save a To-Do without typing something... Either complete the To-Do or Delete it.", vbCritical, ProgramName
    Exit Sub
End If

DoCmd.Close

End Sub

Private Sub cmdCompleted_Click()
ans = MsgBox("Mark this task as completed. Are you sure?", vbYesNo, "Axis MIS")
If ans = vbYes Then
    Me.CompletedBy = cLoginID
    
    Me.CompletedWhen = Now()
End If

End Sub

Private Sub Command34_Click()
a = Forms!navmaster.Form!navSubform!ProjectHeaderID
MsgBox a
MsgBox again

End Sub



Private Sub cmdDeleteToDo_Click()
If Nz(Me.CompletedBy, 0) > 0 Then
    MsgBox "You can't delete a To-Do that has been completed.", vbCritical, ProgramName
    Exit Sub
End If

myid = Me!CreatedByID
If Val(myid) <> Val(cLoginID) Then
    MsgBox "You can't delete a To-Do that has been created by another user.", vbCritical, ProgramName
    Exit Sub
End If

DoCmd.RunCommand acCmdSelectRecord
DoCmd.RunCommand acCmdDeleteRecord
DoCmd.Close

End Sub

Private Sub Form_Close()
Forms!frmDashboard.Refresh

End Sub

Private Sub txtNotetoAdd_AfterUpdate()

If Me.txtNoteToAdd = Null Or Me.txtNoteToAdd = "" Then
    Exit Sub
End If

Me.Notes = Now() & " : " & rsLocSS![FN] & " " & rsLocSS![LN] & " : " & Me.txtNoteToAdd & vbCrLf & Me.Notes

Me.txtNoteToAdd = ""
DoCmd.GoToControl "txtNoteToAdd"

End Sub

Private Sub txtProjectComponentHeaderID_Click()
If Nz(Me!ProjectHeaderID, 0) > 0 Then
    myid = Me!ProjectHeaderID
  
    tmpWhereClause = "[ID] = " & Me!ProjectComponentHeaderID
    DoCmd.Close
    
    Call OpenProject(myid)
      
        'then move to the component.
    DoCmd.BrowseTo acBrowseToForm, "frmProjectComponentHeader", "navMaster.navSubform>navProject.navProject", tmpWhereClause
    
End If
End Sub

Private Sub txtProjectHeaderID_Click()
If Nz(Me!ProjectHeaderID, 0) > 0 Then
    myid = Me!ProjectHeaderID

    DoCmd.Close
    
    Call OpenProject(myid)
End If

End Sub