myCSRID = DLookup("[CSRID]", "dbo_projectHeader", "[ID] = " & Me.ProjectHeaderID)


If Nz(mySalesRepID, 0) > 0 Then
    tmpFN = DLookup("[FN]", "dbo_CompanyEmployee", "[id] = " & mySalesRepID)
    tmpLN = DLookup("[LN]", "dbo_CompanyEmployee", "[id] = " & mySalesRepID)
    tmpEmail = DLookup("[Email]", "dbo_CompanyEmployee", "[id] = " & mySalesRepID)
    
    mySalesRepEmail = tmpFN & " " & tmpLN & " <" & tmpEmail & ">"
    mySalesRepSignature = DLookup("[EmailSignature]", "dbo_CompanyEmployee", "[id] = " & mySalesRepID)
    
End If

If Nz(myCSRID, 0) > 0 Then
    tmpFN = DLookup("[FN]", "dbo_CompanyEmployee", "[id] = " & myCSRID)
    tmpLN = DLookup("[LN]", "dbo_CompanyEmployee", "[id] = " & myCSRID)
    tmpEmail = DLookup("[Email]", "dbo_CompanyEmployee", "[id] = " & myCSRID)
    
    myCSREmail = tmpFN & " " & tmpLN & " <" & tmpEmail & ">"
    myCSRSignature = DLookup("[EmailSignature]", "dbo_CompanyEmployee", "[id] = " & myCSRID)
End If


myEmail = IIf(Hour(Now()) < 12, "Good morning ", "Good afternoon ") & myrecipientpreferredname & "," & vbCrLf & vbCrLf

myEmail = myEmail & DLookup("[Email]", "dbo_EmailTemplate", "[Description] = 'Proof'") & vbCrLf & vbCrLf

'Figure out the sendfrom
mySendFromID = DLookup("SendProofsFrom", "dbo_Company", "ID = 1")

'Figure out who to put as sender
Select Case mySendFromID
    Case 1 'Send from is Sales Rep
        mySender = mySalesRepEmail
        myEmail = myEmail & mySalesRepSignature & vbCrLf & vbCrLf
    Case 2
        mySender = myCSREmail
        myEmail = myEmail & mySalesRepSignature & vbCrLf & vbCrLf
    Case 3
        mySender = DLookup("SendProofsFromAccount", "dbo_Company", "ID = 1")
End Select


myEmail = myEmail & "--------------------------------------------------" & vbCrLf & vbCrLf

myEmail = myEmail & "Please click on the link(s) below to view your proof(s)." & vbCrLf & vbCrLf




    
    'Add Proof URL to Email
    myProofHTTPServerURL = DLookup("[ProofHTTPServerURL]", "dbo_Company", "[ID] = 1")
    myAddress = "http://" & myProofHTTPServerURL & "/" & Right(Me.FileName, Len(Me.FileName) - InStrRev(Me.FileName, "\"))
   
