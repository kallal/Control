Private Sub cmdAttachFile_Click()

Dim strPathIn As String
Dim strPathOut As String
Dim strDocketNum As String
Dim strComponentNum As String
Dim strFileNameOut As String

strPathIn = "C:\"

strPathOut = cControlProofFolder


strDocketNum = "D" & Trim(DLookup("[DocketNum]", "dbo_projectHeader", "[ID] = " & Me.ProjectHeaderID))



strComponentNum = "C" & Trim(Me.ProjectComponentID) & "ID" & Trim(Me.ID)


strFileNameOut = "P" & Format(Me.ProofRevision + 1, "00")



With Application.FileDialog(msoFileDialogFilePicker)

'Select only one file per proof
.AllowMultiSelect = False

'Set the dialog title
.Title = "Locate the Proof to Attach to this Component"

'Set the button caption
.ButtonName = "Choose"

'Clear the Filter list
.Filters.Clear

'Filter for only PDF files
.Filters.Add "PDF", "*.pdf"
.Filters.Add "ZIP", "*.zip"

'set filter index
.FilterIndex = 1

'Set initial path name
.InitialFileName = strPathIn

'Show files as thumbnails
.InitialView = msoFileDialogViewThumbnail

'Show the dialog and test the return
If .Show = 0 Then
    MsgBox "No File Selected - Proof Attachment is Cancelled", vbcrital, ProgramName
    
Else
    'Create Randomized add-on (10 chars)
    tAddOnChar = ""
    For ta = 1 To 10
        SetA = Int(Rnd() * 2)
        If SetA = 1 Then
            SetA = 65
        Else
            SetA = 97
        End If
        
        Setb = Int(Rnd() * 26)
        
        tChar = SetA + Setb
        tAddOnChar = tAddOnChar & Chr(tChar)
     Next ta
    
    
    myExtension = Right(.SelectedItems(1), 3)
        
    strFileNameOut = strFileNameOut & tAddOnChar & "." & myExtension

    
    
    MsgBox "Selected file named: " & .SelectedItems(1)
    
    'Update the proof count.
    
    Me.ProofRevision = Me.ProofRevision + 1
    
    OutFile = Trim(strPathOut) & "\" & Trim(strDocketNum) & Trim(strComponentNum) & Trim(strFileNameOut)
    
    'On Error Resume Next
    'MkDir Trim(strPathOut) & Trim(strDocketNum)
    'On Error GoTo 0
    
    Me.FileName = OutFile
    
    'Show only actual filename without path for FriendlyName - Link will be to value in me.filename
    tFriendlyName = Right(.SelectedItems(1), Len(.SelectedItems(1)) - InStrRev(.SelectedItems(1), "\"))
    
    
    Me.FriendlyName = tFriendlyName
    
    Me.CreatedBy = cLoginID
    Me.Created = Now()
    
    
    FileCopy .SelectedItems(1), OutFile
    
    Dim myContactGeneralID As Integer
    Dim mylocationid As Integer
    Dim mycontactnameid As Integer
    
    
    
    myContactGeneralID = DLookup("[ContactGeneralID]", "dbo_ProjectHeader", "[ID] = " & Me.ProjectHeaderID)
    mylocationid = DLookup("[BillAddLocationID]", "dbo_ProjectHeader", "[ID] = " & Me.ProjectHeaderID)
    mycontactnameid = DLookup("[BillOrderedBy]", "dbo_ProjectHeader", "[ID] = " & Me.ProjectHeaderID)


AddContactHistory myContactGeneralID, mylocationid, mycontactnameid, ProjectHeaderID, 3, "Added or Replaced Proof # " & strComponentNum
   


End If



End With
If Me.Dirty Then Me.Dirty = False
End Sub



