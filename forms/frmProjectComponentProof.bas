Private Sub cmdProofID_Click()




Dim myRecipientFullEmail As String
Dim mySubject As String
Dim myEmail As String
Dim mySender As String
Dim myCC As String

'Build Email

If Me.Dirty Then Me.Dirty = False

myProjectID = Me.ProjectHeaderID

myDocketnum = DLookup("[DocketNum]", "dbo_ProjectHeader", "[ID] = " & Me.ProjectHeaderID)

myRecipientID = Nz(DLookup("[ProofToID]", "dbo_ProjectHeader", "[ID] = " & Me.ProjectHeaderID), 0)

If myRecipientID = 0 Then 'No Proofer found - use the ordered by.
    myRecipientID = DLookup("[BillOrderedBy]", "dbo_ProjectHeader", "[ID] = " & Me.ProjectHeaderID)
End If


myProjectName = DLookup("[ProjectName]", "dbo_ProjectHeader", "[ID] = " & Me.ProjectHeaderID)

mySubject = "D" & Trim(myDocketnum) & " : " & myProjectName & " - PROOFS (Response is Required)"

'Following is from contact name email

myrecipientFN = DLookup("[FN]", "dbo_Contactname", "[id] = " & myRecipientID)
myRecipientLN = DLookup("[LN]", "dbo_Contactname", "[id] = " & myRecipientID)
myrecipientemail = DLookup("[Email]", "dbo_Contactname", "[id] = " & myRecipientID)
myrecipientpreferredname = DLookup("[PreferredName]", "dbo_Contactname", "[id] = " & myRecipientID)

If myrecipientpreferredname = "" Then

    myrecipientpreferredname = DLookup("[FN]", "dbo_Contactname", "[id] = " & myRecipientID)

End If

myRecipientFullEmail = myrecipientFN & " " & myRecipientLN & " <" & myrecipientemail & ">"

