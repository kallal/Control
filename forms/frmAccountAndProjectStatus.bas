Private Sub cmdEdit_Click()

If DLookup("[CanAlterAccountStatus]", "dbo_CompanyEmployee", "[ID] = " & Forms.frmLogin![ID]) = True Then

    
    If Me!cmdEdit.caption = "LOCKED" Then
        Me!cmdEdit.caption = "EDITABLE"
        Me!cmdEdit.BackColor = vbGreen
        
        Me.Form.AllowEdits = True
        
        AddContactHistory Me.ContactGeneralID, , , Me.ID, 1, "Editing Account Status"
        
        
     
     
    Else
        
        Me!cmdEdit.caption = "LOCKED"
        Me!cmdEdit.BackColor = vbRed
        Me.Form.AllowEdits = False
    
    End If

Else
    MsgBox "You do not have permission to change the account status. Please contact the credit manager.", vbCritical, ProgramName
End If

'end if

End Sub



Private Sub CODBalanceDue_AfterUpdate()
Dim myRecipient As String

'check if OK to release
If Me.CODBalanceDue <= 0 Then
    Me.CODPaidYN = True
    Me.CODOKToReleaseYN = True
    
    'Prompt to send shipping info to release COD's
    myResponse = vbYes
    myResponse = MsgBox("Send COD Release Email to Shipping?", vbQuestion, ProgramName)
    If myResponse = vbYes Then
        myRecipient = Nz(DLookup("SendCODReleaseEmailTo", "dbo_Company", "[ID] = 1"), "")
        myDocket = "D" & Me.DocketNum
        myCustomer = DLookup("CompName", "dbo_ContactGeneral", "[ID] = " & Me.ContactGeneralID)
        
        If myRecipient = "" Then
            MsgBox "Cannot Send Email - Send COD Release Email To address in Company record is missing.", vbCritical, ProgramName
        Else
            SendMailCDO myRecipient, "COD Release - " & myDocket & " : " & myCustomer, "This COD project is OK to release to the client."
            MsgBox "Release COD Email has been sent", vbInformation, ProgramName
        End If
        
    End If
    
    
End If

If (Me.CODBalanceDue <= (Me.CODTotalDue / 2) - 1) And Me.CODBalanceDue > 0 Then
    myResponse = vbNo
    myResponse = MsgBox("Over 50% has been paid. OK to Produce?", vbYesNo, ProgramName)
    If myResponse = vbYes Then
        Me.CODOKToProduceYN = True
        Me.CODPaidYN = False
    Else
        Me.CODOKToProduceYN = False
        Me.CODPaidYN = False
        
    End If
End If

End Sub

