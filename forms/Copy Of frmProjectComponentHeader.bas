Private Sub ProofApprovedDate_AfterUpdate()


Dim myRecipient As String
Dim mySubject As String
Dim MyMessage As String

If Nz(Me.DYMailData, 0) = 0 Or Nz(Me.DataArrivedDate, 0) > 0 Then 'If there is NO MAIL TASK, or if MAIL DATA has arrived IN THIS COMPONENT - YOU CAN ASSIGN A DATE
    myTotDays = 0
    If Me.DYPrepress < 2 Then myTotDays = 1 Else myTotDays = Me.DYPrepress - 2
    myTotDays = Nz(myTotDays, 0) + Nz(Me.DYPrint, 0) + Nz(Me.DYOutsideSrv, 0) + Nz(Me.DYLFFinish, 0) + Nz(Me.DYBindery, 0) + Nz(Me.DYMailData, 0) + Nz(Me.DYMailShop, 0)
    Me.AssignedDueDate = fncWorkday(Me.ProofApprovedDate, myTotDays)
    'Check if Assigned date conflicts with Specified date
    If Me.AssignedDueDate > Me.SpecifiedDueDate Then
        MsgBox "PLEASE NOTE: According to the days required to complete this job, we will not be able to meet the specified delivery date.", vbCritical, ProgramName
    End If
    
Else
    MsgBox "I cannot assign a delivery date until we have confirmation that the mail data has been received.", vbCritical, ProgramName
End If

'***** Change code below - if there are multiple components, show a fraction for approvals.

If Me.DYPrepress > 0 Then
    If Nz(Me.ProofApprovedDate, 0) <> 0 Then
        Me.StPrepress = "SOFT APP'D"
    Else
        'NO CHANGE
    End If
End If
    
   
'find out what the stock situation is... is it in, or on hold at vendor?

If Nz(Me.ProofApprovedDate, 0) <> 0 Then
Dim MYDB As Database
Dim myRST As Recordset
Set MYDB = CurrentDb()
Set myRST = MYDB.OpenRecordset("dbo_ProjectComponentStock", dbOpenDynaset, dbSeeChanges)

myRST.FindFirst ("[ProjectComponentID] = " & Me.ID)
done = False
Do Until done = True
    If myRST!OrderOption = 5 Then
        ' job is set to hold at vendor - send a release email.
        

        myCC = cReleasePaperAtVendorEmail
       
        myRecipient = DLookup("[email]", "qryContactIDEmail", "[id] = " & myRST!VendorNameID)
        If Nz(myRecipient, "") <> "" Then
            myRecipient = myRecipient & "; " & myCC
        Else
            myRecipient = myCC
        End If
        
        
        mySubject = "D" & DLookup("DocketNum", "dbo_projectHeader", "[ID] = " & Me.ProjectHeaderID) & " - Proof Approved - Release stock at vendor."
        MyMessage = "Proof has been approved for this job. The order was originally placed as Order and Hold... Be sure to confirm stock and quantity, and release the stock."
        
        SendMailCDO myRecipient, mySubject, MyMessage
        
    End If
    
    If myRST!OrderOption = 4 And Nz(myRST!ArrivalDate, 0) <> 0 Then 'paper is here... release it to the cutter or press
       '***** change to pull shipping email address from database
        myRecipient = cReleaseStockToCutterEmail
        mySubject = "D" & DLookup("DocketNum", "dbo_projectHeader", "[ID] = " & Me.ProjectHeaderID) & " - Proof Approved - Release stock to production."
        MyMessage = "Proof has been approved for this job that you have already received stock for. Please mark the yellow sheet as Proof Approved, and release the stock to production, either to the press or to cutting if required."
        
        SendMailCDO myRecipient, mySubject, MyMessage
        
    End If
    myRST.FindNext ("[ProjectComponentID] = " & Me.ID)
    If myRST.NoMatch Then done = True
    
Loop

End If

    
    
AddContactHistory Parent!ContactGeneralID, Parent!BillAddLocationID, Parent!BillOrderedBy, Parent!ID, 1, "Soft Proof Approved Date Changed to " & Me.ProofApprovedDate & " for component " & Me.ComponentName


If Me.Dirty Then Me.Dirty = False



End Sub

