Public Sub extQuoteSelect_AfterUpdate()

Dim mypressname As String

'*****************************  THIS CODE CALLS ImportPDQXML to preload tables


Dim mystuff As String ' garbage

'Trigger XML input of the job and quantity shown.
Dim mypdqfilename As String
Dim myPlateCount As Integer
Dim myLayoutCount As Integer
Dim myFriendlyDescription As String
Dim Failed As Boolean


myPDQNumber = Me!ExtQuoteNumDisplay
mypdqlocation = cPDQQuoteXMLFolder & "\" 'constPDQxmlfiles
myFieldName = "ExtQuoteQty" & extQuoteSelect
mypdqfilename = mypdqlocation & "PDQ" & myPDQNumber & "_" & Me(myFieldName) & ".xml"
mypdqlayoutprefix = mypdqlocation & "PDQ" & myPDQNumber & "_" & Me(myFieldName)

'MsgBox "Importing file: " & mypdqfilename, vbInformation, ProgramName

'Import the XML Data into the local PDQ tables
Failed = False
'Call ImportPDQXML(mypdqfilename, Failed) ' Not needed - called in next line.

Call ImportPDQXML(mypdqfilename)
    
    
If cPDQImportResult = "Failed" Then Exit Sub

'Populate up the various subsections

'Clean out stocks and replace with new information

Dim MYDB As Database
Dim myProjectComponentStock As Recordset
Dim myPDQStockResult As Recordset

Set MYDB = CurrentDb

Me.Refresh

'clean out the existing stock listings as there is no way to match them up
mysqlcode = "Delete * FROM dbo_ProjectComponentStock where [ProjectComponentID] = " & Me!ID & " AND NZ(OrderDate,0) = 0"
CurrentDb.Execute mysqlcode, dbSeeChanges


'parse out the gross and net sheets from the imposition details.
Set myImposition = MYDB.OpenRecordset("pdqImposition", dbOpenDynaset, dbSeeChanges)
myImposition.MoveFirst



    
Dim myPdqProcess As Recordset
Dim myPricingDetail As Recordset

'Delete the prior data in pricing detail first

mysqlcode = "Delete * FROM dbo_ProjectComponentPricingDetail where [ProjectComponentID] = " & Me!ID
CurrentDb.Execute mysqlcode, dbSeeChanges

'Open needed files - the imported pdqProcess and the component pricing
Set myPdqProcess = MYDB.OpenRecordset("pdqProcess", dbOpenDynaset, dbSeeChanges)
Set myPricingDetail = MYDB.OpenRecordset("dbo_ProjectComponentPricingDetail", dbOpenDynaset, dbSeeChanges)

Dim myProjectComponentBindery As Recordset ' BINDERY - Set flags as it loops through the processes
Set myProjectComponentBindery = MYDB.OpenRecordset("dbo_ProjectComponentBindery", dbOpenDynaset, dbSeeChanges)

myProjectComponentBindery.FindFirst ("[ProjectComponentID] = " & Me!ID) 'IF there is no record yet, create one.
If myProjectComponentBindery.NoMatch Then

    myProjectComponentBindery.AddNew
    myProjectComponentBindery!ProjectComponentID = Me!ID
   
Else
    myProjectComponentBindery.Edit
End If




myTotStock = 0
myTotCreative = 0
myTotPrepress = 0
myTotMailData = 0
myTotPrint = 0
myTotOutsideSrv = 0
myTotLFFinish = 0
myTotBindery = 0
myTotMailShop = 0
myTotDelivery = 0
myTotalPaperCost = 0



myPdqProcess.MoveFirst

Dim myPDQImpositionTemp As Recordset
Set myPDQImpositionTemp = MYDB.OpenRecordset("pdqImposition", dbOpenDynaset, dbSeeChanges)

Dim myPDQServiceList As Recordset
Set myPDQServiceList = MYDB.OpenRecordset("dbo_PDQServiceList", dbOpenDynaset, dbSeeChanges)
'Also need to calc time per print run here.

myPrintRunNum = 1


    
With myPricingDetail

Do Until myPdqProcess.EOF
    .AddNew
    !QuoteID = myPdqProcess!QuoteID
    !SectionName = myPdqProcess!SectionName
    !FunctionID = myPdqProcess!FunctionID
    If !SectionName <> "Global" Then
        !PrintRunNum = myPrintRunNum
    End If
    
    !name = myPdqProcess!name
    If myPdqProcess!name = "Paper" Then
        myPrintRunNum = myPrintRunNum + 1
    End If
    
    !Cost = myPdqProcess!Cost
    !CarbonKG = myPdqProcess!CarbonKG
    !ChargeBasis = myPdqProcess!ChargeBasis
    !ChargeBasisPerHour = myPdqProcess!ChargeBasisPerHour
    !ChargeType = myPdqProcess!ChargeType
    !conversionType = myPdqProcess!conversionType
    !CustomItem = myPdqProcess!CustomItem
    !DirectCosts = myPdqProcess!DirectCosts
    !IndirectCosts = myPdqProcess!IndirectCosts
    !Time = myPdqProcess!Time
    !TimeInMins = myPdqProcess!TimeInMins
    !TimeInHrs = myPdqProcess!TimeInHrs
    !TotalMaterialCosts = myPdqProcess!TotalMaterialCosts
    !ProjectComponentID = Me!ID
    
    'Figure out what category the item is and assign it a category number
    

    myPDQServiceList.MoveFirst
    Do Until myPDQServiceList.EOF
        

        
        
        
        If Left(!name, Len(myPDQServiceList!Service)) = myPDQServiceList!Service Then
        
        
        
            Select Case myPDQServiceList!Category
                Case 1
                !ProjectComponentCategory = 1
                myTotStock = myTotStock + myPdqProcess!TimeInHrs
                
                Case 2
                !ProjectComponentCategory = 2
                myTotCreative = myTotCreative + myPdqProcess!TimeInHrs
                
                Case 3
                !ProjectComponentCategory = 3
                myTotPrepress = myTotPrepress + myPdqProcess!TimeInHrs
                
                Case 4
                !ProjectComponentCategory = 4
                myTotMailData = myTotMailData + myPdqProcess!TimeInHrs
                
                Case 5
                !ProjectComponentCategory = 5
                myTotPrint = myTotPrint + myPdqProcess!TimeInHrs
                
                Case 6
                !ProjectComponentCategory = 6
                tmpCPDays = 0
                If Nz(!Cost, 0) > 0 Then
                    tmpCPDays = Int(Nz(!Cost, 0) / 1000) + 1
                End If
                
                myTotOutsideSrv = myTotOutsideSrv + (8 * tmpCPDays) 'Add a day for each outside srv, plus another for each $1000, plus it adds one more day at the bottom.
                
                Case 7
                !ProjectComponentCategory = 7
                myTotLFFinish = myTotLFFinish + myPdqProcess!TimeInHrs
                
                Case 8
                !ProjectComponentCategory = 8
                myTotBindery = myTotBindery + myPdqProcess!TimeInHrs
                
                Case 9
                !ProjectComponentCategory = 9
                myTotMailShop = myTotMailShop + myPdqProcess!TimeInHrs
                
                Case 10
                !ProjectComponentCategory = 10
                myTotDelivery = myTotDelivery + myPdqProcess!TimeInHrs
                
                End Select
        End If
    myPDQServiceList.MoveNext
    Loop
    



    
    If Left(!name, 5) = "Paper" And !SectionName <> "Global" And Nz(!SectionName, "") <> "" Then
        
        mysrchstring = "[SectionName] = " & Chr(34) & !SectionName & Chr(34)
        
        myPDQImpositionTemp.FindFirst mysrchstring
        myPDQImpositionTemp.Edit
        startPaperCost = Nz(myPDQImpositionTemp!PaperCost, 0)
        myIntPaperCost = !Cost + startPaperCost
        myPDQImpositionTemp!PaperCost = myIntPaperCost
        myPDQImpositionTemp.Update
    End If
    
        
        
    If Left(!name, 5) = "Paper" And !SectionName = "Global" Then
        myTotalPaperCost = myTotalPaperCost + !Cost
        !ProjectComponentCategory = 99

    End If
    
    If Left(!name, 15) = "Component Costs" And !SectionName = "Global" Then
        !ProjectComponentCategory = 99
    End If
    
    
'Increase plate and layout counters
    If Left(!name, 15) = "Platemaking RUN" Then
        myPlateCount = myPlateCount + !ChargeBasis
    End If
    
    If Left(!name, 15) = "Proofing hi-res" Then
        myLayoutCount = myLayoutCount + !ChargeBasis
    End If
    
'Add flags for Bindery work for known tasks
    If Left(!name, 12) = "Guillotining" Then myProjectComponentBindery!IsTrim = True
    If Left(!name, 4) = "Fold" Then myProjectComponentBindery!FoldYN = True
    If Left(!name, 11) = "Press score" Then myProjectComponentBindery!IsScore = True
    If Left(!name, 5) = "Drill" Then myProjectComponentBindery!DrillYN = True
    If Left(!name, 14) = "Pocket Glueing" Then myProjectComponentBindery!IsGlue = True
    If Left(!name, 11) = "Die Cutting" Then myProjectComponentBindery!DieCutYN = True
    If Left(!name, 5) = "Score" Then myProjectComponentBindery!IsScore = True
    
    
    
    
    .Update
    myPdqProcess.MoveNext
 
Loop  'mypdqprocess






End With
myProjectComponentBindery.Update 'Close off Bindery changes


Set myProjectComponentStock = MYDB.OpenRecordset("dbo_ProjectComponentStock", dbOpenDynaset, dbSeeChanges)


'*********** Populate the Print Runs, Stock Items, Bindery Items with the same loop - also create the friendly description.

Dim myProjectComponentPress As Recordset 'Press Runs
Set myProjectComponentPress = MYDB.OpenRecordset("dbo_ProjectComponentPress", dbOpenDynaset, dbSeeChanges)

'Delete any existing print runs - we don't have any way to match them up yet.
mysqlcode = "Delete * FROM dbo_ProjectComponentPress where [ProjectComponentID] = " & Me!ID
CurrentDb.Execute mysqlcode, dbSeeChanges



Set myPDQStockResult = MYDB.OpenRecordset("pdqQryPaperStock", dbOpenDynaset, dbSeeChanges) 'grab the imported XML data table
myPDQStockResult.MoveFirst

myPrintRunCount = 0

Do Until myPDQStockResult.EOF

    myProjectComponentStock.AddNew
    myProjectComponentPress.AddNew
    myPrintRunCount = myPrintRunCount + 1
