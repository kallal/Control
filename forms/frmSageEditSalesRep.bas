Private Sub cmdAddAll_Click()

   Dim rst     As Recordset

   Set rst = Me.RecordsetClone
   
   Do While rst.EOF = False
   
   ' send this one customer to sage
      If IsNull(rst!Added) = True Then
         SageEmployee.AddEmployee Me.SalesRep, _
               Nz(Me!Add1, ""), Nz(Me!Add2, ""), Nz(Me!City, ""), Nz(Me!Prov, ""), _
                 quPhone(Me!PhoneDirect), quPhone(Me!PhoneCell), Nz(Me!PC, "")
      End If
      rst.MoveNext
   Loop
   
   rst.Close
   
   ' exit the form
   
   DoCmd.Close acForm, Me.name
   

End Sub

Private Sub cmdAddSingle_Click()

   
   If IsNull(Me.Added) = False Then
      MsgBox "Already added", vbInformation, "Added"
      Exit Sub
   End If
   
   ' send this one customer to sage
'   SageEmployee.AddEmployee Me.FN, Me.MI, Me.LN, _
'               Nz(Me.Add1, ""), Nz(Me.Add2, ""), Nz(Me.City, ""), Nz(Me.Prov, "AB"), _
'               quPhone(Me.PhoneDirect), quPhone(Me.PhoneCell), Nz(Me.PC, "")

   If SageEmployee.AddEmployee(Nz(Me.FN, ""), Nz(Me.MN, ""), Nz(Me.LN, ""), _
               Nz(Me.Add1, ""), Nz(Me.Add2, ""), Nz(Me.City, ""), Nz(Me.Prov, ""), _
               quPhone(Me.PhoneDirect), quPhone(Me.PhoneCell), Nz(Me.PC, "")) = True Then
               
      Me.Added = "Added"
      Me.Dirty = False
   Else
      MsgBox "unable to add Employee", vbExclamation, "Add employee"
   End If
   
   
   
End Sub

