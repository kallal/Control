Private Sub txtRightArrow_KeyDown(KeyCode As Integer, Shift As Integer)
Select Case KeyCode
    Case vbKeyUp ' Move this record up one
        'MsgBox "Up"
        myOldRec = Me.ID
        
        myoldpriority = Me.Priority
        KeyCode = 0
        
        On Error GoTo exitsub:
        DoCmd.RunCommand acCmdRecordsGoToPrevious
        myNewRecBM = Me.Bookmark
        mynewPriority = Me.Priority
       
    
        Me.Priority = myoldpriority
        DoCmd.RunCommand acCmdRecordsGoToNext
        Me.Priority = mynewPriority


        
    Case vbKeyDown
        myOldRec = Me.ID
        
        myoldpriority = Me.Priority
        KeyCode = 0
        
        On Error GoTo exitsub:
        DoCmd.RunCommand acCmdRecordsGoToNext
        myNewRecBM = Me.Bookmark
        mynewPriority = Me.Priority
       
    
        Me.Priority = myoldpriority
        DoCmd.RunCommand acCmdRecordsGoToPrevious
        Me.Priority = mynewPriority

    Case Else
        GoTo exitsub:
        
End Select

MYORDERBY = Me.OrderBy 'trigger a reorder on screen.
Me.OrderBy = MYORDERBY
Me.OrderByOn = True

'Call cmdSearch_Click
Me.Recordset.FindFirst "[ID] = " & myOldRec


exitsub:

DoCmd.GoToControl "txtRightArrow"
End Sub

