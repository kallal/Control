Private Sub cboShowOnly_AfterUpdate()

Dim myfilter As String

Select Case cboShowOnly
    Case "NOT ORDERED"
        myfilter = "(ORDEROPTION = 4 OR ORDEROPTION = 5) and (NZ(ORDERDATE,0)) = 0"

     Case "WAITING FOR RELEASE AT VENDOR"
        myfilter = "ORDEROPTION = 5 AND (NZ(ORDERDATE,0)) <> 0 and (nz(expecteddate,0)) = 0"

        
    Case "ORDERED, NOT YET ARRIVED" 'Has Expected Date but no Arrival Date - DEFAULT OPTION FOR RECEIVING
        myfilter = "Nz(ExpectedDate, 0) <> 0 And (NZ([QtyOrdered],0) > NZ([QtyReceived],0))"
        
    Case "RECEIVED (BUT NOT INVOICED)" 'HAS EXPECTED DATE AND ARRIVAL DATE BUT NOT INVOICED (INVOICED DON'T COME THROUGH TO THIS QUERY)
        myfilter = "NZ(EXPECTEDDATE,0) <> 0 AND NZ(ArrivalDate, 0) <> 0"
    Case "NOT PLANNED"
        myfilter = "(ORDEROPTION <= 2)"
        

End Select

If Nz(Me.txtFindDocket, 0) > 0 Then
    myfilter = myfilter & " AND [DOCKETNUM] = " & Me.txtFindDocket
End If

If Nz(Me.txtFindVendor, 0) <> 0 Then
    myfilter = myfilter & " AND [VENDORID] = " & Me.txtFindVendor
End If


Me.frmReceivingStock.Form.Filter = myfilter
Me.frmReceivingStock.Form.FilterOn = True
'If Me.Dirty Then Me.Dirty = False

End Sub

Private Sub cmdReceive_Click()
'Validation of data...
Dim myRST As Recordset
Dim myRST2 As Recordset
Dim mysql As String

Dim myProjectComponentRST As Recordset
Set myRST = Me.frmReceivingStock.Form.RecordsetClone

Dim MYDB As Database
Set MYDB = CurrentDb()


Set myProjectComponentRST = MYDB.OpenRecordset("dbo_ProjectComponentHeader", dbOpenDynaset, dbSeeChanges)



myRST.FindFirst ("nz([RecTagQty],0) <> 0") 'Is there any QtyIn at all?
If myRST.NoMatch Then
    MsgBox "None of the items in the list have been tagged to be received. Please select the lines and quantities to receive and try again.", vbCritical, ProgramName
    Exit Sub
End If

tmpDocketNum = myRST!DocketNum
done = False
myMultipleDockets = False


Do Until done = True
    myRST.FindNext ("nz([RecTagQty],0) <> 0")
    If myRST.NoMatch Then
        done = True
    Else
        If myRST!DocketNum <> tmpDocketNum Then
            myMultipleDockets = True
        End If
    End If
Loop

If myMultipleDockets Then 'Is there a mix of purchase orders on one packing slip - need to ok this.
    myResponse = vbNo
    myResponse = MsgBox("You have indicated that you want to receive items on this Packing Slip from multiple purchase orders. Is that correct?", vbYesNo, ProgramName)
    If myResponse = vbNo Then
        Exit Sub
    End If
End If

'Is there a Vendor Packing Slip number

If Nz(Me.txtVendorOrder, "") = "" Then
    MsgBox "You need to enter a Vendor Order Number (Packing Slip) before receiving these items. Please try again.", vbCritical, ProgramName
    Exit Sub
End If



'Then, process the receipt of goods.
'Add the QtyToReceive to the QtyReceived in each line, and clear the selected and tags.
myRST.MoveFirst

Do Until myRST.EOF
    If Nz(myRST!RecTagQty, 0) <> 0 And myRST!RecTagID = cLoginID Then 'Only tag ones that this user is working with.
        mysql = "SELECT * FROM dbo_ProjectComponentStock WHERE [ID] = " & myRST!ID
        Set myRST2 = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
        
        myRST2.Edit
        myRST2!QtyReceived = Nz(myRST2!QtyReceived, 0) + myRST2!RecTagQty
        myRST2!ArrivalDate = Now()
        myRST2!ReceivedBy = myRST2!RecTagID
        myRST2!SupplierPackingListNumber = Me.txtVendorOrder
        myRST2!RecTagQty = Null
        myRST2!RecTagID = Null
        
        
        
        myProjectComponentRST.FindFirst "[ID] = " & myRST!ProjectComponentHeaderID
        myProjectComponentRST.Edit
        If myRST2!QtyReceived >= myRST2!QtyOrdered Then
            myProjectComponentRST!StStock = "COMP-STOCK IN"
        Else
            myProjectComponentRST!StStock = "PART STK RCD"
        End If
        
        myRST2.Update
        Set myRST2 = Nothing
        myProjectComponentRST.Update
        
        
        
        AddContactHistory myRST!ContactGeneralID, , , myRST!ProjectHeaderID, 1, "Received " & myRST!RecTagQty & " of " & myRST!Description 'add a history line to the docket showing how much of each line was received
        myResponse = 1
        myResponse = InputBox("How many labels do you want for this shipment?", ProgramName, 1)
        If myResponse <> 0 Then
             
             If DLookup("IsFSC", "dbo_ProjectHeader", "[ID] = " & myRST!ProjectHeaderID) = True Then
                'Send commands to print FSC version of report on bypass tray
                MsgBox "Please Insert Flourescent paper in Printer Bypass tray.", vbCritical, ProgramName
                Application.Echo False
                DoCmd.Hourglass True
                DoCmd.OpenReport "rptReceivingLabelsFSC", acViewPreview, , "[ID] = " & myRST!ID
                DoCmd.PrintOut , , , , myResponse
                DoCmd.Close acReport, "rptReceivingLabelsFSC"
                Application.Echo True
                DoCmd.Hourglass False
            Else
                Application.Echo False
                DoCmd.Hourglass True
                DoCmd.OpenReport "rptReceivingLabels4", acViewPreview, , "[ID] = " & myRST!ID
                DoCmd.PrintOut , , , , myResponse
                DoCmd.Close acReport, "rptReceivingLabels4"
                Application.Echo True
                DoCmd.Hourglass False
            End If
            
             
        End If


    End If
    myRST.MoveNext

Loop






'requery the screen and get ready for the next receipt

myRST.Close
Me.txtVendorOrder = Null

If Me.Dirty Then Me.Dirty = False
Me.Refresh

Me.Repaint

End Sub

