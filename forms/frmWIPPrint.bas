Private Sub cmdPrint_Click()
myReportName = Me.rptWIPPrint.SourceObject
If Left(myReportName, 7) = "Report." Then
    myReportName = Right(myReportName, Len(myReportName) - 7)
End If

myfilter = ""

'MsgBox "Show only: " & Me.chkShowOnlyReady

If Nz(Me.chkShowProofOkOnly, False) = True Then
    myfilter = myfilter & "(Nz([StPrint], '') = 'IMPOSE' Or Nz([StPrepress], '') = 'IMPOSITION' Or Nz([StPrepress], '') = 'APPROVED' Or Nz([StPrepress], '') = 'PLATEMAKING' Or Nz([StPrint], '') = 'IMPOSE' Or Nz([StPrepress], '') = 'IMPOSITION' Or Nz([StPrepress], '') = 'APPROVED' Or Nz([StPrepress], '') = 'PLATEMAKING') And "
End If

If Nz(Me.chkShowPaperOKOnly, False) = True Then
    myfilter = myfilter & " ([arrivaldate] Is Not Null Or [ORDEROPTION]=3 OR [PAPERDISPLAY] = 'UNKNOWN') AND"
End If


'MsgBox "Press = " & Me.cboSelectPress

If Nz(Me.cboSelectPress, "") <> "" Then
    myfilter = myfilter & " [Press] = " & Me.cboSelectPress & " AND"
End If

If Len(myfilter) >= 4 Then
    myfilter = Left(myfilter, Len(myfilter) - 3)
End If



DoCmd.OpenReport myReportName, acViewPreview, , myfilter

End Sub

Public Sub cmdSearch_Click()
Application.Echo False

With rsLocSS
.Edit
!tmpPrintMachineID = Me.cboSelectPress
'!tmpPrintSelectReadyOnly = Me.chkShowOnlyReady
.Update
End With


Dim mysql As String

If Me.Dirty Then Me.Dirty = False


myfilter = ""

'MsgBox "Show only: " & Me.chkShowOnlyReady

If Nz(Me.chkShowProofOkOnly, False) = True Then
    myfilter = myfilter & "(Nz([StPrint], '') = 'IMPOSE' OR (Left(nz([StPrepress],''),4) = 'COMP') Or Nz([StPrepress], '') = 'IMPOSITION' Or Nz([StPrepress], '') = 'APPROVED' Or Nz([StPrepress], '') = 'PLATEMAKING' Or Nz([StPrint], '') = 'IMPOSE' Or Nz([StPrepress], '') = 'IMPOSITION' Or Nz([StPrepress], '') = 'APPROVED' Or Nz([StPrepress], '') = 'PLATEMAKING') And "
End If

If Nz(Me.chkShowPaperOKOnly, False) = True Then
    myfilter = myfilter & " ([arrivaldate] Is Not Null Or [ORDEROPTION]=3 OR [PAPERDISPLAY] = 'UNKNOWN') AND"
End If

'MsgBox "Press = " & Me.cboSelectPress

If Nz(Me.cboSelectPress, "") <> "" Then
    myfilter = myfilter & " [Press] = " & Me.cboSelectPress & " AND"
End If





strWhere = ""

strWhere = strWhere & " 1 = 1 AND"

myDate = False
mySortByFlag = False 'Flag for if to show the "Sort by" line.
'create filter criteria

'if only put date filter on if either of the dates have entries...




If Len(Me.txtCompanyID) <> 0 Then
    mycomp = Me.txtCompanyID
    

    done = False
        
    Do Until done = True 'create as many criteria as there are spaces
        mymark = InStr(Trim(mycomp), " ")
        
        If mymark = 0 Then done = True
        If mymark = 0 Then mymark = Len(mycomp)
        
        myComp1 = Trim(Left(mycomp, mymark))
        mycomp = Right(mycomp, Len(mycomp) - mymark)
        strWhere = strWhere & " [COMPNAME] like '*" & myComp1 & "*' AND"
        mySortByFlag = True
        myTag = myTag & " | Company: " & myComp1
        
    Loop

End If

'Set up which plant to view - default to all (99)
' If Me.frmPlant = 99 then show all - no line required
If Me.frmPlant >= 1 And Me.frmPlant <= 9 Then
     strWhere = strWhere & " [CompanyLocationID] = " & Me.frmPlant & " AND"
End If


myDocketnum = Val(Nz(Me.txtDocketNum, 0))

If myDocketnum > 0 Then
    If myDocketnum <= 99999 Then 'Pad it with 3xxxxx before running query.
        myDocketnum = myDocketnum + 300000
        Me.txtDocketNum.Value = str(myDocketnum)
    End If
    
    strWhere = strWhere & " [DocketNum] = " & myDocketnum & " AND"
    myTag = myTag & " | Docket Number: " & str(myDocketnum)
    mySortByFlag = True
End If

If Len(Me.txtSalesRepID) <> 0 Then
    strWhere = strWhere & " [SalesRepID] = " & Me.txtSalesRepID & " AND"
    myTag = myTag & " | Sales Rep: " & Me.txtSalesRepID.Column(1)
    mySortByFlag = True
End If

If Len(Me.txtCsrID) <> 0 Then
    strWhere = strWhere & " [CsrID] = " & Me.txtCsrID & " AND"
    myTag = myTag & " | CSR: " & Me.txtCsrID.Column(1)
    mySortByFlag = True
End If


myfilter = myfilter & strWhere




If Len(myfilter) > 0 Then
    myfilter = Left(myfilter, Len(myfilter) - 3)
    myfilter = "WHERE " & myfilter
End If

If cControlCompanyCode = "DPI" Then
    mysql = "SELECT * FROM qryProjectComponentPressSchedule_DPI " & myfilter
Else
    mysql = "SELECT * FROM qryProjectComponentPressSchedule " & myfilter
End If

'test code
'mySQL = "SELECT * from qryProjectComponentPressSchedule"
'Me.rptWIPPrint.Report.Filter = myFilter
Me.rptWIPPrint.Report.RecordSource = mysql



Application.Echo True


End Sub



