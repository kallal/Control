   ' setup label array
   Call SetMLabels
   ' load up recordset
  
End Sub


Private Sub SetMLabels()

   Dim rst     As DAO.Recordset
   Dim strSQL  As String
   Dim j       As Integer
   
   strSQL = "select * from qryCboMachinePressOnly"
   Set rst = CurrentDb.OpenRecordset(strSQL, dbOpenDynaset, dbSeeChanges)
   
   Do While rst.EOF = False
      j = j + 1
      If j <= 11 Then
         Mlabels(j) = Left(rst!MachineModel, 10)
         MlabelsMap(j) = rst!ID
      End If
      rst.MoveNext
   Loop
   
   rst.Close
   Set rst = Nothing
   
   Set rstCrossTab = CurrentDb.OpenRecordset("qrySchedulingSummaryForReportPressTab")
   rstCrossTab.MoveLast
   
End Sub


Public Function MyPressStatus(vPress As Integer) As String

   Dim rF         As String
         
   rstCrossTab.FindFirst "qrySchedulingSummaryForReport.id = " & Me.ID
   If rstCrossTab.NoMatch = False Then
      rF = MlabelsMap(vPress)
      On Error Resume Next
      Select Case rstCrossTab.Fields(rF).Value
         Case 1
            MyPressStatus = "l"
         Case 2
            MyPressStatus = "6"           ' a è (->) in place of hour glass 6
         Case 3
            MyPressStatus = "ü"
      End Select
      
   End If
   

End Function