Private Sub Refcode_Click()

Dim mysql As String
Dim myrs As Recordset

myrefcode = Nz(Me.RefCode, "")

myrefcode = InputBox("Please enter your internal Reference Code Notes here.", ProgramName, myrefcode)

mysql = "Select * FROM ssQryOrderDetails WHERE [ProductID] = " & Me.ProductID
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
myrs.Edit
myrs!RefCode = myrefcode
myrs.Update

Set myrs = Nothing
Me.Requery



End Sub