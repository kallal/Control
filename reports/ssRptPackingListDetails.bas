Private Sub cmdJobDetails_Click()
'Open Job ticket for this job.

Dim MYDB As Recordset
Dim mysql As String

mysql = "SELECT * FROM ssd_OrderDetails WHERE [ProductID] = " & Me.ProductID

Set MYDB = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)

Do Until MYDB.EOF
    MYDB.Edit
    MYDB!TrackingCode = Now()
    MYDB.Update
    MYDB.MoveNext
    
Loop


DoCmd.OpenReport "rptSS_JobTicket", acViewPreview, , "[ProductID] = " & Me.ProductID, acDialog

Me.Report.Requery

End Sub

Private Sub cmdLabel_Click()
DoCmd.OpenReport "rptSS_JobLabel", acViewNormal, , "[ProductID] = " & Me.ProductID, acDialog
End Sub

Private Sub imgThumbnail_Click()
