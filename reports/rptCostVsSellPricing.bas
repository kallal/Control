Private Sub cmdMoveDown_Click()
tmpCurrentSortOrder = Me.SortOrder

On Error Resume Next
DoCmd.GoToRecord , , acNext


tmpNewSortOrder = Me.SortOrder
If tmpCurrentSortOrder <> tmpNewSortOrder Then
    Me.SortOrder = tmpCurrentSortOrder
    DoCmd.GoToRecord , , acPrevious
     Me.SortOrder = tmpNewSortOrder
    Me.Requery
End If

End Sub

Private Sub cmdMoveUp_Click()
'Exchange this component with the one above it.
tmpCurrentSortOrder = Me.SortOrder

On Error Resume Next
DoCmd.GoToRecord , , acPrevious


tmpNewSortOrder = Me.SortOrder
If tmpCurrentSortOrder <> tmpNewSortOrder Then
    Me.SortOrder = tmpCurrentSortOrder
    DoCmd.GoToRecord , , acNext
     Me.SortOrder = tmpNewSortOrder
    Me.Requery
End If



End Sub

