Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Compare Database

Private Sub Detail_Format(Cancel As Integer, FormatCount As Integer)

Dim myrs As Recordset
Dim mysql As String

Dim MYDB As Recordset

Dim MySuccess           As Boolean
Dim myXMLData           As MSXML2.DOMDocument
Dim myNode              As MSXML2.IXMLDOMNode
Dim myNodeList          As MSXML2.IXMLDOMNodeList
Dim myFileName          As String

Set myXMLData = CreateObject("MSXML2.DOMDocument")

On Error Resume Next

'Set txtRelatedJobs from other jobs linked to the same order number

MYTXTRELATEDJOBS = ""

mysql = "SELECT * from ssd_OrderDetails WHERE [OrderID] = " & OrderID

Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenSnapshot)

myCounter = 0
myCounterID = 0


Do Until myrs.EOF
     myCounter = myCounter + 1  'for some unknown reason, myrs.recordcount was giving bad results
     If Me.ProductID <> myrs!ProductID Then
        MYTXTRELATEDJOBS = MYTXTRELATEDJOBS & myrs!ProductID & " / "
     Else
        myCounterID = myCounter
    End If
     myrs.MoveNext
Loop
 
Me.txtRelatedJobs = "Job " & myCounterID & " of " & myCounter
    
If Len(MYTXTRELATEDJOBS) > 0 Then MYTXTRELATEDJOBS = "Additional Jobs on this Order: " & Left(MYTXTRELATEDJOBS, Len(MYTXTRELATEDJOBS) - 3)
Me.txtAssociatedJobs = MYTXTRELATEDJOBS


'***** Set up BillTo
myaddressid = Nz(Me.billingAddressID, Me.ShippingAddressID)

mysql = "Select * from ssd_Addresses WHERE [AddressID] = " & myaddressid
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenSnapshot)
myadd = myrs!firstname & " " & myrs!LastName & vbCrLf
myadd = myadd & IIf(Len(myrs!CompanyName) > 0, myrs!CompanyName & vbCrLf, "")
myadd = myadd & IIf(Len(myrs!DepartmentName) > 0, myrs!DepartmentName & vbCrLf, "")
myadd = myadd & myrs!Street & vbCrLf & IIf(myrs!extendedaddress <> "", myrs!extendedaddress & vbCrLf, "") & myrs!City & ", " & myrs!State & "  " & myrs!ZipCode & "  " & myrs!Country
myadd = myadd & IIf(Len(myrs!phone) > 0, vbCrLf & "Phone: " & myrs!phone, "")
myadd = myadd & IIf(Len(myrs!Email) > 0, vbCrLf & "Email: " & myrs!Email, "")
Me.txtBillTo = myadd



'***** Set up ShipTo
myadd = ""

If Len(Me.AttnTo) > 0 Then
    myadd = "ATTN: " & Me.AttnTo & vbCrLf & myadd
End If

If Nz(billingAddressID, 0) > 0 Then
    mysql = "Select * from ssd_Addresses WHERE [AddressID] = " & ShippingAddressID
    Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenSnapshot)
    myadd = myrs!firstname & " " & myrs!LastName & vbCrLf
    myadd = myadd & IIf(Len(myrs!CompanyName) > 0, myrs!CompanyName & vbCrLf, "")
    myadd = myadd & IIf(Len(myrs!DepartmentName) > 0, myrs!DepartmentName & vbCrLf, "")
    myadd = myadd & myrs!Street & vbCrLf & IIf(myrs!extendedaddress <> "", myrs!extendedaddress & vbCrLf, "") & myrs!City & ", " & myrs!State & "  " & myrs!ZipCode & "  " & myrs!Country
    myadd = myadd & IIf(Len(myrs!phone) > 0, vbCrLf & "Phone: " & myrs!phone, "")
    myadd = myadd & IIf(Len(myrs!Email) > 0, vbCrLf & "Email: " & myrs!Email, "")
Else
    myadd = myadd & vbCrLf & "SAME AS BILL TO"

End If

Me.txtShipTo = myadd


'***** Set up OrderedBy
myadd = ""

myaddressid = DLookup("[AddressID]", "ssd_qryUserIDtoAddresses", "[UserID] = " & Me.UserID)



mysql = "Select * from ssd_Addresses WHERE [AddressID] = " & myaddressid
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenSnapshot)
myadd = myrs!firstname & " " & myrs!LastName & vbCrLf
myadd = myadd & IIf(Len(myrs!CompanyName) > 0, myrs!CompanyName & vbCrLf, "")
myadd = myadd & IIf(Len(myrs!DepartmentName) > 0, myrs!DepartmentName & vbCrLf, "")
myadd = myadd & myrs!Street & vbCrLf & IIf(myrs!extendedaddress <> "", myrs!extendedaddress & vbCrLf, "") & myrs!City & ", " & myrs!State & "  " & myrs!ZipCode & "  " & myrs!Country
myadd = myadd & IIf(Len(myrs!phone) > 0, vbCrLf & "Phone: " & myrs!phone, "")
myadd = myadd & IIf(Len(myrs!Email) > 0, vbCrLf & "Email: " & myrs!Email, "")


Me.txtOrderedBy = myadd





'Set up XML reads for Orderinfo.xml and Report.xml
myFileName = (ServerPicPath(Me.Path) & "\report.xml") 'Set the filename in the same folder as represented by Path
      
MySuccess = myXMLData.Load(myFileName)
    
If MySuccess = False Then
    'Don't try to bring in anything - no file.
Else 'Set anything we can read from report.xml

    Me.txtOriginalFilename = "Original Filename: " & myXMLData.getElementsByTagName("info").item(0).Attributes.getNamedItem("uploadedfilename").Text

    'Me.txtSides = myXMLData.getElementsByTagName("info").Item(0).Attributes.getNamedItem("count").Text

End If


myFileName = (ServerPicPath(Me.Path) & "\OrderInfo.xml") 'Set the filename in the same folder as represented by Path
      
MySuccess = myXMLData.Load(myFileName)
    
If MySuccess = False Then
    'Don't try to bring in anything - no file.
Else 'Set anything we can read from report.xml
    
    Me.txtSheetSize = myXMLData.getElementsByTagName("impositionplanresult").item(0).Attributes.getNamedItem("PressSheetSize").Text
    Me.txtRunLength = myXMLData.getElementsByTagName("impositionplanresult").item(0).Attributes.getNamedItem("PressSheetCount").Text
    'Me.txtFlatSize = myXMLData.getElementsByTagName("flat").Item(0).Attributes.getNamedItem("size").Text

End If

'Get Paper Name from JobletPaper

mysql = "SELECT * from ssQryJobTicketJobletPaper WHERE [JobID] = " & Me.ProductID
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenSnapshot)
'Me.txtPaper = myRS!papername
Set myrs = Nothing


myFileName = (ServerPicPath(Me.Path) & "\layout.xml") 'Set the filename in the same folder as represented by Path
      
MySuccess = myXMLData.Load(myFileName)
    
If MySuccess = False Then
    'Don't try to bring in anything - no file.
Else 'Set anything we can read from report.xml
    myBleed = "Unknown"
    On Error Resume Next
    Me.txtColorProcess = myXMLData.getElementsByTagName("colorchannels").item(0).Attributes.getNamedItem("spot").Text
    Me.txtColorSpot = myXMLData.getElementsByTagName("colorchannels").item(0).Attributes.getNamedItem("process").Text
    Me.txtColorBlack = myXMLData.getElementsByTagName("colorchannels").item(0).Attributes.getNamedItem("black").Text
    myWidth = myXMLData.getElementsByTagName("page").item(0).Attributes.getNamedItem("width").Text
    Me.txtWidth = Format(myWidth, "0.000")
    myHeight = myXMLData.getElementsByTagName("page").item(0).Attributes.getNamedItem("height").Text
    Me.txtHeight = Format(myHeight, "0.000")
    myBleed = myXMLData.getElementsByTagName("page").item(0).Attributes.getNamedItem("bleed").Text
    Me.txtBleed = myBleed
    'Me.txtSides = myXMLData.getElementsByTagName("page").Item(0).Attributes.getNamedItem("pagenum").Text

End If


myFileName = (ServerPicPath(Me.Path) & "\pricing\impositionplan.xml") 'Set the filename in the same folder as represented by Path
      
MySuccess = myXMLData.Load(myFileName)
    
If MySuccess = False Then
    'Don't try to bring in anything - no file.
Else 'Set anything we can read from report.xml

    Me.txtPages = myXMLData.getElementsByTagName("PageCount").item(0).Text
    

End If

myFileName = (ServerPicPath(Me.Path) & "\pricing\impositionplanresult.xml") 'Set the filename in the same folder as represented by Path
      
MySuccess = myXMLData.Load(myFileName)
    
If MySuccess = False Then
    'Don't try to bring in anything - no file.
Else 'Set anything we can read from report.xml

    'Me.txtSides = myXMLData.getElementsByTagName("SurfaceCount").Item(0).Text
    'Me.txtImpoRows = myXMLData.getElementsByTagName("RowCount").Item(0).Text
    'Me.txtImpoColumns = myXMLData.getElementsByTagName("ColumnCount").Item(0).Text
    'Me.txtNumberUp = myXMLData.getElementsByTagName("PressSheetCapacity").Item(0).Text

End If

If Nz(Me.ShippedDate, 0) > 0 Then
    Me.txtProductShipped.visible = True
Else
    Me.txtProductShipped.visible = False
End If

If Nz(Me.ProductStatusID, 0) = 190 Then
    Me.txtInventoryPull.visible = True
Else
    Me.txtInventoryPull.visible = False
End If




