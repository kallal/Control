Private Sub cmdPrintAll_Click()
'Prints Manifest and all Job Tickets

myResponse = vbYes
myResponse = MsgBox("Printing all Job Tickets for " & Me.Numerator & ". Continue?", vbQuestion + vbYesNo + vbDefaultButton1, "Print Complete Order")

If myResponse = vbYes Then
    Dim MYDB As Recordset
    Dim mysql As String
    
    mysql = "SELECT * FROM ssd_OrderDetails WHERE [OrderID] = " & Me.OrderID
    
    Set MYDB = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)
    
    Do Until MYDB.EOF
        MYDB.Edit
        MYDB!TrackingCode = Now()
        MYDB.Update
        MYDB.MoveNext
        
    Loop


    'DoCmd.OpenReport "ssRptOrderManifest", acViewNormal, , "[OrderID] = " & Me.OrderID, acHidden
    DoCmd.OpenReport "rptSS_jobTicket", acViewNormal, , "[OrderID] = " & Me.OrderID, acHidden
End If


End Sub

Private Sub Report_Activate()
'Update Refcodes from XML file in detail for any which do not have a refcode filled in. - put Name or None in refcode so that we know not to retry the next time.


Dim mysql As String
Dim MYDB As Recordset
Dim MySuccess           As Boolean
Dim myXMLData           As MSXML2.DOMDocument
Dim myNode              As MSXML2.IXMLDOMNode
Dim myNodeList          As MSXML2.IXMLDOMNodeList
Dim myFileName          As String

Set myXMLData = CreateObject("MSXML2.DOMDocument")
Set objFSO = CreateObject("Scripting.FileSystemObject")


mysql = "SELECT * from ssd_Products WHERE [Refcode] = ''"
'mySQL = "SELECT * from ssd_Products "  'Use this one to force all to update again.


Set MYDB = CurrentDb.OpenRecordset(mysql, dbOpenDynaset, dbSeeChanges)

'MsgBox mydb!RefCode

'Debug.Print MYDB.RecordCount


Do Until MYDB.EOF

    
    MYDB.Edit
    
    'Grab name from iform_data.xml



    
    myFileName = (ServerPicPath(MYDB!Path) & "\iform_Data.xml") 'Set the filename in the same folder as represented by Path
    
    
    MySuccess = myXMLData.Load(myFileName)
    
    If MySuccess = False Then
        MYDB!RefCode = "No Variable Data File"
    Else
        myfn = ""
        myln = ""
        MyFullname = ""
        On Error Resume Next
        myfn = myXMLData.getElementsByTagName("id_1").item(0).Attributes.item(0).Text
        myln = myXMLData.getElementsByTagName("id_2").item(0).Attributes.item(0).Text
        MyFullname = Trim(myfn & " " & myln)
        If Len(MyFullname) > 0 Then
            MYDB!RefCode = MyFullname
        Else
            MYDB!RefCode = "Variable Data from CSV file"
        End If
        
    End If
    

    MYDB.Update
    'Debug.Print ".";
    MYDB.MoveNext
    
    
Loop
End Sub

Private Sub ReportHeader_Format(Cancel As Integer, FormatCount As Integer)

Me.imgCompanyLogo.Picture = ""
Me.imgCompanyLogo.Picture = cControlInvoiceFolder & "\CompanyLogo.bmp"

Me.imgCompanyLogo2.Picture = ""
Me.imgCompanyLogo2.Picture = cControlInvoiceFolder & "\CompanyLogo.bmp"


'***** Set up BillTo
myaddressid = Nz(Me.billingAddressID, Me.ShippingAddressID)

mysql = "Select * from ssd_Addresses WHERE [AddressID] = " & myaddressid
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenSnapshot)
myadd = myrs!firstname & " " & myrs!LastName & vbCrLf
myadd = myadd & IIf(Len(myrs!CompanyName) > 0, myrs!CompanyName & vbCrLf, "")
myadd = myadd & IIf(Len(myrs!DepartmentName) > 0, myrs!DepartmentName & vbCrLf, "")
myadd = myadd & myrs!Street & vbCrLf & IIf(myrs!extendedaddress <> "", myrs!extendedaddress & vbCrLf, "") & myrs!City & ", " & myrs!State & "  " & myrs!ZipCode & "  " & myrs!Country
myadd = myadd & IIf(Len(myrs!phone) > 0, vbCrLf & "Phone: " & myrs!phone, "")
Me.txtBillTo = myadd



'***** Set up ShipTo
myadd = ""


If Nz(Me.billingAddressID, 0) > 0 Then
    mysql = "Select * from ssd_Addresses WHERE [AddressID] = " & Me.ShippingAddressID
    Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenSnapshot)
    myadd = myrs!firstname & " " & myrs!LastName & vbCrLf
    myadd = myadd & IIf(Len(myrs!CompanyName) > 0, myrs!CompanyName & vbCrLf, "")
    myadd = myadd & IIf(Len(myrs!DepartmentName) > 0, myrs!DepartmentName & vbCrLf, "")
    myadd = myadd & myrs!Street & vbCrLf & IIf(myrs!extendedaddress <> "", myrs!extendedaddress & vbCrLf, "") & myrs!City & ", " & myrs!State & "  " & myrs!ZipCode & "  " & myrs!Country
    myadd = myadd & IIf(Len(myrs!phone) > 0, vbCrLf & "Phone: " & myrs!phone, "")
Else
    myadd = myadd & vbCrLf & "SAME AS BILL TO"

End If

Me.txtShipTo = myadd

'***** Set up OrderedBy
myadd = ""

myaddressid = DLookup("[AddressID]", "ssd_qryUserIDtoAddresses", "[UserID] = " & Me.UserID)



mysql = "Select * from ssd_Addresses WHERE [AddressID] = " & myaddressid
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenSnapshot)
myadd = myrs!firstname & " " & myrs!LastName & vbCrLf
myadd = myadd & IIf(Len(myrs!CompanyName) > 0, myrs!CompanyName & vbCrLf, "")
myadd = myadd & IIf(Len(myrs!DepartmentName) > 0, myrs!DepartmentName & vbCrLf, "")
myadd = myadd & myrs!Street & vbCrLf & IIf(myrs!extendedaddress <> "", myrs!extendedaddress & vbCrLf, "") & myrs!City & ", " & myrs!State & "  " & myrs!ZipCode & "  " & myrs!Country

myadd = myadd & IIf(Len(myrs!phone) > 0, vbCrLf & "Phone: " & myrs!phone, "")
myadd = myadd & IIf(Len(myrs!Email) > 0, vbCrLf & "Email: " & myrs!Email, "")


Me.txtOrderedBy = myadd


End Sub