Private Sub Report_Load()
Dim myrs As Recordset
mysql = "SELECT * from locInvoiceTaxAmounts"
Set myrs = CurrentDb.OpenRecordset(mysql)
myrs.Edit
myrs!txtTaxAmt = 0
myrs!txtNonTaxAmt = 0
myrs.Update
Set myrs = Nothing

End Sub

Private Sub ReportFooter_Format(Cancel As Integer, FormatCount As Integer)
If cControlCompanyCode = "BG" Then
Me.lblGSTNum.visible = True
Else
Me.lblGSTNum.visible = False
End If




myAccountTerms = DLookup("[TermsDesc]", "dbo_InvoiceTerms", "[id] = " & Nz(Me.InvoiceTermsID, 9))
Me.txtDueDate = Me.InvoiceDate
If Me.InvoiceTermsID = 8 Then ' Account
    myAccountTerms = myAccountTerms & " : Net " & Me.InvoiceTermsDays & " Days."
    Me.txtDueDate = Me.InvoiceDate + Me.InvoiceTermsDays
End If

Me.lblAccountTerms = myAccountTerms

Me.txtInvoiceMessage = DLookup("InvoiceMessageAcct", "dbo_Company", "[ID] = 1")

'If Nz(Me.AcctStatus, 0) = 3 Then
'    Me.lblAccountTerms = DLookup("InvoiceMessageAcct", "dbo_Company", "[ID] = 1")
'
'Else
'    Me.lblAccountTerms = DLookup("InvoiceMessageCOD", "dbo_Company", "[ID] = 1")
'End If


Me.lblTax1 = DLookup("GSTName", "dbo_Company", "[ID] = 1")
Me.TaxRate = DLookup("GSTRate", "dbo_Company", "[ID] = 1")
Me.lblAccountTerms.visible = True

Dim myrs As Recordset
mysql = "SELECT * from dbo_ProjectComponentHeader WHERE [ProjectHeaderID] = " & Me.ID
Set myrs = CurrentDb.OpenRecordset(mysql, dbOpenSnapshot)

Me.txtSubTotalTax = 0
Me.txtSubTotalNonTax = 0

Do Until myrs.EOF

mysellfield = "ExtQuoteSell" & Nz(myrs!extQuoteSelect, 1)
myQuantityField = "ExtQuoteQty" & Nz(myrs!extQuoteSelect, 1)


    myqtytaxfield = "ExtQuoteQtyTax" & Nz(myrs!extQuoteSelect, 1)
    myQtyNonTax = Nz(myrs(myQuantityField)) - Nz(myrs(myqtytaxfield))


mypriceper = Nz(myrs(mysellfield), 1) / Nz(myrs(myQuantityField), 1)
'MsgBox "hit"

If cControlCompanyCode <> "DPI" Then
    myqtytaxfield = myQuantityField
    myQtyNonTax = 0
    Me.txtSubTotalTax = Nz(Me.txtSubTotalTax) + myrs(mysellfield)
    Me.txtSubTotalNonTax = 0
Else
    If myrs!MailComponentYN = True Then
    
        Me.txtSubTotalNonTax = Nz(Me.txtSubTotalNonTax) + Nz(myrs(mysellfield))
    Else
        Me.txtSubTotalTax = Nz(Me.txtSubTotalTax) + Nz((myrs(myqtytaxfield) * mypriceper))
        Me.txtSubTotalNonTax = Nz(Me.txtSubTotalNonTax) + (Nz(myQtyNonTax) * mypriceper)
    End If
End If



myrs.MoveNext
Loop

Me.SubTotalBeforeTax = Me.txtSubTotalTax + Me.txtSubTotalNonTax



Set myrs = Nothing

If cControlCompanyCode = "DPI" Then
    Me.txtDueDate.visible = True
    Me.lblDueDate.visible = True
Else
    Me.txtDueDate.visible = False
    Me.lblDueDate.visible = False
End If



End Sub




