mylabel = "DUE DATE"
mycriteria = "DUEDATE"
myLabelCaption = "LBLSORTDUEDATE"

myresult = fncSortByLabel(Me.OrderBy, mycriteria, mylabel)
Me.OrderBy = Left(myresult, InStr(myresult, "|") - 1)
Me(myLabelCaption).caption = Right(myresult, Len(myresult) - InStr(myresult, "|"))



Me.OrderByOn = True
End Sub





